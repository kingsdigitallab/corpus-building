rds scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-08-05\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003231_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003231.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #AA\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Ἐπαφρό\u003Csupplied reason=\"omitted\">δ\u003C/supplied>ειτος\u003C/name>\u003C/persName>\n                    \u003Clb n=\"2\"/>\u003Cw>ἔζησεν\u003C/w>\n                    \u003Clb n=\"3\"/>\u003Cw>ἔτη\u003C/w> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cnum value=\"60\">ξ\u003C/num> \u003Cg ref=\"#interpunct\">·\u003C/g>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 2\">\u003Cnote>lapis: Ἐπαφροειτος\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>Epaphrodeitos lived for 60 years.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>Epaphrodeitos visse 60 anni.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Korhonen considers the inscription not urban because of the structure of the epitaph and the palaeography. Moreover, the name of the deceased, \u003Cforeign xml:lang=\"grc\">Ἐπαφρόδιτος\u003C/foreign>, is highly attested in Sicily (see LGPN 3A: 145) and in another inscription from Catania (Libertini 1937: 78).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>2004.0646\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001283\">https://biblio.inscriptiones.org/epig10001283\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>54.0901\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                         \u003Cauthor>Korhonen\u003C/author>\n                         \u003Cdate>2004\u003C/date> \n                        \u003CcitedRange>80\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl/>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
