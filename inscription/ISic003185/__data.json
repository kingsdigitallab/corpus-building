             \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462503\">Syracusae\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523083\">Siracusa\u003C/placeName>\n                            \t\u003Cgeo>37.08415, 15.27628\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0200\" notAfter-custom=\"0600\" cert=\"low\">Christian\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Vigna Cassia (San Diego)\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                            \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t   \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t               \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n                            \u003Cchange when=\"2021-01-18\" who=\"#VM\">Valentina Mignosa encoded text division, cleaned up the autogenerated text and checked the edition\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003185_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003185.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>Εὐστάχι\u003Csupplied reason=\"omitted\">ο\u003C/supplied>ς \u003Chi rend=\"small\">\u003Cexpan>\u003Cabbr>\u003Chi rend=\"ligature\">ἤ\u003C/hi>\u003C/abbr>\u003Cex>τε\u003C/ex>\u003C/expan>\u003C/hi> \u003Chi rend=\"small\">κ\u003Cchoice>\u003Creg>αὶ\u003C/reg>\u003Corig>ὲ\u003C/orig>\u003C/choice>\u003C/hi> \u003Chi rend=\"small\">Βῶκυς\u003C/hi>\n                    \u003Clb n=\"2\"/>ἐκοιμήθη\n                    \u003Clb n=\"3\"/>εἰδοῖς Μαείες\n                    \u003Clb n=\"4\"/>σώφρων καὶ ἀκα\n                    \u003Clb n=\"5\" break=\"no\"/>τάγνωστος ἔζησεν\n                    \u003Clb n=\"6\"/>ἔτη \u003Cnum value=\"35\">λε\u003C/num>\n                    \u003Clb n=\"7\"/>καὶ ἀνεπαύσατο\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Lo Faro in Marchese 2012\u003C/note>\n                    \u003C/app>\n                    \u003Capp loc=\"1-3\">\n                        \u003Cnote>ἤκε and Βῶκυς are written smaller and adjoins the ends of ll. 1-3. Lo Faro (2012) agrees with Agnello (1950-1951) that the smaller text is an addition to the main text and is meant to specify the name of the deceased (vel quidem Bochus or Bucco).\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>\u003C!--commented out pending revision-->\n                    \u003C!--Eustachis he ke (i.e. kai) Bokys (bwkus; i.e. Bokchos) ekoimethe eidois Maeies (i.e. Maiais) sophron kai akatagnostos; ezesen ete le' kai anepausato. The name Bokys is written smaller between l.2 and 3. It does not seem to be related to the rest of the text.-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>04.0012\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Cdate>1923\u003C/date>\n                        \u003CcitedRange>no.11\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GQ8HXJJE\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001699\">https://biblio.inscriptiones.org/epig10001699\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Agnello\u003C/author>\n                        \u003Cdate>1950-1951\u003C/date>\n                        \u003CcitedRange>58-59 no.10\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/NM9R48KX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002213\">https://biblio.inscriptiones.org/epig10002213\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrua\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>no.273\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/759ZXHKQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001995\">https://biblio.inscriptiones.org/epig10001995\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Marchese\u003C/author>\n                        \u003Cdate>2012\u003C/date>\n                        \u003CcitedRange>119 no.55\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/IQ93VIZM\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002550\">https://biblio.inscriptiones.org/epig10002550\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
