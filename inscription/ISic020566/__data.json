 \u003Cp>Encoded following EpiDoc guidelines 9.2\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>             \n            \u003Cxi:include href=\"../alists/charDecl.xml\">                 \n                \u003Cxi:fallback>                     \n                    \u003Cp>ISicily glyphs authority list\u003C/p>                 \n                \u003C/xi:fallback>             \n            \u003C/xi:include>         \n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.signature\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/105.html\">artist inscription\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-05-13\" who=\"#VM\">Valentina Mignosa added metadata, text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020566_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020566.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>πυραῖς ἐποίε̄σε \u003CpersName type=\"attested\">\u003Cname>Κλισυ\u003Cunclear>μι\u003C/unclear>ς\u003C/name> \u003CpersName type=\"attested\">\u003Cname type=\"patronymic\">\u003Cseg part=\"I\">Πά\u003Cunclear>β\u003C/unclear>\u003C/seg>\u003Cgap reason=\"lost\" quantity=\"2\" unit=\"character\"/>\u003Cgap reason=\"illegible\" quantity=\"1\" unit=\"1\"/>\u003C/name>\u003C/persName>\u003C/persName>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Arena (IGASM 2)\u003C/note>\n                    \u003C/app>\n                    \u003Capp>\n                        \u003Cnote>Orlandini (1968): \u003CpersName type=\"attested\">\u003Cname>Πυραὶς\u003C/name>\u003C/persName> ἐποίε̄σε καὶ \u003CpersName type=\"attested\">\u003Cname>Σύ\u003Cunclear>πα\u003C/unclear>ς\u003C/name>\u003C/persName> πα\u003Cunclear>ρ\u003C/unclear>\u003Csupplied reason=\"lost\">έ\u003C/supplied>\u003Cunclear>β\u003C/unclear>\u003Csupplied reason=\"lost\">απσε\u003C/supplied>.\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp>Klisymis son of Pab(ios?) made me for the tomb.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#VM\">\n                \u003Cp>Per la tomba mi fece Klisymis figlio di Pab(ios?).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp>Artist's signature on the shoulder of a big stamnos, containing bones. According to Arena's interpretation, which I accept, \n                    the inscription would indicate that the vessel was made to serve as an ossuary. The anomalous structure of the sentence would \n                    derive from the producer's wish to emphasise the funerary context for which the vessel was conceived. Usually, in fact, this particular \n                    form of vessel is used for storing or serving liquids.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IGASM2[2]\">\n                        \u003Cauthor>Arena\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>107\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/65P7AM6N\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001373\">https://biblio.inscriptiones.org/epig10001373\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGDS1\">\n                        \u003Cauthor>Dubois\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>174\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RWXNUFEX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000187\">https://biblio.inscriptiones.org/epig10000187\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Guarducci\u003C/author>\n                        \u003Cdate>1967-1978\u003C/date>\n                        \u003CcitedRange>IV, 432, n. 1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/ENQ4AGD6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000193\">https://biblio.inscriptiones.org/epig10000193\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Orlandini\u003C/author>\n                        \u003Cdate>1968\u003C/date>\n                        \u003CcitedRange>154-155, pls. 58.2, 59\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/ZFBMDD7S\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002477\">https://biblio.inscriptiones.org/epig10002477\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Orlandini\u003C/author>\n                        \u003Cdate>1968-1969\u003C/date> \u003C!-- wrong year in Arena 2[2] (1964-65) -->\n                        \u003CcitedRange>329-331, pl. 48.1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4CAZ77JM\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001463\">https://biblio.inscriptiones.org/epig10001463\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
