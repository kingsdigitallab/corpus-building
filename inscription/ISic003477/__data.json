3Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic003477\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-18\">10.5281/zenodo.4358562\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative\n                        Commons-Attribution 4.0 licence.\n                    \u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Halaesa\u003C/settlement>\n                        \u003Crepository role=\"museum\" ref=\"http://sicily.classics.ox.ac.uk/museum/053\">Antiquarium e sito archeologico di Halaesa\u003C/repository>\n                        \u003Cidno type=\"inventory\">ME 30587\u003C/idno>\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>Fragment of a limestone stele. The left margin is preserved, but the stone is broken on all other sides.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.sandstone\" ref=\"http://www.eagle-network.eu/voc/material/lod/75.html\">sandstone\n                                    \u003C/material>\n                                    \u003CobjectType ana=\"#object.stele\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/250\">stele\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">13\u003C/height>\n                                        \u003Cwidth unit=\"cm\">8.5\u003C/width>\n                                        \u003Cdepth unit=\"cm\">14\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>The beginning of five lines of Greek letters more or less aligned with the left margin of the stone.\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>The letters are regular and well spaced. Alpha has broken bar; epsilon has a shorter middle bar.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line5\">Lines 1-5\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">15-18\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">not measured\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462225\">Halaesa\u003C/placeName>\t\t\n                                \u003Coffset>near\n                                    \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525099\">Castel di Tusa\u003C/placeName>\n                                \u003C/offset>\n                            \t\u003Cgeo>37.99907, 14.26197\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0200\" notAfter-custom=\"-0001\" evidence=\"lettering\" precision=\"low\">2nd — 1st century BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" when=\"1956\">Recovered from the area of Temple A in 1956 (either from the main trench along the west side of the platform, or other soundings taken in the vicinity, all made by Carettoni in 1956)\u003Cgeo>38.000505, 14.261362\u003C/geo>\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">On display in the lapidarium on the site\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.list\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/108.html\">list\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-03-08\" who=\"#DM\">Davide Massimo revised the EpiDoc encoding\u003C/change>\n                \u003Cchange when=\"2019-07-23\" who=\"#JP\">Jonathan Prag edited the file based on autopsy for 2017 edition\u003C/change>\n                \u003Cchange when=\"2020-09-09\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003477_tiled.tif\" height=\"3672px\" width=\"2544px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003477.jpg\" height=\"3672px\" width=\"2544px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic003477_copy2_tiled.tif\" height=\"4184px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003477_copy2.jpg\" height=\"4184px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic003477_leftthreequarter_tiled.tif\" height=\"3216px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003477_leftthreequarter.jpg\" height=\"3216px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n        \u003Csurface type=\"rear\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003477_rear_tiled.tif\" height=\"2776px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003477_rear.jpg\" height=\"2776px\" width=\"3680px\">\n                \u003Cdesc>Photo J.Prag courtesy Soprintendenza BBCCAA di Messina\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n     \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#DM\">\n                \u003Cab>\n                    \u003Clb n=\"1\" xml:id=\"DvΤAK\"/>\u003Cw xml:id=\"DvΤAU\">Ἐ\u003Cunclear xml:id=\"DvΤAe\">π\u003C/unclear>\u003Csupplied reason=\"lost\" xml:id=\"DvΤAo\">ι\u003C/supplied>\u003C/w> \u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\" xml:id=\"DvΤAy\"/>\n                    \u003Clb n=\"2\" xml:id=\"DvΤAΙ\"/>\u003Cw xml:id=\"DvΤAΤ\">Ἡρα\u003Csupplied reason=\"lost\" xml:id=\"DvΤAε\">κλειδ\u003C/supplied>\u003C/w> \u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\" xml:id=\"DvΤAο\"/>\n                    \u003Clb n=\"3\" xml:id=\"DvΤBA\"/>\u003Cw xml:id=\"DvΤBK\">Ἀπο\u003Cunclear xml:id=\"DvΤBU\">λ\u003C/unclear>\u003Csupplied reason=\"lost\" xml:id=\"DvΤBe\">λ\u003C/supplied>\u003C/w> \u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\" xml:id=\"DvΤBo\"/>\n                    \u003Clb n=\"4\" xml:id=\"DvΤBy\"/>\u003Cw xml:id=\"DvΤBΙ\">Αἰσχ\u003Cunclear xml:id=\"DvΤBΤ\">ύ\u003C/unclear>\u003Csupplied reason=\"lost\" cert=\"low\" xml:id=\"DvΤBε\">λος\u003C/supplied>\u003C/w> \u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\" xml:id=\"DvΤBο\"/>\n                    \u003Clb n=\"5\" xml:id=\"DvΤCA\"/>\u003Cw xml:id=\"DvΤCK\">Ε\u003Cunclear xml:id=\"DvΤCU\">ὐ\u003C/unclear>\u003C/w> \u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\" xml:id=\"DvΤCe\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text as in Prag and Tigano 2017, from autopsy\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 3\">\u003Cnote>Carettoni: ΑΠΟΛ[ΛΙΝΙ]\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 4\">\u003Cnote>Carettoni: ΑΙΣΚΥ[---]\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation not practicable-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#JP\">\n                \u003Cp>Carettoni (1961: 318) suggested that this was a dedication to Apollo, restoring ΑΠΟΛΛΙΝΙ (‘to Apollo’) in line 3; Manganaro (1999: 68, followed by Facella 2006: 320) speculates instead that the text was a list of names, perhaps of ephebes, or alternatively of dedicants or contributors, but too little survives to be certain of either interpretation. It is possible that the text continued both above and/or below what survives. Carettoni mistakenly read ΑΙΣΚΥ in line 4.  The restorations are those suggested by Manganaro, but must be considered purely exempli gratia.\u003C/p>\n                    \u003Cp>The text cannot be dated precisely, but the letter forms suggest a date in the second or first century BC.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>49.1282\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Carettoni\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1961\u003C/date>\n                        \u003CcitedRange>318 no.12a\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1999\u003C/date>\n                        \u003CcitedRange>68 no.63 fig.145\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GRB3IPD5\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001813\">https://biblio.inscriptiones.org/epig10001813\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Facella\u003C/author>\n                        \u003Cdate>2006\u003C/date>\n                        \u003CcitedRange>320\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/J4XWZPV6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002013\">https://biblio.inscriptiones.org/epig10002013\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Prag and Tigano\u003C/author>\n                        \u003Cdate>2017\u003C/date>\n                        \u003CcitedRange>no.1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/MI64NB9L\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002630\">https://biblio.inscriptiones.org/epig10002630\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
