03Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.tessera.civic\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/130.html\">tessera (civic)\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-12-10\" who=\"#VM\">Valentina Mignosa added metadata and text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic030242_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic030242.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Cordano 1992\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>This document is one of a large deposit (c. 150) of lead tesserae found in a storeroom of Athena's sanctuary at the archaeological site of Kamarina.\n                    The two fragments may belong to the same tessera according to the indications of the context of discovery but are too twisted to read the text or to \n                    ascertain whether any text is present.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Cordano\u003C/author>\n                        \u003Cdate>1992\u003C/date>\n                        \u003CcitedRange>116\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/XTMUJ38N\"/> \n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002437\">https://biblio.inscriptiones.org/epig10002437\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>52.0920\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n           