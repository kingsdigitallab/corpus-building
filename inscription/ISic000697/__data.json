\u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\" cert=\"low\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-09-06\" who=\"#JP\">Jonathan Prag revised file from publication\u003C/change>\n                \u003Cchange when=\"2024-08-05\" who=\"#JP\">Jonathan Prag revised from image\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000697_tiled.tif\" height=\"2592px\" width=\"1944px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana; photo L. Campagna 2004-11-16\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000697.jpg\" height=\"2592px\" width=\"1944px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana; photo L. Campagna 2004-11-16\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Cgap reason=\"illegible\" unit=\"character\" quantity=\"1\"/>\u003Cnum atLeast=\"200\">CC\u003C/num>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> ex \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"3\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Cnum atLeast=\"24\">\u003Cunclear>X\u003C/unclear>XIIII\u003C/num> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text controlled against photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"1\">\u003Cnote>Manganaro: [- - ex test. HS]CC [- m.n. - ]; lapis: CO theoretically possible, but the close proximity of the characters suggests a numeral; lapis: the tail of the preceding character is visible on the stone, and is compatible with the foot of a diagonal stroke, most likely M therefore (since XCC is unlikely), assuming part of the numeral.\u003C/note>\u003C/app>\n                    \u003Capp loc=\"2\">\u003Cnote>Manganaro: [ut - - ]ex [usuris? -]; lapis: traces of serif perhaps lower right after interpunct.\u003C/note>\u003C/app>\n                    \u003Capp loc=\"3\">\u003Cnote>Manganaro: [?die natali distributio] XXIIII [- -]; lapis: space after final vertical.\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Manganaro offers an entirely exempli gratia restoration following IlS 6468, suggesting it might be concerned with financial arrangements for funerary celebrations; but other interpretations are possible. The fragment was omitted from L'Annee Epigraphique 1989 no.339.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>166 no. 27 fig. 27\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/EZMBJ6GN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001831\">https://biblio.inscriptiones.org/epig10001831\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
