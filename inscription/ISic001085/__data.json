na Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2024-02-17\" who=\"#JP\">Jonathan Prag revised file from publication\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001085_tiled.tif\" height=\"4980px\" width=\"2696px\">\n                \u003Cdesc>Crispi 1831 fig.1\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001085.jpg\" height=\"4980px\" width=\"2696px\">\n                \u003Cdesc>Crispi 1831 fig.1\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" unit=\"character\" quantity=\"2\" cert=\"low\"/>\u003Corig>ΥΑΥΣ\u003Cgap reason=\"illegible\" unit=\"character\" quantity=\"1\"/>Ο\u003C/orig>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" unit=\"character\" quantity=\"1\" cert=\"low\"/>ΟΙΑΤΟ\n                    \u003Clb n=\"3\"/>\u003Cgap reason=\"lost\" unit=\"character\" quantity=\"1\" cert=\"low\"/>ΖΟΣ\n                 \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on drawing from Crispi 1831\u003C/note>\u003C/app>\n                    \u003Capp loc=\"1\">\u003Cnote>The fifth character is depicted simply as a rectangle in Crispi's drawing\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdi