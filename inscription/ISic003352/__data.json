aterial>\n                                    \u003CobjectType ana=\"#object.column\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/100\">column\u003C/objectType>\n                                    \u003Cdimensions>\u003C!--from Korhonen 2018-->\n                                        \u003Cheight unit=\"cm\">46.5\u003C/height>\n                                        \u003Cwidth unit=\"cm\">21\u003C/width>\n                                        \u003Cdepth unit=\"cm\">21\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>Text in Greek letters over five lines, over the lower two thirds of the vertical side of the column\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003C!--from Korhonen 2018-->\n                                \u003Clocus from=\"line1\" to=\"line5\">Line 1-5\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">25-30\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462538\">Messana\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2524170\">Messina\u003C/placeName>\n                            \t\u003Cgeo>38.19394, 15.55256\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"1769\" notAfter-custom=\"1784\" precision=\"low\">An 18th century invention, probably between 1769 (publication of Torremuzza's corpus which provides the inspiration) and 1784 (first report by Biscari)\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Biscari (1784) states that it was found at Messina among the ruins of the earthquake in 1783\u003C/provenance> \n                        \u003Cprovenance type=\"observed\" subtype=\"recorded\">subsequent accounts in the early C19 attribute it to Capo Peloro (in Korhonen's view, a later refinement/attribution by Biscari)\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">2016.05.11 (seen in store by Prag)\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.dedication\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/88.html\">dedication\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-09-23\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n                \u003Cchange when=\"2020-08-13\" who=\"#JP\">Jonathan Prag edited the file after Korhonen 2018\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003352_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003352.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#AA\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>θεοῖς πᾶσι\n                    \u003Clb n=\"2\"/>σωτῆρσιν\n                    \u003Clb n=\"3\"/>Γόργον\n                    \u003Clb n=\"4\"/>καὶ\n                    \u003Clb n=\"5\"/>οἱ ἄλλοι \u003Cexpan>\u003Cabbr>πολῖ\u003C/abbr>\u003Cex>ται\u003C/ex>\u003C/expan>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Korhonen 2004\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#AA\">\n                \u003Cp>To all the deliverer Gods. Gorgon and the other citizens.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Although originally believed by Korhonen 2004 to be an imperial Roman archaising text, Korhonen 2018 provides convincing arguments and explanations for believing this to be a later 18th century creation.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">\n                                14.0349bis\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                         \u003Cauthor>Korhonen\u003C/author>\n                         \u003Cdate>2004\u003C/date> \n                        \u003CcitedRange>236\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003Cauthor>Franz\u003C/author>\n                        \u003Cdate>1853\u003C/date>\n                        \u003CcitedRange>5592 adn\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/TQMVHEVL\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10001250\">https://biblio.inscriptiones.org/epig10001250\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Korhonen\u003C/author>\n                        \u003Cdate>2018\u003C/date>\n                        \u003CcitedRange>112-116 no.2 fig.2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/MBBWJWWU\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002620\">https://biblio.inscriptiones.org/epig10002620\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
