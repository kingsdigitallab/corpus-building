\n                                \u003Csupport>\u003Cp>Marble parallelepiped block, damaged on the lower right corner. The text is engraved inside a frame.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble\u003C/material>\n                                    \u003CobjectType ana=\"#object.block\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/189.html\">block\u003C/objectType>\n                                    \u003Cdimensions>\u003C!--measurements from Korhonen-->\n                                        \u003Cheight unit=\"cm\">30.5\u003C/height>\n                                        \u003Cwidth unit=\"cm\">25\u003C/width>\n                                        \u003Cdepth unit=\"cm\">15.5\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.damaged\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Letters with apices, squared Ε and Σ, rhomboid Ο, cursive Ω at l. 1 (but as a reversed M at l. 2), ligature between Ε and Σ at l. 2. Chi-Rho at the end of the text.\u003C/p>\u003C!--measurements from Korhonen-->\n                                \u003Clocus from=\"line1\" to=\"line6\">Lines 1-6\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">15-29\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">not recorded\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0250\" notAfter-custom=\"0350\" precision=\"low\">Second half of 3rd century CE or first\n                                half of 4th century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found in area of Via Dottor Consoli.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Observed by Ferrua in sala VIII of Castel Ursino and later by Korhonen in Magazzino superiore, Museo Civico.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-09-02\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003283_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003283.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #AA\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Κλαυδία\u003C/name> \u003Cname>Ῥωσσκ\u003Csupplied reason=\"omitted\">ί\u003C/supplied>\u003Clb n=\"2\" break=\"no\"/>α\u003C/name>\u003C/persName> \u003Cw>ζῶσα\u003C/w> \u003Cw>ἐποίησα\u003C/w>\n                    \u003Clb n=\"3\"/>\u003Cw>α\u003Csupplied reason=\"omitted\">ὐ\u003C/supplied>τῇ\u003C/w> \u003Cw>καὶ\u003C/w> \u003Cw>τοῖς\u003C/w> \u003Cw>εἰ\u003Clb n=\"4\" break=\"no\"/>\u003Cspace quantity=\"2\" unit=\"character\"/>δίοις\u003C/w>\n                    \u003Clb n=\"5\"/>\u003Cw>μετὰ\u003C/w> \u003Cw>εἰρ\u003Csupplied reason=\"lost\">ήνης\u003C/supplied>\u003C/w>\n                    \u003Clb n=\"6\"/>\u003Cg ref=\"#christogram\">☧\u003C/g>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"lines 1-2\">\u003Cnote>Libertini: Ῥωσσί|α; Mercurelli, Ferrua, Korhonen: Ῥωσσκ\u003Csupplied reason=\"omitted\">ί\u003C/supplied>|α\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 2\">\u003Cnote>Libertini: Ζώηλ(ε); Ferrua, Korhonen: ζῶσα\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 2\">\u003Cnote>Libertini: ἐποίησε; Ferrua, Korhonen: ἐποίησα\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 3\">\u003Cnote>Libertini, Ferrua: αὐτῇ; Korhonen: α\u003Csupplied reason=\"omitted\">ὐ\u003C/supplied>τῇ (i.e. ἐμαυτῇ)\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 5\">\u003Cnote>Libertini: μετ’ εἰρ[ένῃ] (sic); Ferrua, Korhonen: μετὰ εἰρ[ήνης]\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>Klaudia Rosskia while alive made (this) for herself and her relatives. In peace.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>Klaudia Rosskia mentre era viva realizzò (questo) per lei e per i suoi parenti. In pace.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>This Christian epitaph presents the structure with the name of the deceased and the verb \u003Cforeign xml:lang=\"grc\">ποιέω\u003C/foreign> indicating that the deceased, while still alive (participle of \u003Cforeign xml:lang=\"grc\">ζάω\u003C/foreign>), built the monument (or composed the epigram on the monument) for herself and his relatives (on the same structure in Catania see IG 14.489 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001312\">ISic001312\u003C/ref> and Korhonen 2004: 88-89). The clause \u003Cforeign xml:lang=\"grc\">μετὰ εἰρήνης\u003C/foreign> is not frequent, more frequently attested is \u003Cforeign xml:lang=\"grc\">ἐν εἰρήνῃ\u003C/foreign> (see IG 14.556 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001375\">ISic001375\u003C/ref>). The gentilicium \u003Cforeign xml:lang=\"grc\">Ῥωσκία\u003C/foreign> reveals the Roman citizenship of the deceased (for other evidence of the gentilicium in inscriptions from Catania, see IG 14.486 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001309\">ISic001309\u003C/ref>, IG 14.500 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001321\">ISic001321\u003C/ref>, IG 14.1981 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic003252\">ISic003252\u003C/ref>).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>179\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Libertini\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1937\u003C/date>\n                        \u003CcitedRange>78.i\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n\t\t\t \u003Cbibl>\n                        \u003Cauthor>Libertini\u003C/author>\n                        \u003Cdate>1981\u003C/date>\n                        \u003CcitedRange>125\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/ZJ65DX74\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001863\">https://biblio.inscriptiones.org/epig10001863\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrua\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>111-112 no.424\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/759ZXHKQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001995\">https://biblio.inscriptiones.org/epig10001995\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n\t           \u003Cauthor>Mercurelli\u003C/author>\n\t           \u003Cdate>1938\u003C/date>\n\t           \u003CcitedRange>52-53\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/PHQV78NC\"/>\n\t       \u003Cref target=\"https://biblio.inscriptiones.org/epig10002228\">https://biblio.inscriptiones.org/epig10002228\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\n\t       \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n\t           \u003CcitedRange>pp. 88-89\u003C/citedRange>\n\t           \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n\t       \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n\t   \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
