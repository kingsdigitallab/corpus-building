003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Csupplied reason=\"subaudible\">ἱερόν\u003C/supplied> ἐνθάφιον\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Arena (IGASM 2) with slight changes\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp>Offering to the dead.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp>Offerta funeraria.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>The inscription states explicitly that the vase is a funerary offering. See, for the double aspiration, Kretschmer 1969, 149ff. \n                    For the use of the term ἐντάφιον in epigraphy see, e.g. IGCyr016700.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IGASM2[2]\">\n                        \u003Cauthor>Arena\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>11\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/65P7AM6N\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001373\">https://biblio.inscriptiones.org/epig10001373\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl>\n                        \u003Cauthor>Kretschmer\u003C/author>\n                        \u003Cdate>1969\u003C/date>\n                        \u003CcitedRange>149ff.\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/72DGCQAJ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002507\">https://biblio.inscriptiones.org/epig10002507\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGCyr\">\n                        \u003CcitedRange>https://igcyr.unibo.it/igcyr016700\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/CGR3P9UF\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002501\">https://biblio.inscriptiones.org/epig10002501\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
