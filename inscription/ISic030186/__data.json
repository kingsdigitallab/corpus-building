       \u003Cmilestone unit=\"face\" n=\"a\"/>\n                \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"a\">\n                        \u003Cab>\n                            \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΤΙΑ\u003C/orig>\n                        \u003C/ab>\n                    \u003C/div>\n                    \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"2\">\n                        \u003Cab>\n                            \u003Clb n=\"1\"/>\u003CpersName>\u003Cname type=\"idionym\">Σ\u003Csupplied reason=\"lost\">ο̄\u003C/supplied>σία\u003Csupplied reason=\"lost\">ς\u003C/supplied>\u003C/name>\u003C/persName> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                            \u003Clb n=\"2\"/>\u003Cnum value=\"3\">τρίτα\u003Csupplied reason=\"lost\">ς\u003C/supplied>\u003C/num>\n                            \u003Clb n=\"3\"/>ἀπὸ νε̄́\u003Csupplied reason=\"lost\">τας\u003C/supplied>\n                        \u003C/ab>\n                    \u003C/div>\n                \n                \u003Cmilestone unit=\"face\" n=\"b\"/>\n                \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"b\">\n                    \u003Cab>\n                        \u003Clb n=\"1\"/>\u003Cnum value=\"3\">\u003Csupplied reason=\"lost\">τρί\u003C/supplied>τας\u003C/num>\n                        \u003Clb n=\"2\"/>\u003Csupplied reason=\"lost\">ἀπὸ\u003C/supplied> νε̄́\u003Csupplied reason=\"lost\">τας\u003C/supplied>\n                        \u003Clb n=\"3\"/>\u003Csupplied reason=\"lost\">τέ\u003C/supplied>θνα\u003Csupplied reason=\"lost\">κε\u003C/supplied>\n                    \u003C/ab>\n                \u003C/div>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Cordano 1992\u003C/note>\n                    \u003C/app>\n                \u003C/li