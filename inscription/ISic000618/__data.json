             \u003Csupport>\n                                    \u003Cmaterial ana=\"#material.stone.limestone\" ref=\"http://www.eagle-network.eu/voc/material/lod/66.html\">limestone\u003C/material>\n                                    \u003CobjectType ana=\"#object.stele\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/250.html\">stele\u003C/objectType>\n                                    \u003Cp>A rectangular limestone stele with a relief on the upper half of the front face,\n                                        and a two line inscription below. The stele is wider at the base, with a\n                                        triangular profile.\u003C/p>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">58.5\u003C/height>\n                                        \u003Cwidth unit=\"cm\">46.2\u003C/width>\n                                        \u003Cdepth unit=\"cm\">12-25.5\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                    \u003Cp>The text is engraved over two lines, on the same field as the relief above, with\n                                        a border below.\u003C/p>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Cp>Wide letters incised in a thin v-cut. Limited use of triangular serifs at the end of\n                                    some strokes, and some strokes widen towards the end (e.g. in the letter R). Letters\n                                    of late Republican date. Triangular interpuncts between words.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">60-84\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line2\" to=\"line2\">Line 2\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">65-78\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462503\">Syracusae\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523083\">Siracusa\u003C/placeName>\n                            \t\u003Cgeo>37.08415, 15.27628\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0100\" notAfter-custom=\"0050\">late Republican or\n                                Augustan\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" when=\"1922\">Excavated in December 1922 in the\n                            \u003CplaceName ref=\"https://pleiades.stoa.org/places/895916364\">necropolis of contrada Canalicchio\u003C/placeName>\u003Cgeo>37.0750458, 15.2590759\u003C/geo>\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Display Sector D, cippi funerari\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#JP\">2006.07.25; 2013.09.30\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-01-20\" who=\"#JP\">Jonathan Prag checked the EpiDoc and added images\u003C/change>\n                \u003Cchange when=\"2017-05-21\" who=\"#JP\">Jonathan Prag revised the EpiDoc, encoded the text, added translation and brief commentary, further bibliography, and new images\u003C/change>\n                \u003Cchange when=\"2020-09-10\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-17\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000618_tiled.tif\" height=\"4320px\" width=\"3504px\">\n                \u003Cdesc>Photo J. Prag\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000618.jpg\" height=\"4320px\" width=\"3504px\">\n                \u003Cdesc>Photo J. Prag\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic000618_text_tiled.tif\" height=\"3016px\" width=\"4356px\">\n                \u003Cdesc>Text detail, photo J. Prag\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000618_text.jpg\" height=\"3016px\" width=\"4356px\">\n                \u003Cdesc>Text detail, photo J. Prag\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n        \u003Csurface type=\"threequarter\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000618_threequarter_tiled.tif\" height=\"2148px\" width=\"2978px\">\n                \u003Cdesc>Three-quarter view, photo J. Prag\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000618_threequarter.jpg\" height=\"2148px\" width=\"2978px\">\n                \u003Cdesc>Three-quarter view, photo J. Prag\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cexpan>\u003Cabbr>Cn\u003C/abbr>\u003Cex>aie\u003C/ex>\u003C/expan>\u003C/name> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cname>Marci\u003C/name>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cexpan>\u003Cabbr>Cn\u003C/abbr>\u003Cex>aii\u003C/ex>\u003C/expan>\u003C/name>\u003C/persName> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cexpan>\u003Cabbr>f\u003C/abbr>\u003Cex>ilie\u003C/ex>\u003C/expan>\u003C/persName> \u003Cg ref=\"#interpunct\">·\u003C/g> salve\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>Gnaeus Marcius, son of Gnaeus, farewell!\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The inscription has never been formally published, although first reported in the 1929 guide to the\n                    museum by Libertini, and subsequently noted by various scholars (see under editions). A parallel for\n                    the depiction of a plough on a tombstone has yet to be identified. Manganaro 1962 noted that the\n                    plough occurs on coinage, and in some contexts at least serves as a symbol for the foundation of a\n                    colonia (see, e.g. E. T. Salmon, Roman colonization under the Republic (1969) plate 10, with\n                    reference to a Trajanic coin). It seems more likely, in this instance, that the stone provides\n                    interesting support for the existence of a distinct, self-identifying category of Roman aratores on\n                    the island (in distinction to the commonly attested negotiatores), extensively referenced in\n                    Cicero's In Verrem orations of 70 BC. A parallel for the vocative+salve in Latin can be seen in a\n                    bilingual funerary inscription for a Quintus Domitius from Catania of similar date (\u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic000348\">ISic000348\u003C/ref>).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Libertini\u003C/author>\n                        \u003Cdate>1929\u003C/date>\n                        \u003CcitedRange>126\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/AHZ3TB5P\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001872\">https://biblio.inscriptiones.org/epig10001872\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1962\u003C/date>\n                        \u003CcitedRange>499 n.72\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q7KT855P\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001851\">https://biblio.inscriptiones.org/epig10001851\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Rizzo\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>66-67 no.28\u003C/citedRange>\n                        \u003Cptr target=\"https://zotero.org/groups/382445/items/K83EWCEC\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001640\">https://biblio.inscriptiones.org/epig10001640\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Wilson\u003C/author>\n                        \u003Cdate>1990\u003C/date>\n                        \u003CcitedRange>356 n.97\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/VF9MMBI9\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001509\">https://biblio.inscriptiones.org/epig10001509\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Tribulato\u003C/author>\n                        \u003Cdate>2012\u003C/date>\n                        \u003CcitedRange>310\u003C/citedRange>\n                        \u003Cptr target=\"https://zotero.org/groups/382445/items/X5ZZ4GPT\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001557\">https://biblio.inscriptiones.org/epig10001557\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
