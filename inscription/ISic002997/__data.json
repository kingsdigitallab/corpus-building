 survive.",{"_":44,"ana":45,"ref":46},"chiselled","#execution.chiselled","http://www.eagle-network.eu/voc/writing/lod/1",{"dimensions":48},[49,55],{"type":50,"l":51,"from":52,"to":52,"h":53,"unit":54},"letterHeight","Line 1-16","line1","4-5","mm",{"type":56,"l":57,"from":52,"to":58,"unit":54},"interlinear","Interlineation line 1 to 2","line2",{"_":60,"notBefore":61,"notAfter":62},"35 CE",35,36,[64],{"_":65,"type":66,"ref":67},"Buscemi","modern","http://pleiades.stoa.org/places/462120",[69],[70,71],37.0790016,14.8863106,{"_":73,"type":74,"geo":75},"Original discovery not recorded.","found",null,{"_":77,"type":78,"geo":75},"Depositi, Mag C (Buscemi)","observed",[80,87,89],{"n":81,"url":82,"height":83,"width":84,"desc":85,"surfaceType":86},"screen","ISic002997_tiled.tif","7360px","4912px","I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana; photo J. Prag 2013-10-02","front",{"n":81,"url":88,"height":83,"width":84,"desc":85,"surfaceType":86},"ISic002997_copy2_tiled.tif",{"n":81,"url":90,"height":83,"width":84,"desc":85,"surfaceType":91},"ISic002997_rear_tiled.tif","rear","Italy","Sicily","Siracusa",{"_":96,"role":97,"ref":98,"museum":99},"Museo Archeologico Regionale Paolo Orsi","museum","http://sicily.classics.ox.ac.uk/museum/097",{"type":97,"name":96,"uri":98,"description":100,"location":101},"An outstanding collection of archaeological material. The collection ranges from Siracusa and the surrounding territory (Megara Hyblaia, Akrai, Kasmenai etc) to encompass all of eastern Sicily, reaching as far as Gela and Agrigento. The ground floor covers prehistory through to the end of the Classical period. Upstairs includes Hellenistic and Roman Syracuse and a display of material from the catacombs. A gallery displaying material from Hellenistic eastern Sicily is in preparation.",{"settlement":94,"region":94,"country":92,"address":102,"geo":103},"Viale Teocrito 66,