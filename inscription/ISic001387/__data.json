           Commons-Attribution 4.0 licence.\n                    \u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>unknown\u003C/settlement>\n                        \u003Crepository>unknown\u003C/repository>\n                     \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>Neither the material nor the form of this inscription is recorded.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone\" ref=\"http://www.eagle-network.eu/voc/material/lod/2.html\">stone\u003C/material>\n                                    \u003CobjectType ana=\"#object.unknown\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/2.html\">unknown\u003C/objectType>\n                                    \u003Cdimensions> \n                                        \u003Cheight unit=\"cm\"/>\n                                        \u003Cwidth unit=\"cm\"/>\n                                        \u003Cdepth unit=\"cm\"/>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n              