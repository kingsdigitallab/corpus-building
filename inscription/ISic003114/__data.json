    \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/6542148\">Marsala\u003C/placeName>\n                            \t\u003Cgeo>37.80355, 12.4379\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0250\" notAfter-custom=\"-0150\" evidence=\"lettering\" precision=\"low\">Suggested to date to the later 3rd or earlier 2nd century BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\">Original discovery not recorded.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">On display.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#MM\">Metcalfe 2015.04.18\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n     