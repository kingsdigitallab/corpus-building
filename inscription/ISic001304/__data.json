geo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0050\" notAfter-custom=\"0200\" precision=\"low\">From the second half of the 1st cent. to the end of the 2nd cent.  CE(Korhonen)\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1762\">Described as 'found in the city of Catania, by Torremuzza\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Observed by Torremuzza and Kaibel in Museo dei Benedettini, later by Korhonen in magazzino del cortile, Collezione dei Benedettini (formerly sala VI.102)\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-08\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-05-17\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, apparatus, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001304_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001304.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cw>\u003Cexpan>\u003Cabbr>\u003Cchoice>\u003Ccorr>Θ\u003C/corr>\u003Csic>Ο\u003C/sic>\u003C/choice>\u003C/abbr>\u003Cex>εοῖς\u003C/ex>\u003C/expan>\u003C/w> \u003Cw>\u003Cexpan>\u003Cabbr>Κ\u003C/abbr>\u003Cex>ατακθονίοις\u003C/ex>\u003C/expan>\u003C/w>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>Ἰουλία\u003C/name> \u003Cname>Δομιτία\u003C/name>\u003C/persName> \u003Cw>ἔζησεν\u003C/w>\n                    \u003Clb n=\"3\"/>\u003Cw>ἔτη\u003C/w> \u003Chi rend=\"supraline\">\u003Cnum value=\"7\">ζ\u003C/num>\u003C/hi>, \u003Cw>μῆνας\u003C/w> \u003Chi rend=\"supraline\">\u003Cnum value=\"3\">γ\u003C/num>\u003C/hi>, \u003Cw>ἡμέρας\u003C/w> \u003Chi rend=\"supraline\">\u003Cnum value=\"5\">ε\u003C/num>\u003C/hi>.\n                    \u003Clb n=\"4\"/>\u003Cw>Γονεῖς\u003C/w> \u003Cw>ἐποίησ\u003Cchoice>\u003Ccorr>α\u003C/corr>\u003Csic>λ\u003C/sic>\u003C/choice>ν\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photographs\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 1\">\u003Cnote>lapis: Ο\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 3\">\u003Cnote>lapis: ΕΠΟΙΗΣΛΝ\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf\">\n                \u003Cp>To the underworld spirits. Iulia Domitia lived for 7 years, 3 months, and 5 days. Her parents made this.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>Agli Dei inferi. Giulia Domizia visse sette anni, tre mesi, cinque giorni. I suoi genitori realizzarono questo.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#martaf\">\n                \u003Cp>The structure of the epitaph is the most common for pagan epitaphs, with the invocation to the underworld spirits, the name of the deceased in nominative and his/her age, the names of the dedicant (in this case, the parents, who are not named, see also IG 14.483 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001306\">ISic001306\u003C/ref>) and the dedication verb in the aorist form (Korhonen 2004: 85-86). The \u003Cforeign xml:lang=\"la\">nomen\u003C/foreign> \u003Cforeign xml:lang=\"grc\">Δομιτία\u003C/foreign> from the \u003Cforeign xml:lang=\"la\">gens Domitia\u003C/foreign> may have spread especially since the reign of Nero: from prosopography and palaeography, therefore, the inscription can be dated from the mid-1st century CE to the end of the 2nd century CE.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0481\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>98\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>3.5704\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>151 no. 32\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SPTB56H3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1784\u003C/date>\n                        \u003CcitedRange>159 no. 35\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7PSFHSUH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002083\">https://biblio.inscriptiones.org/epig10002083\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrara\u003C/author>\n                        \u003Cdate>1829\u003C/date> \n                        \u003CcitedRange>346 no. 2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/82T4IWDN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002005\">https://biblio.inscriptiones.org/epig10002005\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\n\t       \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n\t           \u003CcitedRange>85-86\u003C/citedRange>\n\t           \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n\t       \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n\t   \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
