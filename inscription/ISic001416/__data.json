/idno>\n                \u003Cidno type=\"EDR\"/>\n                \u003Cidno type=\"EDH\"/>\n                \u003Cidno type=\"EDCS\">38000220\u003C/idno>\n                \u003Cidno type=\"PHI\">178081\u003C/idno>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic001416\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-22\">10.5281/zenodo.4385910\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative Commons-Attribution 4.0 licence.\u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Gela\u003C/settlement>\n                        \u003Crepository role=\"museum\" ref=\"http://sicily.classics.ox.ac.uk/museum/010\">Museo Archeologico Regionale di Gela\u003C/repository>\n                        \u003Cidno type=\"inventory\">9361\u003C/idno>\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>An irregular plaque of 'pietra arenaria', of irregular shape, broken into three joining fragments, with a section missing from the lower edge; the text is complete.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.sandstone\" ref=\"http://www.eagle-network.eu/voc/material/lod/75.html\">sandstone\u003C/material>\n                                    \u003CobjectType ana=\"#object.plaque\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/259\">plaque\u003C/objectType>\n                                    \u003Cdimensions>\u003C!--Brugnone-->\n                                        \u003Cheight unit=\"cm\">72\u003C/height>\n                                        \u003Cwidth unit=\"cm\">42\u003C/width>\n                                        \u003Cdepth unit=\"cm\">6\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragments.contiguous\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>Text over four lines to fit the width of the stone, below a menorah\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage ana=\"#text_condition.complete\"/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Crudely incised / scratched letters, Alpha with downard left-sloping bar, eyes of the beta are not closed, epsilon is lunate, sigma is lunate but with straight lines.\u003C/p>\n                                \u003Clocus from=\"line2\" to=\"line5\">Line 2-5\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">40-70\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462423\">Philosophiana\u003C/placeName>\t\t\n                                \u003Coffset>near\n                                    \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2524200\">Mazzarino\u003C/placeName>\n                                \u003C/offset>\n                            \t\u003Cgeo>37.30188, 14.20959\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0401\" notAfter-custom=\"0500\" evidence=\"lettering textual-context\">5th century CE (so Brugnone)\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found in contrada Fontanazzi, in the eastern part of the Sofiana cemetery, re-use in a boundary wall between two lots of the cemetery\u003Cgeo>37.3185981,14.3024473\u003C/geo>\u003C/provenance>\u003C!-- location of the eastern necropolis at Sofiana -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">none\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2025-03-12\" who=\"#JP\">Jonathan Prag revised from publication\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001416_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001416.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\" source=\"https://www.zotero.org/groups/382445/items/I6XXZEB6\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cg ref=\"#menorah\">&#x1f54e;\u003C/g>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>Ἀττίν\n                    \u003Clb n=\"3\" break=\"no\"/>ις\u003C/name>\u003C/persName> \u003Cchoice>\u003Corig>βρε\u003Csupplied reason=\"omitted\">σ\u003C/supplied>β\n                    \u003Clb n=\"4\" break=\"no\"/>ύτερο\n                    \u003Clb n=\"5\" break=\"no\"/>ς\u003C/orig>\u003Creg>πρεσβύτερος\u003C/reg>\u003C/choice>\n               \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text after Brugnone and photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"3\">\u003Cnote>Brugnone: βρ&lt;ε&gt;σβ; lapis: βρϲβ, and since elsewhere lunate sigma is quadrate while epsilon is lunate, a weakly incised epsilon and omitted sigma seems more likely\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Attinis, (the) elder\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Presbuteros with initial B is also attested in \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic003631\">ISic003631\u003C/ref> (Siracusa). The inscription is for a member of the council of elders within the local jewish community. The form Attinis is not otherwise documented.\n                    \u003C!--epitaph of a member of the council of the elders, one Attinis (presbuteros). Attinis is an otherwise undocumented name, which Curbera concludes might be from Attinos or Attinas in a syncopated from from Attinios, a common Egyptian formation. C notes 'the scarcity of Latin names among Sicilian Jews'-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>1996.0808\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001283\">https://biblio.inscriptiones.org/epig10001283\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>56.1081\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>52.0908\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>46.1245\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>15.0599\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"BE\">\n                        \u003CcitedRange>1958.0563\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/77P5MNIR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001282\">https://biblio.inscriptiones.org/epig10001282\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIJ\">\n                        \u003CcitedRange>1.653b\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/ACIZXQF2\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000582\">https://biblio.inscriptiones.org/epig10000582\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Noy\u003C/author>\n                        \u003Cdate>1993\u003C/date>\n                        \u003CcitedRange>no.157\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/IBA4JMAN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000166\">https://biblio.inscriptiones.org/epig10000166\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Bonomi\u003C/author>\n                        \u003Cdate>1964\u003C/date>\n                        \u003CcitedRange>200 fig.35\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/BBEDUUG3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002127\">https://biblio.inscriptiones.org/epig10002127\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Brugnone\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>297-298 no.3 fig.4\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/I6XXZEB6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002118\">https://biblio.inscriptiones.org/epig10002118\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
