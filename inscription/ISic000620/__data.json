              \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Siracusa\u003C/settlement>\n                        \u003Crepository role=\"site\" ref=\"http://sicily.classics.ox.ac.uk/museum/160\">Area archeologica della Neapolis, Orecchio di Dionisio e Teatro Greco\u003C/repository>\n                        \u003C!--No inventory number found-->\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"la\">Latin\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>four separate blocks each bearing one or more letters, which appear to join up to form part of a monumental text\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.limestone\" ref=\"http://www.eagle-network.eu/voc/material/lod/66.html\">limestone\u003C/material>\n                                    \u003CobjectType ana=\"#object.block\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/189.html\">block\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003C!--combined measurements from Orsi NSA-->\n                                        \u003Cheight unit=\"cm\">60\u003C/height>\n                                        \u003Cwidth unit=\"cm\">298 (combined)\u003C/width>\n                                        \u003Cdepth unit=\"cm\">20-37\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003C!--measurement from Orsi-->\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">375\u003C/height>\n                                \u003C/dimensions>\n                                \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462503\">Syracusae\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523083\">Siracusa\u003C/placeName>\n                            \t\u003Cgeo>37.08415, 15.27628\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0001\" notAfter-custom=\"0200\" evidence=\"lettering\">first or second century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"first-recorded\" when=\"1889\">First reported by Paolo Orsi in 1889, as lying in the garden of the custodian's hut near the amphitheatre, and as coming originally from the amphitheatre\u003Cgeo>37.074191, 15.278876\u003C/geo>\u003C/provenance>\u003C!-- geo for the amphitheatre -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">No autopsy, but last reported as in the chiesetta di S. Nicolò, just north of the amphitheatre at the entrance to the archaeological park.\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.building\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/86.html\">building\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-11-17\" who=\"#JP\">Jonathan Prag added a bibl element\u003C/change>\n                \u003Cchange when=\"2018-02-23\" who=\"#TA\">Tuuli Ahlholm revised the Epidoc, encoded the text and added an apparatus\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2020-10-28\" who=\"#JP\">Jonathan Prag revised bibliography and made minor edits\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-17\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2021-06-11\" who=\"#JP\">Jonathan Prag revised file further from published information from Orsi\u003C/change>\n                \u003Cchange when=\"2024-06-28\" who=\"#JP\">Jonathan Prag added bibliography and commentary, amended date\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000620_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000620.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003CpersName type=\"attested\">\u003Cname>Betilie\u003Csupplied reason=\"lost\">nus\u003C/supplied>\u003C/name>\u003C/persName> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text after Manganaro and photographs\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Orsi in NSA reports the dimensions of all four blocks: (a), reading TIL, is 1.2 x 0.6 x 0.25m; (b) reading E is 0.45 x 0.6 x 0.37; (c) reading IE is 0.48 x 0.6 x 0.31; (d) reading B is 0.85 x 0.6 x 0.2. The inscription blocks were photographed in the 1970s and 1980s seemingly outside the entrance to the small church of S. Nicolo at the entrance to the archaeological park, but they are no longer in that position and have not recently been observed. Buonocore reports autopsy in 1987. The dating of the amphitheatre is much disputed, between the Augustan period and the early third century CE, and since the inscription can only itself be dated on prosopographical or palaeographical grounds, the arguments either for its date, or to use it to date the amphitheatre rapidly become circular. Wilson has argued strongly for an Augustan or earlier first century date. Certainly it is difficult to assert on palaeographical grounds that the inscription must be second or even early third century CE, and a first century date seems at least as possible.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Cdate>1899\u003C/date>\n                        \u003CcitedRange>385\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>1989.0342i\u003C/citedRange>\n                        \u003Cref target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gentili\u003C/author>\n                        \u003Cdate>1973\u003C/date>\n                        \u003CcitedRange>69 fig.66\u003C/citedRange>\n                        \u003Cref target=\"https://www.zotero.org/groups/382445/items/GX47AQI8\"/>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Wilson\u003C/author>\n                        \u003Cdate>1980\u003C/date>\n                        \u003CcitedRange>2217-2218, 2230 tav.2.1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/TXDV3V94\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002581\">https://biblio.inscriptiones.org/epig10002581\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1988\u003C/date>\n                        \u003CcitedRange>55 n.273\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/RZSFKACR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001427\">https://biblio.inscriptiones.org/epig10001427\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"EAOR\">\n                        \u003CcitedRange>119 no.84, tav. XXXIII fig. 2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/TC8M5CGM\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002647\">https://biblio.inscriptiones.org/epig10002647\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Wilson\u003C/author>\n                        \u003Cdate>1990\u003C/date>\n                        \u003CcitedRange>40 n.68, 82 n.167\u003C/citedRange>\n         