ge":290,"thumb":291,"title":105},"ISic000934.jpg","ISic000934-thumb.jpg","\u003C?xml version='1.0' encoding='UTF-8'?>\n\u003C?xml-model href=\"https://epidoc.stoa.org/schema/latest/tei-epidoc.rng\" type=\"application/xml\" schematypens=\"http://relaxng.org/ns/structure/1.0\"?>\n\u003C?xml-model href=\"../schematron/ircyr-checking.sch\" schematypens=\"http://purl.oclc.org/dsdl/schematron\"?>\n\u003CTEI xmlns=\"http://www.tei-c.org/ns/1.0\" xmlns:xi=\"http://www.w3.org/2001/XInclude\" xml:lang=\"en\">\n    \u003CteiHeader>\n        \u003CfileDesc>\n            \u003CtitleStmt>\n                \u003Ctitle>Epitaph of Zodoros\u003C/title>\n                \u003Ceditor ref=\"#JP\">Jonathan Prag\u003C/editor>\n                \u003Cprincipal ref=\"#JP\">Jonathan Prag\u003C/principal>\n                \u003Cfunder>John Fell OUP Research Fund\u003C/funder>\n\t   \u003Cfunder>\u003Cref target=\"https://cordis.europa.eu/project/id/885040\">ERC Advanced Grant no.885040\u003C/ref>\u003C/funder>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JP\" ref=\"http://orcid.org/0000-0003-3819-8537\">Jonathan Prag\u003C/name>\n                    \u003Cresp>original data collection and editing\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JCu\" ref=\"http://orcid.org/0000-0002-6686-3728\">James Cummings\u003C/name>\n                    \u003Cresp>conversion to EpiDoc\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JCh\" ref=\"http://orcid.org/0000-0001-6823-0265\">James Chartrand\u003C/name>\n                    \u003Cresp>site construction and encoding\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"VV\" ref=\"http://orcid.org/0000-0002-9695-0240\">Valeria Vitale\u003C/name>\n                    \u003Cresp>editing of geo data\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"MM\">Michael Metcalfe\u003C/name>\n                    \u003Cresp>museum data collection\u003C/resp>\n                \u003C/respStmt>\n\t    \u003CrespStmt>\n     \t       \u003Cname xml:id=\"SS\" ref=\"https://orcid.org/0000-0003-3914-9569\">Simona Stoyanova\u003C/name>\n     \t       \u003Cresp>standardisation of template and tidying up encoding\u003C/resp>\n \t   \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"VM\" ref=\"https://orcid.org/0000-0002-7122-2511\">Valentina Mignosa\u003C/name>\n                    \u003Cresp>encoding text division and checking the edition\u003C/resp>\n                \u003C/respStmt>\n\t    \u003CrespStmt>\n                    \u003Cname xml:id=\"system\">system\u003C/name>\n                    \u003Cresp>automated or batch processes\u003C/resp>\n                \u003C/respStmt>\n            \u003C/titleStmt>\n            \u003CpublicationStmt>\n                \u003Cauthority>I.Sicily\u003C/authority>\n                \u003Cidno type=\"filename\">ISic000934\u003C/idno>\n                \u003Cidno type=\"TM\">492564\u003C/idno>\n                \u003Cidno type=\"EDR\"/>\n                \u003Cidno type=\"EDH\"/>\n                \u003Cidno type=\"EDCS\">39102091\u003C/idno>\n                \u003Cidno type=\"PHI\">140415\u003C/idno>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic000934\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-17\">10.5281/zenodo.4340114\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative Commons-Attribution 4.0 licence\u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Siracusa\u003C/settlement>\n                        \u003Crepository role=\"museum\" ref=\"http://sicily.classics.ox.ac.uk/museum/097\">Museo Archeologico Regionale Paolo Orsi\u003C/repository>\n                        \u003Cidno type=\"inventory\">68\u003C/idno>\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>A fragment of grey marble, broken on all sides, but with the text applied after such cutting of the stone.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble\u003C/material>\n                                    \u003CobjectType ana=\"#object.plaque\" ref=\"https://www.eagle-network.eu/voc/objtyp/lod/259.html\">plaque\u003C/objectType>\n                                    \u003Cdimensions>\u003C!-- from Orsi -->\n                                        \u003Cheight unit=\"cm\">18\u003C/height>\n                                        \u003Cwidth unit=\"cm\">23\u003C/width>\n                                        \u003Cdepth unit=\"cm\"/>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.damaged\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>The text maintains a regular left margin, ignoring the irregularity of the form of the stone, and filling the space to the right; however, the cutter ran out of space at the end of the stone, and inserted the remaining letters of the word Δεκεμβρίῳ and the concluding staurogram, firstly at the end of line 2 (letter β) and then, under a 'paragraphos' line to the left of the main text of line 2, level with lines 3 (letters ριω) and 4 (staurogram)\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage ana=\"#text_condition.complete\"/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>plainly cut, relatively regular letters, in a curved groove rather than v-cut, without serifs. Alpha has an oblique crossbar, sloping down to left, and a horizontal stroke to left off the top of the letter; beta has closed eyes, the upper is smaller; epsilon is lunate (except for the first in line 5 which is rectilinear); zeta has regular Z form; kappa has near full-legnth arms; mu has cursive form; omicron is full size; rho has closed eye; sigma is lunate; omega is lunate.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462503\">Syracusae\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523083\">Siracusa\u003C/placeName>\n                            \t\u003Cgeo>37.08415, 15.27628\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0301\" notAfter-custom=\"0600\">4th - 6th century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1875\">From the \u003CplaceName ref=\"https://pleiades.stoa.org/places/560149180\">catacomb of S. Giovanni\u003C/placeName>\u003Cgeo>37.0767995, 15.2848558\u003C/geo>, possibly from the explorations of c.1874 by Cavallari.\u003C/provenance>\u003C!-- representative geo for the catacomb -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                   \u003Cchange when=\"2020-11-04\" who=\"#VM\">Valentina Mignosa encoded text division, cleaned up the autogenerated text and checked the edition\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t\u003Cchange when=\"2020-12-17\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2021-06-20\" who=\"#JP\">Jonathan Prag edited file from available published data and photograph\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000934_tiled.tif\" height=\"2448px\" width=\"3264px\">\n                \u003Cdesc>Photo Museo Arch. Reg. P. Orsi (2015), Aut. Assessorato Beni Culturali Regione Siciliana n.10681 del 06/05/2014\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000934.jpg\" height=\"2448px\" width=\"3264px\">\n                \u003Cdesc>Photo Museo Arch. Reg. P. Orsi (2015), Aut. Assessorato Beni Culturali Regione Siciliana n.10681 del 06/05/2014\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM #JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Ζόδωρος\u003C/name>\u003C/persName>\n                    \u003Clb n=\"2\"/>\u003Cw>ἀπὸ\u003C/w> \u003CplaceName ref=\"https://pleiades.stoa.org/places/678374\">Μά\n                    \u003Clb n=\"3\" break=\"no\"/>κρης κώ\n                    \u003Clb n=\"4\" break=\"no\"/>μης\u003C/placeName> \u003Cw>ἐτε\n                    \u003Clb n=\"5\" break=\"no\"/>λεύτησε\u003Chi rend=\"superscript\">ν\u003C/hi>\u003C/w>\n                    \u003Clb n=\"6\"/>\u003Cw>μηνὶ\u003C/w> \u003Crs type=\"month\">\u003Cw>Δε\n                    \u003Clb n=\"7\" break=\"no\"/>κεμ\n                    \u003Clb n=\"8\" break=\"no\"/>β\n                    \u003Clb n=\"9\" break=\"no\"/>\u003Cg ref=\"#paragraphos\"/>ρίῳ\u003C/w>\u003C/rs>\n                    \u003Clb n=\"10\"/>\u003Cg ref=\"#staurogram\">𐆠\u003C/g>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Orsi's apograph and photograph. Kaibel's text is the same as that of Carini (in terms of letters), although he claims to have used an apograph of Ioannis Arezzo di Targia, and notes that Mommen also undertook autopsy.\u003C/note>\n                    \u003C/app>\n                    \u003Capp loc=\"line 5\">\u003Cnote>Carini, Kaibel: ΛΕΥΤΗΣΕ\u003C/note>\u003C/app>\n                    \u003Capp loc=\"7-10\">\u003Cnote>As noted by Kaibel (and Mommsen), but not by Carini, the letters following the end of line 7 are inserted in sequence at the end of line 2 and the beginnings of lines 3 and 4, with a paragraphos line presumably intended to mark this fact to the left of line 2.\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Zodoros from Makre Kome (the village of Makre) died in the month of December. \u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Feissel, in addition to confirming the observation of previous editors that Zodoros should be understood as a variant of Diodoros, identifies the toponym of Makre Kome with the village of \u003CplaceName type=\"ancient\" ref=\"https://pleiades.stoa.org/places/678374\">Sarepta\u003C/placeName> in Phoenicia, which appears also in the mosaic map of Madaba, noting that the presence of individuals from the eastern Mediterranean in Syracuse is hardly surprising or unparalleled. \u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0117\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>36.1279.E.2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>36.0859\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Carini\u003C/author>\n                        \u003Cdate>1875\u003C/date>\n                        \u003CcitedRange>no.11\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/H7KWCRG9\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002090\">https://biblio.inscriptiones.org/epig10002090\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Cdate>2018\u003C/date>\n                        \u003CcitedRange>Tacc. 2, p. 2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RGRSRUHK\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002617\">https://biblio.inscriptiones.org/epig10002617\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGCVO\">\n                        \u003CcitedRange>99\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/9CT5ZWIK\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000394\">https://biblio.inscriptiones.org/epig10000394\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Feissel\u003C/author>\n                        \u003Cdate>1982\u003C/date>\n                        \u003CcitedRange>340-341\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/VHTQG3Z8\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002451\">https://biblio.inscriptiones.org/epig10002451\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
