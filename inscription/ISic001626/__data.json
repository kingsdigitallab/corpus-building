        \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0301\" notAfter-custom=\"0400\" precision=\"low\">4th century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found in a cemetery to the East of the chamber with paintings and graffiti on Via Vittorio Emanuele (Orsi).\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Observed by Orsi in Museo Civico, later by Korhonen in Magazzino del cortile, Museo Civico (formerly sala VI 123).\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-18\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001626_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001626.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #AA\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Σωκράτης\u003C/name>\u003C/persName>\n                    \u003Clb n=\"2\"/>\u003Cg ref=\"#christogram\">☧\u003C/g> \u003Cw>πάντων\u003C/w> \u003Cw>ὁ\u003C/w> \u003Cg ref=\"#christogram\">☧\u003C/g>\n                    \u003Clb n=\"3\"/>\u003Cw>φίλος\u003C/w> \u003Cw>ἐνθά\u003Clb n=\"4\" break=\"no\"/>δε\u003C/w> \u003Cw>κεῖται\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 2\">\u003Cnote>Orsi, Agnello, Wessel: πάντων ὁ | φίλος; Ferrua, Korhonen: πάντων | φίλος (Ο is part of the Christogram)\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>Sokrates, the friend of all, lies here.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>Sokrates, l’amico di tutti, giace qui.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>This epitaph has a formula very similar to pagan epitaphs, although the presence of the two Christograms denotes it as Christian. Ferrua and Korhonen read \u003Cforeign xml:lang=\"grc\">πάντων φίλος\u003C/foreign> with the interpretation of \u003Cforeign xml:lang=\"grc\">Ο\u003C/foreign> as part of the Christogram, which in this case should be interpreted as \u003Cforeign xml:lang=\"grc\">ὁ Χ(ριστό)ς\u003C/foreign> (the same hight of the letter \u003Cforeign xml:lang=\"grc\">O\u003C/foreign> compared to the other letters of the text is probably a misinterpretation of the stonecutter). However, there is no need to eliminate the article since other parallels of similar epithets denoting friendship and affection in Christian inscriptions, such as IMusCatania 178 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001359\">ISic001359\u003C/ref> and SEG 65. 791 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001633\">ISic001633\u003C/ref> present the names of the deceased followed by adjectives preceded by the article (\u003Cforeign xml:lang=\"grc\">Κάλλιστος καὶ | Εὐκαρπία οἱ φιλόστορ|γοι\u003C/foreign> and \u003Cforeign xml:lang=\"grc\">ὁ πανφίλ|ητος ... Εὐδέμων\u003C/foreign> respectively).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>196\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Agnello\u003C/author>\n                        \u003Cdate>1953\u003C/date>\n                        \u003CcitedRange>no.58)\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/D9BZFDDR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002211\">https://biblio.inscriptiones.org/epig10002211\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1918\u003C/date>\n                        \u003CcitedRange>62 no.3\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGCVO\">\n                        \u003CcitedRange>1022\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/9CT5ZWIK\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000394\">https://biblio.inscriptiones.org/epig10000394\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrua\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>110 no.416\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/759ZXHKQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001995\">https://biblio.inscriptiones.org/epig10001995\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
