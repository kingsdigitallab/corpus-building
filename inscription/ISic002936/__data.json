ectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462251\">Hybla Gereatis\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523866\">Paternò\u003C/placeName>\n                            \t\u003Cgeo>37.56675, 14.90254\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0300\" notAfter-custom=\"-0101\" precision=\"low\">3rd — 2nd century BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"recorded\">Said by Orsi in 1900 to have been found 'recently' in the garden of the (ex) convent of the Cappuccini, Paternò\u003Cgeo>37.562836, 14.893780\u003C/geo>, although Soraci 1958 notes that a reading is attributed to a local antiquarian c.1808\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cprovenance type=\"not-observed\" subtype=\"lost\">Lost.\u003C/provenance>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.honorific\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/69.html\">honorific\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-11-15\" who=\"#JP\">Jonathan Prag revised file based on publications\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic002936_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic002936.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CroleName type=\"gymnasiarchos\">Γυμνασιάρχης\u003C/roleName>\n                    \u003Clb n=\"2\"/>Εὐβουλίδας\n                    \u003Clb n=\"3\"/>Ἡρακλείου\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n               \u003ClistApp>\u003Capp>\u003Cnote>Text after Orsi 1900, but corrected\u003C/note>\u003C/app>\n                   \u003Capp loc=\"line 1\">\u003Cnote>Cordiano notes the oddity of line 1 and speculates that we should read Γυμνασιαρχήσ[ας] \u003C/note>\u003C/app>\n                   \u003Capp loc=\"line 2\">\u003Cnote>Orsi: ΕΥΚΟΥΛΙΔΑΣ\u003C/note>\u003C/app>\n               \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Gymnasiarch Euboulidas, son of Herakleios\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The origin of the stone is uncertain, found together with ISic002946, according to Orsi, in the garden of the former convent of the Cappuchins, in the late C19 AD; however both are apparently reported in earlier antiquarian texts (see Soraci 1958, and Manganaro 1963), and Manganaro speculates that both perhaps come originally from nearby Civita, thought possibly to be the site of ancient Aetna-Inessa.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Cdate>1900\u003C/date>\n                        \u003CcitedRange>55 no.28\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/UE3VRJA7\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001707\">https://biblio.inscriptiones.org/epig10001707\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Soraci\u003C/author>\n                        \u003Cdate>1958\u003C/date>\n                        \u003CcitedRange>253\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/ZXGG3V8Z\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001571\">https://biblio.inscriptiones.org/epig10001571\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1963\u003C/date>\n                        \u003CcitedRange>57\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/M3UEW68D\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001850\">https://biblio.inscriptiones.org/epig10001850\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Cordiano\u003C/author>\n                        \u003Cdate>1997\u003C/date>\n                        \u003CcitedRange>38-39\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/DBDJ7RGP\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002064\">https://biblio.inscriptiones.org/epig10002064\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
