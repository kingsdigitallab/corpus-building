teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic030310_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic030310.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"a\">\n                    \u003Cab>\n                        \u003Cmilestone unit=\"face\" n=\"a\"/>\n                        \u003Clb n=\"1\"/>\u003ChandShift new=\"#h1\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Εϝπράσξενος\u003C/name>\n                        \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname cert=\"low\" type=\"patronym\">\u003Cseg part=\"I\">\u003Corig>ΣΙ\u003C/orig>\u003C/seg>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003C/name>\u003C/persName>\u003C/persName>\n                    \u003C/ab>\n                \u003C/div>\n                \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"b\">\n                    \u003Cab>\n                        \u003Cmilestone unit=\"face\" n=\"b\"/>\n                        \u003Clb n=\"1\"/>\u003ChandShift new=\"#h2\"/>\u003Corig>Ε\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003C/ab>\n                \u003C/div>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Cordano, Di Stefano 2017\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>This document is part of a group of two tesserae (\u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic030309\">ISic030309\u003C/ref>-\u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic030310\">ISic030310\u003C/ref>) found during a survey in 1990 (14th of September) in the area north north of the temple of Athena, where a large deposit (c. 150) of lead tesserae was previously found.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Cordano, Di Stefano\u003C/author>\n                        \u003Cdate>2017\u003C/date>\n                        \u003CcitedRange>151, fig. 5a-b\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/CJTNS9AF\"/> \n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002354\">https://biblio.inscriptiones.org/epig10002354\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
