     \u003Cp>Tessera.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.metal.lead\" ref=\"https://www.eagle-network.eu/voc/material/lod/108.html\">lead\u003C/material>\n                                    \u003CobjectType ana=\"#object.tessera\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/276.html\">tessera\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">2.5\u003C/height>\n                                        \u003Cwidth unit=\"cm\">4.9\u003C/width>\n                                        \u003Cdepth unit=\"cm\"/>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragment\"/>\n                            \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.engraved\" ref=\"https://www.eagle-network.eu/voc/writing/lod/3.html\">engraved\u003C/rs>\n                                    \u003Cp>The text is placed on both sides of the fragment of the lamina.\u003C/p>\n                                    \u003Cdamage ana=\"#text_condition.incomplete\"/>\n                                \u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003C!-- Based on Glyph table: -->\n                                \u003Clocus from=\"line1\" to=\"line1\"/>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>                       \n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462126\">Kamarina\u003C/placeName>\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/6540853\">Santa Croce Camerina\u003C/placeName>\n                                \u003Cgeo>36.8736, 14.4507\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" evidence=\"lettering archaeological-context\" notBefore-custom=\"-0480\" notAfter-custom=\"-0450\">480—450 BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found in the archaeological area of Kamarina, inside a storeroom in the sanctuary of Athena, in contrada Cammarana\u003Cgeo>36.87316, 14.4415\u003C/geo>.\u003C/provenance> \u003C!-- double check coordinates -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">No Autopsy\u003C/provenance>\n                        \u003Cacquisition>Santa Croce Camerina, Museo Archeologico Regionale di Kamarina\u003C/acquisition>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n            \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>             \n            \u003Cxi:include href=\"../alists/charDecl.xml\">                 \n                \u003Cxi:fallback>                     \n                    \u003Cp>ISicily glyphs authority list\u003C/p>                 \n                \u003C/xi:fallback>             \n            \u003C/xi:include>         \n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.tessera.civic\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/130.html\">tessera (civic)\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-12-10\" who=\"#VM\">Valentina Mignosa added metadata and text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic030244_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic030244.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                    \u003Cab>\n                        \u003Cmilestone unit=\"face\" n=\"a\"/>\n                        \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Πυρ\u003Csupplied reason=\"lost\">ρί\u003C/supplied>ας\u003C/name>\u003C/persName>\n                    \n                        \u003Cmilestone unit=\"face\" n=\"b\"/>\n                        \u003Clb n=\"2\"/>\u003Cnum value=\"6\">hέκτα\u003C/num> φρ\u003Cunclear>ά\u003C/unclear>\u003Csupplied reason=\"lost\">τρα\u003C/supplied>\n                    \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Cordano 1992\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>This document is one of a large deposit (c. 150) of lead tesserae found in a storeroom of Athena's sanctuary at the archaeological site of Kamarina.\n                    The masculine name Πυρρίας, particularly common in Greek and non-Greek Sicily (see for Greek contexts e.g. \u003Cref target=\"http://zotero.org/groups/382445/items/F2GG87EQ\">SEG 38.0949; SEG 39.1008, l. 12\u003C/ref>; \n                    \u003Cref target=\"http://zotero.org/groups/382445/items/VC9IHZAR\">Arena, ISAMG I\u003C/ref>, nos.39 and 50; for non-Greek contexts e.g. \u003Cref target=\"http://zotero.org/groups/382445/items/CNP5CJ7G\">Arena, ISAMG II\u003C/ref>, no.122a), \n                    recurs on another tessera from the same deposit, \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic030254\">ISic030254\u003C/ref> (Cordano 1992, no. 127).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Cordano\u003C/author>\n                        \u003Cdate>1992\u003C/date>\n                        \u003CcitedRange>118a\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/XTMUJ38N\"/> \n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002437\">https://biblio.inscriptiones.org/epig10002437\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>42.0846\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>52.0920\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>54.0892\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
