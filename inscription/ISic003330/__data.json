es, uris and references","2021-01-19",{"surface":449},[450,465,472,483],{"type":451,"graphic":452},"front",[453,457,460,463],{"n":454,"url":63,"height":455,"width":456,"corresp":324,"desc":64},"screen","3680px","5520px",{"n":458,"url":459,"height":455,"width":456,"corresp":324,"desc":64},"print","ISic003330_A.jpg",{"n":454,"url":461,"height":455,"width":456,"corresp":362,"desc":462},"ISic003330_B_tiled.tif","Block B: Photo J. Prag, courtesy Parco archeologico di Tindari",{"n":458,"url":464,"height":455,"width":456,"corresp":362,"desc":462},"ISic003330_B.jpg",{"type":466,"graphic":467},"left",[468,470],{"n":454,"url":469,"height":455,"width":456,"corresp":324,"desc":64},"ISic003330_A_left_tiled.tif",{"n":458,"url":471,"height":455,"width":456,"corresp":324,"desc":64},"ISic003330_A_left.jpg",{"type":473,"graphic":474},"right",[475,477,479,481],{"n":454,"url":476,"height":455,"width":456,"corresp":324,"desc":64},"ISic003330_A_right_tiled.tif",{"n":458,"url":478,"height":455,"width":456,"corresp":324,"desc":64},"ISic003330_A_right.jpg",{"n":454,"url":480,"height":455,"width":456,"corresp":362,"desc":462},"ISic003330_B_right_tiled.tif",{"n":458,"url":482,"height":455,"width":456,"corresp":362,"desc":462},"ISic003330_B_right.jpg",{"type":484,"graphic":485},"rear",[486,490],{"n":454,"url":487,"height":488,"width":489,"corresp":362,"desc":462},"ISic003330_B_rear_tiled.tif","3024px","4032px",{"n":458,"url":491,"height":488,"width":489,"corresp":362,"desc":462},"ISic003330_B_rear.jpg",{"body":493},{"div":494},[495,533,542,544,547],{"type":83,"xml:space":496,"xml:lang":80,"resp":61,"div":497},"preserve",[498,517],{"type":499,"subtype":500,"n":501,"corresp":324,"ab":502},"textpart","fragment-physical","A",{"lb":503,"gap":506,"orig":514},{"n":504,"xml:id":505},"1","DhAAK",[507,512],{"reason":508,"extent":509,"unit":510,"xml:id":511},"lost","unknown","character","DhAAU",{"reason":508,"extent":509,"unit":510,"xml:id":513},"DhAAo",{"_":515,"xml:id":516},"ΕΡΩ","DhAAe",{"type":499,"s