sc>\n                            \u003ChandNote>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462187\">Drepanum\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2522876\">Trapani\u003C/placeName>\n                            \t\u003Cgeo>38.0176, 12.53617\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"1769\" notAfter-custom=\"1800\">A forgery produced in the immediate decades after the publication of Torremuzza 1769\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Reportedly found in 1768 during work on the Chiesa di San Pietro, Trapani\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cprovenance type=\"not-observed\" subtype=\"lost\">Lost (manuscript copy only)\u003C/provenance>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-09-06\" who=\"#JP\">Jonathan Prag revised file from publications\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003559_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003559.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>πάσῃ \u003Cgap reason=\"lost\" unit=\"character\" quantity=\"1\"/> ἀρετῇ \u003Cgap reason=\"lost\" quantity=\"3\" unit=\"character\"/> σοφία \u003Cgap reason=\"lost\" quantity=\"2\" unit=\"character\"/> καὶ \u003Corig>ΤΗ\u003C/orig> \u003Corig>ΠΙΤΑΝΗ\u003C/orig>\n                    \u003Clb n=\"3\"/>Κλαυδίου \u003Cgap reason=\"lost\" quantity=\"2\" unit=\"character\"/> \u003Cchoice>\u003Corig>Γαυου\u003C/orig>\u003Creg>Γαΐου\u003C/reg>\u003C/choice>\n                    \u003Clb n=\"4\"/>ἐποίησε καὶ ἐνεωτέ\u003Csupplied reason=\"lost\">ρισε\u003C/supplied>\n                    \u003Clb n=\"5\"/>μνήμης χάριν\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on comparison of Kaibel, Filippi and SEG, prioritising Kaibel's transcription of the manuscript tradition.\u003C/note>\u003C/app>\n                    \u003Capp loc=\"1\">\u003Cnote>Kaibel notes that the likely source text, IG XIV.282, has ΕΠΙ ΤΑΜΙΑ misread as ΤΗ ΠΙΤΑΝΗ by Torremuzza\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>As Korhonen lucidly details, the inscription is one of a series of late 18th century local antiquarian forgeries, constructed on the basis of existing texts, principally those to be found in the 1769 first edition of the corpus of Torremuzza. Kaibel classed the entire series already as forgeries in IG XIV.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0028*\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>52.0904\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Filippi\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>75 no. 13/4\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/PIPP325H\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001991\">https://biblio.inscriptiones.org/epig10001991\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Filippi\u003C/author>\n                        \u003Cdate>2005\u003C/date>\n                        \u003CcitedRange>68 no.13/4\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/WBLZEBQ8\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002296\">https://biblio.inscriptiones.org/epig10002296\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Korhonen\u003C/author>\n                        \u003Cdate>2017\u003C/date>\n                        \u003CcitedRange>198\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/92ITYJC6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001484\">https://biblio.inscriptiones.org/epig10001484\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
