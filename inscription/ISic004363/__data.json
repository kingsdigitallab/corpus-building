ypeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t   \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t   \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                    \u003Cchange when=\"2022-02-03\" who=\"#JP\">Jonathan Prag edited file from autopsy notes and publication\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic004363_tiled.tif\" height=\"2777px\" width=\"2456px\">\n                \u003Cdesc>Photo J. Prag, 2014-09-11, by permission of Museo Archeologico Regionale Paolo Orsi\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic004363.jpg\" height=\"2777px\" width=\"2456px\">\n                \u003Cdesc>Photo J. Prag, 2014-09-11, by permission of Museo Archeologico Regionale Paolo Orsi\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n        \u003Csurface type=\"rear\">\n            \u003Cgraphic n=\"screen\" url=\"ISic004363_rear_tiled.tif\" height=\"2627px\" width=\"2245px\">\n                \u003Cdesc>Photo J. Prag, 2014-09-11, by permission of Museo Archeologico Regionale Paolo Orsi\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic004363_rear.jpg\" height=\"2627px\" width=\"2245px\">\n                \u003Cdesc>Photo J. Prag, 2014-09-11, by permission of Museo Archeologico Regionale Paolo Orsi\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Εὐκαρπί\u003Csupplied reason=\"lost\">α\u003C/supplied>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"2\"/>χρηστ\u003Csupplied reason=\"lost\">ὲ\u003C/supplied>\n                    \u003Clb n=\"3\"/>χαῖρ\u003Csupplied reason=\"lost\">ε\u003C/supplied>\n                    \u003Clb n=\"4\"/>ἔζη\u003Cunclear>σ\u003C/unclear>\u003Csupplied reason=\"lost\">εν\u003C/supplied>\n                    \u003Clb n=\"5\"/>ἔτη \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\">\u003Cdesc>age\u003C/desc>\u003C/gap>\n                \u003C/ab>\n            \u003C/div> \n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text based on autopsy\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>Eukarpia, a good woman, farewell, she lived [--] years\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The layout of the text suggests \u003Cforeign xml:lang=\"grc\">ἔζησεν\u003C/foreign> in full, but other Greek funerary texts from Centuripe suggest that \u003Cforeign xml:lang=\"grc\">ἔζησες\u003C/foreign> might be possible.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1907\u003C/date>\n                        \u003CcitedRange>494\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
