n                    \u003Cp>ISicily glyphs authority list\u003C/p>                 \n                \u003C/xi:fallback>             \n            \u003C/xi:include>         \n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.ownership\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/105\">ownership\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-04-13\" who=\"#VM\">Valentina Mignosa added metadata, text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020481_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020481.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\" style=\"text-direction:anti-clockwise\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cseg part=\"I\">Γοργε\u003C/seg>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003C/name>\u003C/persName> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Cunclear>υ\u003C/unclear>φος\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Arena (IGASM 2)\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#VM\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp>If the inscription is to be read anti-clockwise, as if it were arranged in a circular way, \n                    Arena proposes to integrate it as Γοργε[ῖος κόρ]υφος, variation of the Homeric \n                    Γοργείη κεφαλή ('the head of the Gorgon', Il. 5.741; Od. 11.634).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IGASM2[2]\">\n                        \u003Cauthor>Arena\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>34, pl. XIII.3\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/65P7AM6N\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001373\">https://biblio.inscriptiones.org/epig10001373\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>45.1360\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGDS1\">\n                         \u003Cauthor>Dubois\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>\n                            \u003Cref target=\"http://www.persee.fr/doc/efr_0000-0000_1989_cat_119_1\">145\u003C/ref>\n                            \u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RWXNUFEX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000187\">https://biblio.inscriptiones.org/epig10000187\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>29.0863\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
