d files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-10-13\" who=\"#MB\">Maria Barbieri encoded text\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001261_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001261.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" resp=\"#MB\" xml:lang=\"grc\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΝΑΝ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\u003Corig>ΠΙΕ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΟΝΙΕ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΥΝΟΑ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \n                    \u003Clb n=\"3\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΑΣΚΑΙΤΑ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΜΑΥΥΟ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n       