t\" url=\"ISic000143.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#TA #JP\" source=\"#http://zotero.org/groups/382445/items/ZP8DE8KS\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cw part=\"I\" n=\"5\">Ces\u003C/w>\u003C/name>\u003C/persName> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"10\"/>\n                    \u003Clb n=\"2\"/>\u003Corig n=\"15\">ESTV\u003C/orig> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"20\"/>\n                    \u003Clb n=\"3\"/>\u003Cnum atLeast=\"15\">\u003Cw n=\"25\">XV\u003C/w>\u003C/num> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"30\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"edition\" subtype=\"simple-lemmatized\" resp=\"#VF\">\n                \u003Cab>\n                    \u003Cw n=\"5\" lemma=\"NOLEMMA\">Ces\u003C/w>\n                    \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"10\">\u003Cdesc>[-?-]\u003C/desc>\u003C/gap>\n                    \u003Corig n=\"15\">ESTV\u003C/orig>\n                    \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"20\">\u003Cdesc>[-?-]\u003C/desc>\u003C/gap>\n                    \u003Cw n=\"25\" lemma=\"XV\">XV\u003C/w>\n                    \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" n=\"30\">\u003Cdesc>[-?-]\u003C/desc>\u003C/gap>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003Cli