u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-06-11\" who=\"#JP\">Jonathan Prag made initial revision of the EpiDoc, added translation\u003C/change>\n                \u003Cchange when=\"2017-07-06\" who=\"#JP\">Jonathan Prag added elements for parallel Italian\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2023-09-17\" who=\"#JP\">Jonathan Prag revised text, and some metadata\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001359_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001359.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Κάλλιστ\u003Chi rend=\"ligature\">ος\u003C/hi>\u003C/name>\u003C/persName> καὶ \u003CpersName type=\"attested\">\u003Cname>Ε\u003Cunclear>ὐ\u003C/unclear>\n                    \u003Clb n=\"2\" break=\"no\"/>καρπία\u003C/name>\u003C/persName> οἱ φιλ\u003Chi rend=\"ligature\">όσ\u003C/hi>τορ\n                    \u003Clb n=\"3\" break=\"no\"/>γοι ζῶν\u003Chi rend=\"tall\">τ\u003C/hi>ες ἑαυ\u003Chi rend=\"tall\">τ\u003C/hi>οῖς\n                    \u003Clb n=\"4\"/>ἐποίησαν καὶ \u003Chi rend=\"tall\">τ\u003C/hi>οῖς\n                    \u003Clb n=\"5\"/>ἰδίοις \u003Cg ref=\"#interpunct\">·\u003C/g> εἰρήνη \u003Chi rend=\"tall\">π\u003C/hi>ᾶσι\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text based upon photograph.\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Kallistos and Eukarpia, who, loving one another tenderly, made this while living for themselves and\n                    their family. Peace to all.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#SA\">\n                \u003Cp>Callistos e Eukarpia, che, amandosi reciprocamente con tenerezza, in vita la costruirono per se\n                    stessi e per i propri cari. Pace a tutti.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>\u003C!--commented out pending revision-->\n                    \u003C!--damage to upper right corner. Kallistos and Eukarpia (cf. IG 14.473); living made it for themselves and their children (i.e. sibi et suis fecit); idios as poss.pron.; Agnello, after Ferrua ASS 4-5 (1938-39), 66-70, notes that the eauto kai tois idiois, i.e. sibi et suis, is a uniquely Catanian thing in Sicily.-->\n                    The letter forms (e.g. rhomboid shapes and Σ not Ϲ for sigma) suggest a date not later than the\n                    third century BC; but the final phrase is typical of Christian inscriptions, which implies that this\n                    is one of the earliest Christian texts from Catania. Kallistos is a common name, although rare in\n                    Sicily; Eukarpia is less common generally, but well attested in eastern Sicily. The paint is a\n                    modern addition and misses several of the less well-preserved letter strokes\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0540\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>178\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>4.9488\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Agnello\u003C/author>\n                        \u003Cdate>1953\u003C/date>\n                        \u003CcitedRange>no.51\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/D9BZFDDR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002211\">https://biblio.inscriptiones.org/epig10002211\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGCVO\">\n                        \u003CcitedRange>592\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/9CT5ZWIK\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000394\">https://biblio.inscriptiones.org/epig10000394\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
