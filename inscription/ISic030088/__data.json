n                    \u003Clb n=\"1\" style=\"text-direction:r-to-l\"/>\u003Cspace quantity=\"1\" unit=\"line\"/>\n                    \u003Clb n=\"2\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Cseg part=\"I\">Ἀφαιστο\u003C/seg>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"3\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Ἀρίστο̄ν\u003C/name>\u003C/persName>\n                    \u003Clb n=\"4\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Cunclear>Ξ\u003C/unclear>ένο̄\u003Cunclear>ν\u003C/unclear>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"5\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Ἐμμεν\u003Cunclear>ί\u003C/unclear>\u003Csupplied reason=\"lost\">δας\u003C/supplied>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"6\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Cunclear>Ξ\u003C/unclear>ένιος\u003C/name>\u003C/persName>\n                    \u003Clb n=\"7\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Cunclear>Σ\u003C/unclear>ύνο̄ν\u003C/name>\u003C/persName>\n                    \u003Clb n=\"8\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Εὔμνασ\u003Csupplied reason=\"lost\">τος\u003C/supplied>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"9\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Csupplied reason=\"lost\">Ἔ\u003C/supplied>\u003Cunclear>ν\u003C/unclear>ορμος\u003C/name>\u003C/persName>\n                    \u003Clb n=\"10\" style=\"text-direction:r-to-l\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Cseg part=\"M\">\u003Cunclear>ι\u003C/unclear>μο\u003C/seg>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003C/name>\u003C/persName>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Bettarini.\u003C/note>\u003C!-- ISicDef no. 44 -->\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"it\">\n                \u003Cp>La struttura della maledizione è nella forma tipica della lista di nomi posti al nom. Sono tutti antroponimi greci, nella maggior parte dei casi già ampiamente noti dalle epigrafi siceliote. (Bettarini)\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"ISelinunteDef\">\n                        \u003Cauthor>Bettarini\u003C/author>\n                        \u003Cdate>2005\u003C/date>\n                        \u003CcitedRange>2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SEW88NC7\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001388\">https://biblio.inscriptiones.org/epig10001388\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>55.1021\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
