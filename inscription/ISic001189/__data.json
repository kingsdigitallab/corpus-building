003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-05-23\" who=\"#NQ\">edited file from autopsy\u003C/change>\n                \u003Cchange when=\"2022-06-16\" who=\"#JP\">Jonathan Prag edited and revised images\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001189_text_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>Photo J. Prag, 2022-05-23, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001189_text.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>Photo J. Prag, 2022-05-23, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"screen\" url=\"ISic001189_tiled.tif\" height=\"3894px\" width=\"3410px\">\n                \u003Cdesc>Photo J. Prag, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001189.jpg\" height=\"3894px\" width=\"3410px\">\n                \u003Cdesc>Photo J. Prag, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n           \u003Cgraphic n=\"screen\" url=\"ISic001189_rightthreequarter_tiled.tif\" height=\"4416px\" width=\"3680px\">\n                \u003Cdesc>Photo J. Prag, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001189_rightthreequarter.jpg\" height=\"4416px\" width=\"3680px\">\n                \u003Cdesc>Photo J. Prag, courtesy  Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP #NQ\">\n                \u003Cab>\n                    \u003Clb n=\"1\" xml:id=\"BSοAK\"/>\u003Cspace quantity=\"4\" unit=\"character\" xml:id=\"BSοAU\"/>\u003Cw xml:id=\"BSοAe\">ὁ\u003C/w> \u003Cw xml:id=\"BSοAo\">δᾶμος\u003C/w> \u003Cspace quantity=\"3\" unit=\"character\" xml:id=\"BSοAy\"/>\n                    \u003Clb n=\"2\" xml:id=\"BSοAΙ\"/>\u003Cw xml:id=\"BSοAΤ\">\u003Cunclear xml:id=\"BSοAε\">Β\u003C/unclear>ίωτον\u003C/w> \u003Cw xml:id=\"BSοAο\">Τιμάνδρο\u003Csupplied reason=\"lost\" xml:id=\"BSοBA\">υ\u003C/supplied>\u003C/w> \n                    \u003Clb n=\"3\" xml:id=\"BSοBK\"/>\u003Cw xml:id=\"BSοBU\">εὐνοίας\u003C/w> \u003Cw xml:id=\"BSοBe\">ἕνεκε\u003Cunclear xml:id=\"BSοBo\">ν\u003C/unclear>\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based upon autopsy\u003C/note>\u003C/app>\n                    \u003Capp loc=\"2\">\u003Cnote>Salinas: [Κ]ίωτον; Kaibel: [Β]ίωτον; lapis: trace of upper eye of Β (contra Salinas, trace of K).\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#NQ\">\n                \u003Cp>Il damos (onora) Biotos figlio di Timandros per benevolenza\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>The people (honours) Biotos son of Timandros, for the sake of goodwill\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Notwithstanding the damage, it is clear that there was no ethnic included in the first part of the text, and the inscription appears complete. The name \u003Cforeign xml:lang=\"grc\">Βίωτος\u003C/foreign> is also attested on tile and amphora stamps from southern Italy; no other form ending -ιωτος is known from the region.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0366\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGMusPalermo\">\n                        \u003CcitedRange>43\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/F66S4X3M\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000531\">https://biblio.inscriptiones.org/epig10000531\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Salinas\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1880\u003C/date>\n                        \u003CcitedRange>194 no. 4\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Consani\u003C/author>\n                        \u003Cdate>1996\u003C/date>\n                        \u003CcitedRange>125\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GQG9CZGX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002071\">https://biblio.inscriptiones.org/epig10002071\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
