προαι\n                    ρουμένοις αὐτόν · τὸ δὲ δόγμα τόδε κολάψαντας ἐς χαλκώ\n                    ματα δύο, τὸ μὲν ἓν ἀναθέμειν εἰς τὸ βουλευτήριον, τὸ δὲ\n                    ἄλλο ἀποδόμειν  ὑπό\n                    μναμα τᾶς ποτὶ τὸν δᾶμον εὐνοίας · τοὺς δὲ \n                    ἐξοδιάξαι ἐς τὰ προγεγραμμένα ὅσον κα χρεία ἦ καὶ φέ\n                    ρειν τᾶν ἔξοδον διὰ τῶν ἀπολόγων.\n                    Ὁμογνώμονες τοῦ συνεδρίου πάντες.\n                ",[379,381,382,384,386,388,390,392,394,396,398,401,403,405,407,409,411,413,415,417,419,421,423,425,427,429],{"n":380},"1",{"n":136},{"n":383},"3",{"n":385},"4",{"n":387},"5",{"n":389},"6",{"n":391},"7",{"n":393},"8",{"n":395},"9",{"n":397},"10",{"n":399,"break":400},"11","no",{"n":402},"13",{"n":404},"14",{"n":406},"15",{"n":408},"16",{"n":410,"break":400},"17",{"n":412},"18",{"n":414},"19",{"n":416,"break":400},"21",{"n":418,"break":400},"22",{"n":420,"break":400},"23",{"n":422},"24",{"n":424,"break":400},"25",{"n":426},"26",{"n":428,"break":400},"27",{"n":430},"28",[432,436,442,445,448,452,455],{"_":433,"type":434,"subtype":435},"ἱεροθύτᾶ","civic.religious","hierothutes",{"type":437,"subtype":438,"expan":439},"civic","paraprostates",{"abbr":440,"ex":441},"παραπροστά","τα",{"_":443,"type":437,"subtype":444},"προαγοροῦντος","proagoros",{"_":446,"type":437,"subtype":447},"γραμματεύοντος","grammateus",{"_":449,"type":450,"subtype":451},"προξενίας","paracivic","proxenos",{"_":453,"type":437,"subtype":454},"πρέσβεες","presbys",{"_":456,"type":437,"subtype":457},"ταμίας","tamias",[459,467,471,476,484,496,505,511,518],{"_":460,"type":461,"name":462,"persName":463}," τοῦ ","attested","Νυμφοδώρου",{"type":461,"name":464},{"_":465,"type":466},"Φίλωνος","patronym",{"_":460,"type":461,"name":468,"persName":469},"Διοκλέος",{"type":461,"name":470},{"_":468,"type":466},{"type":461,"name":472,"persName":473},"Ἀδρανίωνος",{"type":461,"name":474},{"_":475,"type":466},"Ἀλεξάνδρου",{"type":461,"name":477,"persName":4