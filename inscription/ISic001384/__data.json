         \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001384_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001384.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #AA\">\n                \u003Cab>\n                    \u003Clb n=\"0\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\">\u003Cdesc>name of deceased\u003C/desc>\u003C/gap>\n                    \u003Clb n=\"1\"/>\u003Cw>ἐτελεύτη\u003Csupplied reason=\"lost\">σε\u003C/supplied>\u003C/w>\n                    \u003Clb n=\"2\"/>\u003Cw>τῇ\u003C/w> \u003Cw>\u003Cexpan>\u003Cabbr>πρ\u003C/abbr>\u003Cex>ὸ\u003C/ex>\u003C/expan>\u003C/w> \u003Cnum value=\"19\">ιθ\u003C/num> \u003Cw>\u003Cexpan>\u003Cabbr>Καλα\u003Clb n=\"3\" break=\"no\"/>νδ\u003C/abbr>\u003Cex>ῶν\u003C/ex>\u003C/expan>\u003C/w> \u003Cw>Ἰανουαρί\u003Clb n=\"4\" break=\"no\"/>ων\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 1\">\u003Cnote>Kirchhoff, Kaibel: ἐτελεύτη[σε]ν (Kaibel: vel ἐτελεύτ[α]); Korhonen: ἐτελεύτη[σε]\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>... died 19 days before the Kalends of January.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>... morì 19 giorni prima delle Calende di gennaio.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#martaf\">\n                \u003Cp>This epitaph, already recognised as Christian by Kirchhoff, has a formula fairly attested in Catania without the expression \u003Cforeign xml:lang=\"grc\">ἐνθάδε κεῖται\u003C/foreign> and with only the name of the deceased followed by the date of death (expressed with the aorist of \u003Cforeign xml:lang=\"grc\">τελευτάω\u003C/foreign>) and occasionally the age (but not in this case): e.g. IG 14.551 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001370\">ISic001370\u003C/ref>.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Amico\u003C/author>\n                        \u003Cdate>1741\u003C/date>\n                        \u003CcitedRange>276\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F5INCW3T\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002186\">https://biblio.inscriptiones.org/epig10002186\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>263 no.10\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SPTB56H3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1784\u003C/date>\n                        \u003CcitedRange>281 no.10\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7PSFHSUH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002083\">https://biblio.inscriptiones.org/epig10002083\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrara\u003C/author>\n                        \u003Cdate>1829\u003C/date>\n                        \u003CcitedRange>392 no.8\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/82T4IWDN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002005\">https://biblio.inscriptiones.org/epig10002005\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0565\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>215\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>4.9511\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
