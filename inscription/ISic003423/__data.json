l href=\"https://epidoc.stoa.org/schema/latest/tei-epidoc.rng\" type=\"application/xml\" schematypens=\"http://relaxng.org/ns/structure/1.0\"?>\n\u003C?xml-model href=\"../schematron/ircyr-checking.sch\" schematypens=\"http://purl.oclc.org/dsdl/schematron\"?>\n\u003CTEI xmlns=\"http://www.tei-c.org/ns/1.0\" xmlns:xi=\"http://www.w3.org/2001/XInclude\" xml:lang=\"en\">\n    \u003CteiHeader>\n        \u003CfileDesc>\n            \u003CtitleStmt>\n                \u003Ctitle>Building inscription on a re-used louterion basin\u003C/title>\n                \u003Ceditor ref=\"#JP\">Jonathan Prag\u003C/editor>\n                \u003Cprincipal ref=\"#JP\">Jonathan Prag\u003C/principal>\n                \u003Cfunder>John Fell OUP Research Fund\u003C/funder>\n\t   \u003Cfunder>\u003Cref target=\"https://cordis.europa.eu/project/id/885040\">ERC Advanced Grant no.885040\u003C/ref>\u003C/funder>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JP\" ref=\"http://orcid.org/0000-0003-3819-8537\">Jonathan Prag\u003C/name>\n                    \u003Cresp>original data collection and editing\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JCu\" ref=\"http://orcid.org/0000-0002-6686-3728\">James Cummings\u003C/name>\n                    \u003Cresp>conversion to EpiDoc\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JCh\" ref=\"http://orcid.org/0000-0001-6823-0265\">James Chartrand\u003C/name>\n                    \u003Cresp>site construction and encoding\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"VV\" ref=\"http://orcid.org/0000-0002-9695-0240\">Valeria Vitale\u003C/name>\n                    \u003Cresp>editing of geo data\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"MM\">Michael Metcalfe\u003C/name>\n                    \u003Cresp>museum data collection\u003C/resp>\n                \u003C/respStmt>\n\t    \u003CrespStmt>\n     \t       \u003Cname xml:id=\"SS\" ref=\"https://orcid.org/0000-0003-3914-9569\">Simona Stoyanova\u003C/name>\n     \t       \u003Cresp>standardisation of template and tidying up encoding\u003C/resp>\n \t   \u003C/respStmt>\n\t    \u003CrespStmt>\n                    \u003Cname xml:id=\"system\">system\u003C/name>\n                    \u003Cresp>automated or batch processes\u003C/resp>\n                \u003C/respStmt>\n            \u003C/titleStmt>\n            \u003CpublicationStmt>\n                \u003Cauthority>I.Sicily\u003C/authority>\n                \u003Cidno type=\"filename\">ISic003423\u003C/idno>\n                \u003Cidno type=\"TM\">644818\u003C/idno>\n                \u003Cidno type=\"EDR\"/>\n                \u003Cidno type=\"EDH\"/>\n                \u003Cidno type=\"EDCS\"/>\n                \u003Cidno type=\"PHI\"/>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic003423\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-18\">10.5281/zenodo.4358498\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative\n                        Commons-Attribution 4.0 licence.\n                    \u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Licata\u003C/settlement>\n                        \u003Crepository role=\"museum\" ref=\"http://sicily.classics.ox.ac.uk/museum/134\">Museo Archeologico della Badia\u003C/repository>\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble (white)\u003C/material>\n                                    \u003CobjectType ana=\"#object.vessel.leuterion\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/144.html\">louterion\u003C/objectType>\n                                    \u003Cp>Multiple marble fragments (of which three join to make one larger fragment, and a\n                                        fourth is approximately contiguous to the main fragment), which appear to form\n                                        part of the circular basin of a 'louterion'. The remains of a square projection\n                                        (12 x 12 cm, projecting a further 6 cm from the base) are partly preserved on\n                                        the reverse of the main fragment, by which the basin was fixed onto the stand.\u003C/p>\n                                    \u003Cdimensions n=\"1\">\n                                        \u003Cheight unit=\"cm\">38\u003C/height>\n                                        \u003Cwidth unit=\"cm\">47\u003C/width>\n                                        \u003Cdepth unit=\"cm\">3.25-4.8\u003C/depth>\n                                    \u003C/dimensions>\n                                    \u003Cdimensions n=\"2\">\n                                        \u003Cheight unit=\"cm\">19\u003C/height>\n                                        \u003Cwidth unit=\"cm\">18.5\u003C/width>\n                                        \u003Cdepth unit=\"cm\">3.1-3.55\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                    \u003Cp>The text is inscribed on the concave inner surface of the basin, over seven\n                                        lines. The visible vacats suggest that although the text is fragmentary, it only\n                                        covered seven lines originally. The disposition of the text within the basin\n                                        implies that the basin, in whole or in part, was re-used for the purposes of the\n                                        inscription. The text curves slightly, due to the concave surface and form of\n                                        the basin. The text preserves a clear left margin in lines 3-7. Both letter\n                                        height and interlineation are uneven.\u003C/p>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Cp>The letters are broad, square and regular in form. Alpha is very broad; sigma has\n                                    parallel top and bottom bars; delta, theta and omicron are smaller than the other\n                                    letters and mid-line; epsilon has a shorter middle bar; pi has a slightly\n                                    overlapping cross-bar; the mid-point of the mu nearly reaches the lower line; theta\n                                    is fully closed by the middle bar.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line7\">Lines 1-7\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">21-32\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line7\">Interlineation lines 1 to 7\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">1.7-3.2\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462424\">Phintias\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2524393\">Licata\u003C/placeName>\n                            \t\u003Cgeo>37.10267, 13.93972\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0250\" notAfter-custom=\"-0150\" precision=\"low\">later 3rd century BCE or early 2nd century BCE (letter forms)\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" from=\"2003\" to=\"2005\">Monte Sant'Angelo di Licata, Excavation 2003-2005, sectors 1000 and 2000\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">On display\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#MM\">2016.09.28 (Metcalfe)\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage id