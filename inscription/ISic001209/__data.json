  \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2020-03-08\" who=\"#JP\">Jonathan Prag revised the EpiDoc\u003C/change>\n                \u003Cchange when=\"2020-06-09\" who=\"#JP\">Jonathan Prag made minor revisions and added image\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic120_tiled.tif\" height=\"2034px\" width=\"1459px\">\n                \u003Cdesc>Scan from IGMusPal, courtesy Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001209.jpg\" height=\"2034px\" width=\"1459px\">\n                \u003Cdesc>Scan from IGMusPal, courtesy Museo Archeologico Regionale Antonino Salinas\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>Φιλιστοῦς\n                \u003C/ab>\n               \u003C/div>\n             \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text of IG\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>(Tomb) of Philisto\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The name \u003Cref target=\"http://clas-lgpn2.classics.ox.ac.uk/cgi-bin/lgpn_search.cgi?name=%CE%A6%CE%B9%CE%BB%CE%B9%CF%83%CF%84%CF%8E\">Φιλιστώ\u003C/ref> is not common; it is also attested in Sicily in the theatre seat inscriptions of \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001262\">ISic001262\u003C/ref> (Taormina). Although dated on the grounds of letter forms to the C2-1 BC by Manni Piraino, the evidence from the necropolis suggests the range C3-2 BC is more plausible.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0382d\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGMusPalermo\">\n                        \u003CcitedRange>123\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/F66S4X3M\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000531\">https://biblio.inscriptiones.org/epig10000531\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Salinas\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1886\u003C/date>\n                        \u003CcitedRange>464 no.b\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"SGDI\">\n                        \u003CcitedRange>5208d\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/JB4GMPBH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000817\">https://biblio.inscriptiones.org/epig10000817\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Sofia\u003C/author>\n                        \u003Cdate>2015\u003C/date>\n                        \u003CcitedRange>107 and fig.110\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/9QTE4E3Z\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001575\">https://biblio.inscriptiones.org/epig10001575\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
