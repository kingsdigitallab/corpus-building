>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462410\">Panhormus\u003C/placeName>\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523920/\">Palermo\u003C/placeName>\n                                \u003Cgeo>38.13205, 13.33561\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0525\" notAfter-custom=\"-0500\" evidence=\"lettering archaeological-context\">525—500 BCE (so Arena on letters).\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found amongst terracotta fragments in the Virzi collection, and assumed to come from the area of Palermo\u003Cgeo>38.13205, 13.3356\u003C/geo>.\u003C/provenance> \u003C!-- Generic Palermo coordinates-->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">No Autopsy\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n            \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>             \n            \u003Cxi:include href=\"../alists/charDecl.xml\">                 \n                \u003Cxi:fallback>                     \n                    \u003Cp>ISicily glyphs authority list\u003C/p>                 \n                \u003C/xi:fallback>             \n            \u003C/xi:include>         \n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.defixio\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/76.html\">defixio\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-03-24\" who=\"#VM\">Valentina Mignosa created new file and added metadata\u003C/change>\n                \u003Cchange when=\"2021-06-28\" who=\"#SN\">Savannah Neibart encoded inscription using data from ISicDef, supplemented by Manganaro and other editions\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic030025_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic030025.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#SN\">\n                    \u003Cmilestone unit=\"face\" n=\"a\"/>\n                    \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"a\">\n                        \u003Cab>\n                            \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Σίμε̄\u003C/name>\u003C/persName> \u003Cg ref=\"#dipunct\">:\u003C/g> \u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Πρατομακες\u003C/name>\u003C/persName> \u003Cg ref=\"#dipunct\">:\u003C/g>\n                            \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Λεπτίνας\u003C/name>\u003C/persName> \u003Cg ref=\"#dipunct\">:\u003C/g> \u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Πραξίας\u003C/name>\u003C/persName>                     \n                            \u003Clb n=\"3\"/>\u003Corig>Ρ\u003Cunclear>Λ\u003C/unclear>Τ\u003C/orig>\u003Cgap reason=\"illegible\" quantity=\"3\" unit=\"character\"/>\u003Corig>ΟΚΥΚΥΙΕΣ\u003C/orig> \u003Cg ref=\"#dipunct\">:\u003C/g>\n                         \u003C/ab>\n                      \u003C/div>\n                     \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"b\">\n                        \u003Cab>\n                          \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Αραοτε̄ς\u003C/name>\u003C/persName> \u003Cg ref=\"#dipunct\">:\u003C/g> πυριν \u003Cg ref=\"#dipunct\">:\u003C/g> πεσ\u003Cgap reason=\"illegible\" unit=\"character\" quantity=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                           \u003Clb n=\"2\"/>\u003Cgap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>τιλλαμ \u003Cg ref=\"#dipunct\">:\u003C/g> πέδαν \u003Cg ref=\"#dipunct\">:\u003C/g> \u003Cgap reason=\"illegible\" unit=\"character\" quantity=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                            \u003Clb n=\"3\"/>τε πεσα\n                            \u003Clb n=\"4\"/>\u003Corig>ΡE\u003C/orig>\n                        \u003C/ab>\n                    \u003C/div>\n                \n                \u003Cmilestone unit=\"face\" n=\"b\"/>\n                \u003Cdiv type=\"textpart\" subtype=\"section\" n=\"c\">\n                    \u003Cab>\n                        \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>ν \u003Corig>ΠΥΤ\u003Cunclear>Λ\u003C/unclear>Ι\u003C/orig> τοτε πε\u003Cgap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>ει\n                        \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>Ε\u003Cunclear>Λ\u003C/unclear>ΤΥΙ\u003C/orig>\u003Cgap reason=\"illegible\" quantity=\"4\" unit=\"character\"/>\u003Corig>Ν\u003C/orig>\u003Cgap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\u003Corig>ΛΜ\u003C/orig>\u003Cgap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\u003Corig>Υ\u003C/orig>\n                        \u003Clb n=\"3\"/>\u003Cspace quantity=\"1\" unit=\"line\"/>\n                        \u003Clb n=\"4\"/>\u003Cspace quantity=\"1\" unit=\"line\"/>\n                        \u003Clb n=\"5\"/>\u003Corig>ΡΕΔΕΙΤΙ\u003C/orig>\u003Cgap reason=\"illegible\" quantity=\"3\" unit=\"character\"/>\u003Corig>ΙΡΙ\u003C/orig>\n                    \u003C/ab>\n                \u003C/div>\n                \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Manganaro\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp xml:lang=\"fr\">Dans la suite, Manganaro, d'une façon ingénieuse, a voulu retrouver des substantifs désignant des gênes physiques qui serait souhaitées par le rédacteur de la tablette à la personne maudite que doit être ici Adrasté. Sur la face b on ne lit clairement que έ]ν Πύται qui doit être une indication topographique.(Dubois)\u003C/p>\n                    \u003Cp xml:lang=\"es\">El caràcter de malediciòn parece claro por los nombres proprios y la serie de males y enfermedades mencionados en términos dialectales, interpretables sòlo por la vìa conjectural: πέσον (pesario), πέσα, τίλλαμ (còlico), πεσετν... La mezcla de nombres griegos e indìgenas en esta defixio, asì como otros documentos epigràficos de Montagna de Marzo y Terravecchia di Cuti indican una mezcla de poblaciòn. Segùn Manganaro, grupos de habitantes dorios, venidos a Sicilia probablemente del Peloponeso, se superponen a los elementos locales, esposando mujeres indìgenas. Asì se explica adecuadamente en la onomàstica de estos centro la persistencia de formas no helènicas, como Ρατορώ, Κυκυώ para las hembras, y Αραότεσ, Ρατορᾶσ, Πρατομακέσ para los varones. (Lopez-Jimeno)\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1977\u003C/date>\n                        \u003CcitedRange>1335-1338, tav. 57-58\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/isicily/items/F5HM44RP/\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002692\">https://biblio.inscriptiones.org/epig10002692\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGASM2[2]\">\n                        \u003Cauthor>Arena\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>118\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/65P7AM6N\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001373\">https://biblio.inscriptiones.org/epig10001373\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGDS1\">\n                        \u003Cauthor>Dubois\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>\u003Cref target=\"http://www.persee.fr/doc/efr_0000-0000_1989_cat_119_1\">no. 176\u003C/ref>\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RWXNUFEX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000187\">https://biblio.inscriptiones.org/epig10000187\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Brugnone\u003C/author>\n                        \u003Cdate>1980-1981\u003C/date>\n                        \u003CcitedRange>443\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/86H6PMPS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001462\">https://biblio.inscriptiones.org/epig10001462\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>27.0656\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl>\n                        \u003Cauthor>Jordan\u003C/author>\n                        \u003Cdate>1985\u003C/date>\n                        \u003CcitedRange>no. 111\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7UQBFBPB\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001391\">https://biblio.inscriptiones.org/epig10001391\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>López-Jimeno\u003C/author>\n                        \u003Cdate>1991\u003C/date>\n                        \u003CcitedRange>no. 5\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/N5IF2DZR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001858\">https://biblio.inscriptiones.org/epig10001858\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Curbera\u003C/author>\n                        \u003Cdate>1999\u003C/date>\n                        \u003CcitedRange>no. 50\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/g