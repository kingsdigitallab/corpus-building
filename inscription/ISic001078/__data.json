3Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.decree\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/384.html\">decree\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2023-06-12\" who=\"#JP\">Jonathan Prag checked and revised edition from publications\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface/>\n        \u003C!--\u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001078_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001078.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>-->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>ἐπὶ \u003CroleName type=\"civic.religious\" subtype=\"hierapolos\">ἱεραπόλου\u003C/roleName> \u003CpersName>\u003Cname>\u003Cseg part=\"I\">Τελίνε\u003C/seg>\u003C/name>\u003C/persName>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003Csupplied reason=\"lost\">τ\u003C/supplied>ο\u003Csupplied reason=\"lost\">ῦ\u003C/supplied> \u003Corig>Α\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\u003Corig>Γ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text of IG\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The name Τηλίνης is attested at Gela in Herodotos 7.153. It seems unlikely that this inscription is genuine: the similar text preserved as ISic001079 = IG XIV.259 is almost certainly a late 18th century fake, and this one appears to have similar context (see principally the discussion of  Pareti); the reproduction of the name from Herodotus adds to the doubt. (Note that Carità mis-references this as IG XIV.259.)\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0258\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Schubring\u003C/author>\n                        \u003Cdate>1873\u003C/date>\n                        \u003CcitedRange>75\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/5NHGE6V5\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002684\">https://biblio.inscriptiones.org/epig10002684\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Pareti\u003C/author>\n                        \u003Cdate>1914\u003C/date>\n                        \u003CcitedRange>214-220\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/3H8T7K4U\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002680\">https://biblio.inscriptiones.org/epig10002680\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>1959.0288\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001283\">https://biblio.inscriptiones.org/epig10001283\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Carità\u003C/author>\n                        \u003Cdate>1978\u003C/date>\n                        \u003CcitedRange>21-22\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2RZUVGV7\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002089\">https://biblio.inscriptiones.org/epig10002089\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
