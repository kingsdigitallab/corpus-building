3C!--No inventory number found-->\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"la\">Latin\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>A marble plaque, broken off above and lower left, but with parts of left and right margin and lower right corner preserved. Text is framed within a double border.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble\u003C/material>\n                                    \u003CobjectType ana=\"#object.plaque\" ref=\"https://www.eagle-network.eu/voc/objtyp/lod/259.html\">plaque\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003C!--manganaro-->\n                                        \u003Cheight unit=\"cm\">11-15\u003C/height>\n                                        \u003Cwidth unit=\"cm\">22.5\u003C/width>\n                                        \u003Cdepth unit=\"cm\">3\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragment\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>Text is framed by a double border, with well spaced letters filling the field.\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage ana=\"#text_condition.incomplete\"/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Well-spaced letters with a broad module, large, rounded C, open P, well-formed serifs.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line5\">Line 1-5\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">20\u003C/height>\u003C!-- Manganaro -->\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462372\">Morgantina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2524052\">Morgantina\u003C/placeName>\n                            \t\u003Cgeo>37.430596, 14.479485\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0001\" notAfter-custom=\"0100\" evidence=\"lettering textual-context\">Manganaro suggests C1 BCE (on the basis of lettering?), but concedes it could be imperial, and a first century CE date seems more likely.\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1989\">Described by Manganaro simply as found at Morgantina, in the property of a citizen of Aidone; apparently gifted to Manganaro, and not seen since.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition>private collection\u003C/acquisition>\n                    \u003C/history>\n                    \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-11-10\" who=\"#JP\">Jonathan Prag made minor edits, added bibliography, uncoded text\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-17\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2025-05-30\" who=\"#JP\">Jonathan Prag revised from publication\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000761_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000761.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#JP\" source=\"https://www.zotero.org/groups/382445/items/EZMBJ6GN\">\n                \u003Cab>\n                    \u003Clb n=\"0\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cw part=\"I\">Al\u003Cunclear>b\u003C/unclear>\u003C/w>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003C/name>\n                    \u003Clb n=\"2\"/>\u003Cname>Pompe\u003Cunclear>i\u003C/unclear>\u003Csupplied reason=\"lost\">o\u003C/supplied>\u003C/name>\n                    \u003Clb n=\"3\"/>\u003Cname>Marcello\u003C/name>\u003C/persName>\n                    \u003Clb n=\"4\"/>patrono suo\n                    \u003Clb n=\"5\"/>\u003Csupplied reason=\"lost\">\u003Cexpan>\u003Cabbr>opt\u003C/abbr>\u003Cex>imo\u003C/ex>\u003C/expan>\u003C/supplied> bene \u003Cexpan>\u003Cabbr>m\u003C/abbr>\u003Cex>erenti\u003C/ex>\u003C/expan>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003Cp>Text of Manganaro 1989\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>[-----] to Alb[---] Pompeius Marcellus, his patron, an excellent man, well deserving.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Manganaro speculatively restores the name as [-]Cornelius Albinianus Pompeius Marcellus, but this seems pure speculation, as does the claim that the missing dedicant in the first part of the text was 'certamente un Cornelius(?) Pompeius'.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>1989.0344d\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001283\">https://biblio.inscriptiones.org/epig10001283\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1988\u003C/date>\n                        \u003CcitedRange>6 n.8\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RZSFKACR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001427\">https://biblio.inscriptiones.org/epig10001427\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1989\u003C/date>\n                        \u003CcitedRange>187 no.74 fig.79\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/EZMBJ6GN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001831\">https://biblio.inscriptiones.org/epig10001831\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
