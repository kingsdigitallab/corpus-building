spiral-clockwise\">\u003Cspan class=\"linenumber\">1\u003C/span>ΓΑΣ\u003Cspan data-type=\"persname\" data-name-type=\"attested\" data-text=\"Μύσχελυς\">\u003Cspan data-type=\"name\" data-name-type=\"\" data-text=\"Μύσχελυς\">ΜΥΣΧΕΛΥΣ\u003C/span>\u003C/span>\u003C/span>\u003C/span>\u003C/div>\u003Cp class=\"miniapp\">\u003C/p>",[233,235],{"image":89,"thumb":234,"title":92},"ISic020581_tiled-thumb.tif",{"image":236,"thumb":237,"title":92},"ISic020581.jpg","ISic020581-thumb.jpg","\u003C?xml version='1.0' encoding='UTF-8'?>\n\u003C?xml-model href=\"https://epidoc.stoa.org/schema/latest/tei-epidoc.rng\" type=\"application/xml\" schematypens=\"http://relaxng.org/ns/structure/1.0\"?>\n\u003C?xml-model href=\"../schematron/ircyr-checking.sch\" schematypens=\"http://purl.oclc.org/dsdl/schematron\"?>\n\u003CTEI xmlns=\"http://www.tei-c.org/ns/1.0\" xmlns:xi=\"http://www.w3.org/2001/XInclude\" xml:lang=\"en\">\n    \u003CteiHeader>\n        \u003CfileDesc>\n            \u003CtitleStmt>\n                \u003Ctitle>ISic020581\u003C/title>\n                \u003Ceditor ref=\"#JP\">Jonathan Prag\u003C/editor>\n                \u003Cprincipal ref=\"#VM\">Valentina Mignosa\u003C/principal>\n                \u003Cfunder>John Fell OUP Research Fund\u003C/funder>\n                \u003Cfunder>\u003Cref target=\"https://cordis.europa.eu/project/id/885040\">ERC Advanced Grant no.885040\u003C/ref>\u003C/funder>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"JP\" ref=\"https://orcid.org/0000-0003-3819-8537\">Jonathan Prag\u003C/name>\n                    \u003Cresp>project direction, template development and general editor\u003C/resp>\n                \u003C/respStmt>\n                \u003CrespStmt>\n                    \u003Cname xml:id=\"VM\" ref=\"https://orcid.org/0000-0002-7122-2511\">Valentina Mignosa\u003C/name>\n                    \u003Cresp>original data collection and editing, encoding text in Epidoc\u003C/resp>\n                \u003C/respStmt>\n   