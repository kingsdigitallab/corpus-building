d EDCS numbers","2020-11-20",{"_":406,"when":407,"who":398},"Simona Stoyanova restructured bibliography","2020-11-26",{"_":409,"when":269,"who":410},"Updated Zenodo DOI","#system",{"_":412,"when":413,"who":398},"renumbered files, uris and references","2021-01-19",{"_":415,"when":416,"who":200},"Jonathan Prag added pilot bilingualism annotation and images","2023-09-24",{"surface":418},[419,433],{"type":420,"graphic":421},"front",[422,426,429,431],{"n":423,"url":93,"height":424,"width":425,"desc":94},"screen","2856px","4272px",{"n":427,"url":428,"height":424,"width":425,"desc":94},"print","ISic000348.jpg",{"n":423,"url":430,"height":424,"width":425,"desc":94},"ISic000348_copy2_tiled.tif",{"n":427,"url":432,"height":424,"width":425,"desc":94},"ISic000348_copy2.jpg",{"type":434,"graphic":435},"rear",[436,438],{"n":423,"url":437,"height":424,"width":425,"desc":94},"ISic000348_rear_tiled.tif",{"n":427,"url":439,"height":424,"width":425,"desc":94},"ISic000348_rear.jpg",{"body":441},{"div":442},[443,486,491,494,497,500],{"type":120,"xml:space":444,"resp":200,"div":445},"preserve",[446,469],{"type":447,"subtype":448,"n":449,"xml:lang":113,"ab":450},"textpart","section","a",{"_":451,"lb":452,"persName":457,"g":465,"unclear":468},"\n                      pei\n                    salve\n                ",[453,455],{"n":454},"1",{"n":456},"2",{"type":458,"name":459},"attested",[460,464],{"expan":461},{"abbr":462,"ex":463},"Q","uinte","Domitei",{"_":466,"ref":467},"·","#interpunct","e",{"type":447,"subtype":448,"n":470,"xml:lang":114,"ab":471},"b",{"_":472,"lb":473,"persName":478,"g":483,"space":484},"\n                    \n                    εὐσεβῆ  χαῖρε\n                ",[474,476],{"n":475},"3",{"n":477},"4",{"type":458,"name":479,"g":482},[480,481],"Κόϊντε","Δομίτιε",{"_":466,"ref":467},{"_":466,"ref":467},{"quantity":454,"unit":485},"character",{"type":487,"listApp":488},"apparatus",{"app":489},{"note":490},"Text from autopsy",{"type":492,"xml:lang":194,"resp":200,"p":493},"translation","Quintus Domitius, pious, farewell!\n                    Quintus Domitius, pious, farewell!",{"type":492,"xml:lang":351,"resp":495,"p":496},"#SA","Quinto Domizio, pio, addio!\n                    Quinto Domizio, pio, addio!",{"type":498,"xml:lang":194,"p":499},"commentary","\n                    \n                    The text shows one of the typical differences between Greek and Latin practice: the name Quintus is\n                    abbreviated in the Latin version, but written in full in Greek. pius / eusebes could be a third name\n                    (cognomen) or just an epithet: the fact that it is translated rather than transliterated, and the\n                    fact that pie salve / eusebe chaire is an attested formula in funerary inscriptions (in Latin,\n                    specifically in eastern Sicily) suggests it is an epithet. The forms Domitei and peie (for Domiti\n                    and pie) suggest the text was written by someone who was primarily a Greek speaker, since it is a\n                    Greek tendency to render long Latin i with ei.",{"type":501,"listBibl":502},"bibliography",[503,524],{"type":120,"bibl":504},[505,508,513,518,521],{"type":123,"n":124,"citedRange":125,"ptr":506,"ref":507},{"target":127},{"_":129,"target":129},{"type":123,"n":133,"citedRange":509,"ptr":511,"ref":512},{"ref":510},{"_":136,"target":137},{"target":139},{"_":141,"target":141},{"type":123,"n":145,"citedRange":514,"ptr":516,"ref":517},{"ref":515},{"_":148,"target":149},{"target":151},{"_":153,"target":153},{"author":157,"date":158,"citedRange":159,"ptr":519,"ref":520},{"target":161},{"_":163,"target":163},{"type":123,"n":167,"citedRange":168,"ptr":522,"ref":523},{"target":170},{"_":172,"target":172},{"type":176,"bibl":33},[526,527,528],{"n":423,"url":93,"height":424,"width":425,"desc":94,"surfaceType":420},{"n":423,"url":430,"height":424,"width":425,"desc":94,"surfaceType":420},{"n":423,"url":437,"height":424,"width":425,"desc":94,"surfaceTyp