      \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n                \u003Cchange when=\"2020-12-17\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n                \u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2025-05-23\" who=\"#JP\">Jonathan Prag made minor revisions and added bibl\u003C/change>\n                \u003Cchange when=\"2025-07-24\" who=\"#VF\">Victoria Fendel's lemmatizations were added\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000323_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000323.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#EK\" source=\"https://www.zotero.org/groups/382445/items/GQN8UZSI\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cw n=\"5\">\u003Cexpan>\u003Cabbr>D\u003C/abbr>\u003Cex>is\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"10\">❦\u003C/g> \u003Cw n=\"15\">\u003Cexpan>\u003Cabbr>m\u003C/abbr>\u003Cex>anibus\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"20\">❦\u003C/g> \u003Cw n=\"25\">\u003Cexpan>\u003Cabbr>S\u003C/abbr>\u003Cex>acrum\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"30\">❦\u003C/g>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cw n=\"35\">\u003Cexpan>\u003Cabbr>L\u003C/abbr>\u003Cex>ucius\u003C/ex>\u003C/expan>\u003C/w>\u003C/name> \u003Cg ref=\"#ivy-leaf\" n=\"40\">❦\u003C/g> \u003Cname>\u003Cw n=\"45\">Arrius\u003C/w>\u003C/name>\n                    \u003Clb n=\"3\"/>\u003Cname>\u003Cw n=\"50\">Secundus\u003C/w>\u003C/name>\u003C/persName>\n                    \u003Clb n=\"4\"/>\u003Cw n=\"55\">\u003Cexpan>\u003Cabbr>vix\u003C/abbr>\u003Cex>it\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"60\">❦\u003C/g> \u003Cw n=\"65\">\u003Cexpan>\u003Cabbr>an\u003C/abbr>\u003Cex>nos\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"70\">❦\u003C/g> \u003Cnum value=\"17\">\u003Cw n=\"75\">XVII\u003C/w>\u003C/num>\n                    \u003Clb n=\"5\"/>\u003Cw n=\"80\">\u003Cchoice>\u003Creg>marmorarii\u003C/reg>\u003Corig n=\"85\">marmorari\u003C/orig>\u003C/choice>\u003C/w>\n                    \u003Clb n=\"6\"/>\u003Cw n=\"90\">\u003Cexpan>\u003Cabbr>convive\u003C/abbr>\u003Cex>ntes\u003C/ex>\u003C/expan>\u003C/w> \u003Cg ref=\"#ivy-leaf\" n=\"95\">❦\u003C/g> \u003Cw n=\"100\">\u003Cexpan>\u003Cabbr>fecer\u003C/abbr>\u003Cex>unt\u003C/ex>\u003C/expan>\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"edition\" subtype=\"simple-lemmatized\" resp=\"#VF\">\n                \u003Cab>\n                    \u003Cw n=\"5\" lemma=\"dis\">Dis\u003C/w>\n                    \u003Cw n=\"15\" lemma=\"manes\">manibus\u003C/w>\n                    \u003Cw n=\"25\" lemma=\"sacrum\">Sacrum\u003C/w>\n                    \u003Cw n=\"35\" lemma=\"Lucius\">Lucius\u003C/w>\n                    \u003Cw n=\"45\" lemma=\"Arrius\">Arrius\u003C/w>\n                    \u003Cw n=\"50\" lemma=\"Secundus\">Secundus\u003C/w>\n                    \u003Cw n=\"55\" lemma=\"vivo\">vixit\u003C/w>\n                    \u003Cw n=\"65\" lemma=\"annus\">annos\u003C/w>\n                    \u003Cw n=\"75\" lemma=\"XVII\">XVII\u003C/w>\n                    \u003Cw n=\"80\" lemma=\"marmorarius\">marmorarii\u003C/w>\n                    \u003Corig n=\"85\"/>\n                    \u003Cw n=\"90\" lemma=\"convivo\">conviventes\u003C/w>\n                    \u003Cw n=\"100\" lemma=\"facio\">fecerunt\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text of Mommsen in CIL\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#EK\">\n                \u003Cp>Sacred to the shades of the underworld. Lucius Arrius Secundus lived for 17 years. The marbleworkers living with him made this.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"de\" resp=\"#EK\">\n                \u003Cp>Den Seelen der Unterwelt gewidmet. Lucius Arrius Secundus lebte für 17 Jahre. Die Steinmacher, die mit ihm leben, haben das gemacht.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>As Korhonen notes, convive could also be equivalent to convivae.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"CIL\">\n                        \u003Cauthor>Mommsen\u003C/author>\n                        \u003Cdate>1883\u003C/date>\n                        \u003CcitedRange>\n                            \u003Cref target=\"http://arachne.uni-koeln.de/item/buchseite/650758\">7039\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GQN8UZSI\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10000589\">https://biblio.inscriptiones.org/epig10000589\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Korhonen\u003C/author>\n                        \u003Cdate>2004\u003C/date>\n                        \u003CcitedRange>96, L4.xxv\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\n                    \u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>"],"uses":{"params":["slug"]}}]}
