      \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.votive\" cert=\"low\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/83\">votive\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-04-20\" who=\"#VM\">Valentina Mignosa added metadata, text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020496_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020496.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" quantity=\"3\" unit=\"character\"/>\u003Corig>Ο\u003C/orig> \u003CpersName type=\"attested\">\u003Cname>\u003Csupplied reason=\"lost\">Ἰ\u003C/supplied>λάρχ\u003Csupplied reason=\"lost\">ο̄\u003C/supplied>\u003C/name>\u003C/persName> ἐφ\u003Cgap reason=\"lost\" quantity=\"3\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Arena (IGASM 2)\u003C/note>\n                    \u003C/app>\n                    \u003Capp>\n                        \u003Cnote>Arena suggests integrating ἐφ[...] as ἔθε̄κε\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#VM\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp>Lejeune (1980, 1315) interpreted this inscription as Sikan. The name ῎Ιλαρχος\n                    suggested by the integration of Arena is also attested in Sparta (LGPN V3A: 10206)\n                    in c. 420 BC and in Chalkis (LGPN V1: 73992) in the Hellenistic period.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IGASM2[2]\">\n                        \u003Cauthor>Arena\u003C/author>\n                        \u003Cdate>2002\u003C/date>\n                        \u003CcitedRange>50, pl. XVIII.2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/65P7AM6N\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001373\">https://biblio.inscriptiones.org/epig10001373\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Lejeune\u003C/author>\n                        \u003Cdate>1980\u003C/date>\n                        \u003CcitedRange>1315\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/Z9C6U4Z5\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002499\">https://biblio.inscriptiones.org/epig10002499\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manni Piraino\u003C/author>\n                        \u003Cdate>1980\u003C/date>\n                        \u003CcitedRange>1778-1779, no. 5, pl. 1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/IFH7SBMP\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001765\">https://biblio.inscriptiones.org/epig10001765\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>29.0837\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
