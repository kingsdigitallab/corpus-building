003ChandDesc>\n                            \u003ChandNote>\n                                \u003C!-- Based on Glyph table: alpha35; beta16; gamma01 (or 11?); delta09. -->\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"https://pleiades.stoa.org/places/462239\">Herbessus (?)\u003C/placeName>\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/11102431/\">Montagna di Marzo\u003C/placeName>\n                                \u003Cgeo>37.42658, 14.30061\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" evidence=\"archaeological-context material-context lettering\" notBefore-custom=\"-0500\" notAfter-custom=\"-0451\">500—450 BCE\u003C/origDate>\u003C!-- check Mussinano's archive -->\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\">Found during the excavations (Soprintendenza alle Antichità di Siracusa, 1966—1968) in the area of the archaic necropolis, grave 113\u003Cgeo>37.42658, 14.30061\u003C!-- check and add more accurate coordinates here, these are the generic ones -->\u003C/geo>.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n            \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>\n            \u003Cxi:include href=\"../alists/charDecl.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>ISicily glyphs authority list\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>\n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.abecedarium\" ref=\"http://www.eagle-network.eu/voc/typeins/l od/112.html\">abecedarium\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"deprecated\" change=\"'#deprecation\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2021-10-14\" who=\"#VM\">Valentina Mignosa added metadata, text\u003C/change>\n                \u003Cchange when=\"2022-12-21\" who=\"#VM\" xml:id=\"deprecated\">Valentina Mignosa deprecated the file in favour of \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic020558\">ISic020558\u003C/ref> \n                    as this text appeared to be a different inscription due to the difference in the reading of the ductus of alpha (see Arena IGASM 2, 92, fig. 27.3; Agostiniani 2018, p. 183-184, fig. 89) but it is actually the same document (see commentary in ISic020558).\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020769_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020769.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>    \n                   \u003Clb n=\"1\"/>αβγδ\n               \u003C/ab>          \n            \u003C/div>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" subtype=\"transliteration\" xml:lang=\"scx-Latn\">                 \n               \u003Cab/>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Agostiniani, in Agostiniani, Albanese Procelli 2018.\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\" resp=\"#VM\">\n                \u003Cp>This is clearly a beginning of an alphabetarium. Interestingly, the ductus of the letters is different from what we see in the \n                    other texts: the arrow-shaped alpha is of the type with a bar attached to the vertex of the letter; the gamma is of the type \n                    with a single bar, perhaps slightly curved; the delta is of the type found in Montagna di Marzo, but with a dot in its centre. \n                    Clearly, the reason for this difference is to be found in the particular nature of the text, the alphabetarium, which presents \n                    the 'model' types of the local alphabet. It should also be noted that, without the possibility of tracing the archaeological \n                    context in this case, it is difficult to date the document on a palaeographic basis, due to the reasons mentioned above.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IAS2\">\n                        \u003Cauthor>Agostiniani\u003C/author>\n                        \u003Cdate>Forthcoming\u003C/date>\n                        \u003CcitedRange>MMA 45\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/UE7VX29V\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002519\">https://biblio.inscriptiones.org/epig10002519\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl>\n                        \u003Cauthor>Agostiniani, Albanese Procelli\u003C/author>\n                        \u003Cdate>2018\u003C/date>\n                        \u003CcitedRange>183-184, fig. 89\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/RASC3AR5\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001345\">https://biblio.inscriptiones.org/epig10001345\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
