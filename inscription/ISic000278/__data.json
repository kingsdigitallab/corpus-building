                    \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462506\" cert=\"low\">Tauromenium\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2522975\">Taormina\u003C/placeName>\n                            \t\u003Cgeo>37.85358, 15.28851\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0100\" notAfter-custom=\"0300\">Roman\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Described in the 16th century as found in an underground oven or kiln in Taormina; subsequent descriptions assigning it to Palermo suggest that it was moved there, before being lost.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cprovenance type=\"not-observed\" subtype=\"lost\">lost\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.dedication\" ref=\"https://www.eagle-network.eu/voc/objtyp/lod/88.html\">ritual dedication\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2018-03-23\" who=\"#TA\"> Tuuli Ahlholm revised the Epidoc, encoded the text and added an apparatus.\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-17\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2024-08-01\" who=\"#JP\">Jonathan Prag made initial revisions from publications\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000278_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000278.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>fulgur\n                    \u003Clb n=\"2\"/>co\u003Chi rend=\"ligature\">nd\u003C/hi>itum\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n            \u003ClistApp>\n                \u003Capp>\u003Cnote>Text of Gualtherus\u003C/note>\u003C/app>\n                \u003Capp loc=\"2\">\u003Cnote>Neither Caietanus nor Torremuzza based on Caietanus record the ligature\u003C/note>\u003C/app>\n            \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>(here) lightning has been buried\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The text is recorded in a number of late 16th century  / early 17th century antiquarians (Caietanus [d.1620, but published posthumously in 1707], Agustin, Pinelli). It is unclear if any actually saw it apart possibly from Caietanus, who describes the text as found at Taormina, whereas the others record it, seemingly via Roisius, as among the lost inscriptions in Palermo. This leads to a doublet in Torremuzza, recording the same text twice, once from Panormus and once from Taormina. Note that Mommsen speculated that the unintelligible CIL X.2.7015 from Catania might be a second example from Sicily of such a fulgur conditum text, marking the ritual burying of the ashes of a lightning strike.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Caietanus\u003C/author>\n                        \u003Cdate>1707\u003C/date>\n                        \u003CcitedRange>66\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q8AGX4XJ\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002774\">https://biblio.inscriptiones.org/epig10002774\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624\u003C/date>\n                        \u003CcitedRange>no. 61\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/GIN6NANU\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002388\">https://biblio.inscriptiones.org/epig10002388\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624 [1625]\u003C/date>\n                        \u003CcitedRange>no.219\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/ZP8DE8KS\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10001931\">https://biblio.inscriptiones.org/epig10001931\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>cl.1, no.31=32\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SPTB56H3\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1784\u003C/date>\n                        \u003CcitedRange>cl.1, no.34=35\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7PSFHSUH\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002083\">https://biblio.inscriptiones.org/epig10002083\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIL\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"http://arachne.uni-koeln.de/item/buchseite/650752\">10.6990\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GQN8UZSI\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000589\">https://biblio.inscriptiones.org/epig10000589\u003C/ref>\u003C/bibl>\n                    \u003Cbibl/>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
