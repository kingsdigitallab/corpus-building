03Cgraphic n=\"print\" url=\"ISic000282_detail.jpg\" height=\"1704px\" width=\"2272px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana; photo L. Campagna 2004-05-05\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#TA #JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> Mevius \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003CroleName type=\"civic.religious\" subtype=\"pontifex\">\u003Cexpan>\u003Cabbr>\u003Csupplied reason=\"lost\">p\u003C/supplied>ont\u003C/abbr>\u003Cex>ifex\u003C/ex>\u003C/expan>\u003C/roleName> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003CroleName type=\"civic\" subtype=\"duumvir\">\u003Cnum value=\"2\">\u003Chi rend=\"supraline\">II\u003C/hi>\u003C/num>\u003Csupplied reason=\"lost\">vir\u003C/supplied>\u003C/roleName> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                 \u003Clb n=\"3\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"line\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text from photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"1\">\u003Cnote>Mommsen: C·MEVIVS; although Mommsen marks the upper half of a C, the only traces on the stone are significantly dislocated with reference to the rest of the line, and cannot be part of the praenomen, and neither is an interpunct visible. Neither De Spuches in 1864 nor Schubring in his apograph record the C.\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>De Spuches\u003C/author>\n                        \u003Cdate>1864\u003C/date>\n                        \u003CcitedRange>13 Taormina no.2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/NBACXKXQ\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002042\">https://biblio.inscriptiones.org/epig10002042\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIL\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"http://arachne.uni-koeln.de/item/buchseite/650752\">10.6994\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/GQN8UZSI\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000589\">https://biblio.inscriptiones.org/epig10000589\u003C/ref>\u003C/bibl>\n                    \u003Cbibl/>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
