               \u003Cdepth unit=\"cm\">1-5\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragment\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Squared Ε, Α with broken bar. A wavy stroke at l. 3 divides the numeral from the following word.\u003C/p>\u003C!--measurements from Korhonen-->\n                                \u003Clocus from=\"line1\" to=\"line3\">Lines 1-3\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">22-35\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">not recorded\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0435\" notAfter-custom=\"0435\" evidence=\"internal-date\">435 CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Original discovery not recorded, but probably from Catania.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Observed by Amico in Museo dei Benedettini and later by Korhonen in Magazzino del cortile, Collezione dei Benedettini (formerly sala VI 94).\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-19\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003087_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003087.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #AA\">\n                \u003Cab>\n                    \u003Clb n=\"0\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"line\"/>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003Cw>Νων\u003Csupplied reason=\"lost\">ῶν\u003C/supplied>\u003C/w> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003Cw>\u003Csupplied reason=\"lost\">\u003CroleName type=\"supracivic\" subtype=\"consul\">ὑπατείᾳ\u003C/roleName>\u003C/supplied>\u003C/w>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\" subtype=\"imperial\">\u003Cname>\u003Csupplied reason=\"lost\">Θεοδ\u003C/supplied>οσίο\u003Csupplied reason=\"undefined\" evidence=\"previouseditor\">υ\u003C/supplied>\u003C/name>\u003C/persName> \u003Cw>\u003Csupplied reason=\"lost\">τὸ\u003C/supplied>\u003C/w> \u003Csupplied reason=\"lost\">\u003Cnum value=\"15\">ιε\u003C/num>\u003C/supplied> \u003Cw>\u003Csupplied reason=\"lost\">καὶ\u003C/supplied>\u003C/w> \u003CpersName type=\"attested\" subtype=\"imperial\">\u003Cname>\u003Csupplied reason=\"lost\">Βαλεν\u003C/supplied>\u003Clb n=\"3\" break=\"no\"/>\u003Csupplied reason=\"lost\">τινιανοῦ\u003C/supplied>\u003C/name>\u003C/persName> \u003Cw>\u003Csupplied reason=\"lost\">τ\u003C/supplied>ὸ\u003C/w> \u003Cnum value=\"4\">δ\u003C/num> \u003Cname>\u003Cexpan>\u003Cabbr>Αὐγ\u003Cam>γ\u003C/am>\u003C/abbr>\u003Cex>ούστων\u003C/ex>\u003C/expan>\u003C/name> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\" cert=\"low\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph and on Ferrua's edition\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 1\">\u003Cnote>The only trace at l. 1 seems to belong neither to a N nor to a Ω. ΝΩΝ was seen by Amico in 1741.\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 1\">\u003Cnote>According to Korhonen, also μετὰ τὴν ὑπατείαν is possible.\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>... the Nones of ... in the fifteenth consulate of Theodosius and the fourth of Valentinian, the Augusti.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>... le None di ... durante il quindicesimo consolato di Teodosio e il quarto di Valentiniano, gli Augusti.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The inscription is probably the fragment of the end of an epitaph with a consular date: the date on which the deceased died is recorded (the indication \u003Cforeign xml:lang=\"grc\">Νων[ῶν]\u003C/foreign> must have been accompanied by the day and followed by the month) and the consuls of that year. Ferrua’s supplements are based on the traces at l. 2 \u003Cforeign xml:lang=\"grc\">ΟΣΙΟ\u003C/foreign>, which could only belong to the name Theodosius and the numeral \u003Cforeign xml:lang=\"grc\">δ\u003C/foreign> probably indicating the number of the consulate of Theodosius’colleague, both Augusti. The only possibility is then 435 AD, the year of the fifteenth consulate of Theodosius II and the fourth of Valentian III (a date also confirmed by palaeography).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Amico\u003C/author>\n                        \u003Cdate>1741\u003C/date>\n                        \u003CcitedRange>277\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F5INCW3T\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002186\">https://biblio.inscriptiones.org/epig10002186\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>36.0842bis\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>168\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrua\u003C/author>\n                        \u003Cdate>1982-1983\u003C/date>\n                        \u003CcitedRange>19 no.66 fig.1g\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q7DDNWD6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001996\">https://biblio.inscriptiones.org/epig10001996\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
