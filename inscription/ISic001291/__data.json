dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.damaged\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                    \u003Cdamage ana=\"#text_condition.complete\"/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Regular letters with extensions and serifs, Ε, Σ lunate, Μ a cursive form (narrowed Σ at the end of l. 2 and l. 6), ΗΣ are in ligature at l. 6.\u003C/p>\n                                \u003C!--measurements from Korhonen-->\n                                \u003Clocus from=\"line1\" to=\"line6\">Lines 1-6\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">17-23\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">not recorded\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0051\" notAfter-custom=\"0200\" precision=\"low\">Second half of 1st century CE or 2nd century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" when=\"1760\">According to Ferrara, the inscription was found in 1760 in Catania.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"seen\">Observed by Torremuzza and Kaibel in Museo dei Benedettini, more recently by Korhonen in magazzino del cortile, Collezione dei Benedettini (formerly sala VI.81)\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-02\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>     \n                \u003Cchange when=\"2022-05-16\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001291_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001291.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cw>\u003Cexpan>\u003Cabbr>Θ\u003C/abbr>\u003Cex>εοῖς\u003C/ex>\u003C/expan>\u003C/w> \u003Cw>\u003Cexpan>\u003Cabbr>Κ\u003C/abbr>\u003Cex>αταχθονίοις\u003C/ex>\u003C/expan>\u003C/w>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>Γαλάτης\u003C/name>\u003C/persName>\n                    \u003Clb n=\"3\"/>\u003Cw>ἔζησε\u003C/w> \u003Cw>ἔτη\u003C/w>\n                    \u003Clb n=\"4\"/>\u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cnum value=\"80\">π\u003C/num> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cw>ἐποίη\u003Clb n=\"5\" break=\"no\"/>σεν\u003C/w> \u003Cw>υἱὸς\u003C/w>\n                    \u003Clb n=\"6\"/>\u003CpersName type=\"attested\">\u003Cname>Πριμιγένης\u003C/name>\u003C/persName>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photographs\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 6\">\u003Cnote>Torremuzza, Ferrara: Πριμιγένη; Franz, Korhonen: Πριμιγένης; Kaibel: Πριμιγένις\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf\">\n                \u003Cp>To the underworld spirits. Galates lived for 80 years. His son, Primigenes made it.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                 \u003Cp>Agli Dei inferi. Galates visse 80 anni. Lo ha realizzato il figlio, Primigenes.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#martaf\">\n                \u003Cp>The epitaph presents the traditional formula with the invocation to the underworld spirits, the name of the deceased, his age and the name of the dedicant (Korhonen 2004: 86-87). \u003Cforeign xml:lang=\"grc\">Γαλάτης\u003C/foreign> recurs in the Latin form in inscriptions of South Italy (LGPN 3a: 96), whereas \u003Cforeign xml:lang=\"grc\">Πριμιγένης\u003C/foreign> seems to be a contamination between the Greek form \u003Cforeign xml:lang=\"grc\">Πρωτογένης\u003C/foreign> and the Latin form \u003Cforeign xml:lang=\"la\">Primigenius\u003C/foreign> (Solin 2005: 170). This inscription is classified in LGPN 3a: 376 under the name \u003Cforeign xml:lang=\"grc\">Πριμογένις\u003C/foreign>. \u003Cforeign xml:lang=\"grc\">Πριμογένης\u003C/foreign> recurs in another inscription from Catania, IG 14.546 = \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001365\">ISic001365\u003C/ref>, and in an inscription from Syracuse, IGCVO 1339 = MARPO inv.15591 (in both the form is \u003Cforeign xml:lang=\"grc\">Πριγομένης\u003C/foreign>, with an inversion of \u003Cforeign xml:lang=\"grc\">μ\u003C/foreign> and \u003Cforeign xml:lang=\"grc\">γ\u003C/foreign>).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003Cauthor>Kaibel\u003C/author>\n                        \u003Cdate>1890\u003C/date>\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">467\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                         \u003Cauthor>Korhonen\u003C/author>\n                         \u003Cdate>2004\u003C/date> \n                        \u003CcitedRange>90\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003Cauthor>Franz\u003C/author>\n                        \u003Cdate>1853\u003C/date>\n                        \u003CcitedRange>5701\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/TQMVHEVL\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10001250\">https://biblio.inscriptiones.org/epig10001250\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>151 no. 29\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SPTB56H3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1784\u003C/date>\n                        \u003CcitedRange>158 no. 32\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7PSFHSUH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002083\">https://biblio.inscriptiones.org/epig10002083\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrara\u003C/author>\n                        \u003Cdate>1829\u003C/date> \n                        \u003CcitedRange>354 no. 2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/82T4IWDN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002005\">https://biblio.inscriptiones.org/epig10002005\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\n\t       \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                         \u003Cauthor>Korhonen\u003C/author>\n                         \u003Cdate>2004\u003C/date> \n\t           \u003CcitedRange>86-87\u003C/citedRange>\n\t           \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n\t       \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n\t       \u003Cbibl>\n\t           \u003Cauthor>Solin\u003C/author>\n\t           \u003Cdate>2005\u003C/date>\n\t           \u003CcitedRange>170\u003C/citedRange>\n\t           \u003Cptr target=\"http://zotero.org/groups/382445/items/2UJHZZXJ\"/>\n\t       \u003Cref target=\"https://biblio.inscriptiones.org/epig10001423\">https://biblio.inscriptiones.org/epig10001423\u003C/ref>\u003C/bibl>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
