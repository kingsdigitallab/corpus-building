sertedValue=\"grc\">\u003Cdesc>Ancient Greek\u003C/desc>\u003C/certainty>\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\n                                    \u003Cp>Fragments of foot of a black-glaze lekythos (?).\u003C/p>\n                                    \u003Cmaterial ana=\"#material.clay.fired\" ref=\"https://www.eagle-network.eu/voc/material/lod/131.html\">ceramic\u003C/material>\n                                    \u003CobjectType ana=\"#object.vessel.lekythos\" cert=\"low\" ref=\"http://kerameikos.org/id/lekythos\">lekythos\u003C/objectType> \u003C!-- ref=\"https://www.eagle-network.eu/voc/objtyp/lod/305.html\" -->\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\"/>\n                                        \u003Cdim type=\"diameter\" subtype=\"min\" unit=\"cm\"/>\n                                        \u003Cdim type=\"diameter\" subtype=\"max\" unit=\"cm\"/>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragments\"/>\n                            \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.scratched\" ref=\"https://www.eagle-network.eu/voc/writing/lod/9\">scratched\u003C/rs>\n                                    \u003Cp>The text consists of two inscriptions, arranged on two different sides of the same object. \n                                        The first one (face a) is on the underside of the foot: it is arranged in a clockwise spiral, on one line. \n                                        The second one (face b) is on the underside of the foot: it is arranged in a clockwise spiral, on one line.\n                                        On both faces the text has been highlighted with white painting in modern times to make it more legible.\u003C/p>\n                                    \u003Cdamage ana=\"#text_condition.incomplete text_condition.partly_legible\"/>\n                                \u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003C!--Based on Glyph table: (a) alpha09; epsilon09(?); iota01; pi11; sigma02; upsilon03. (b) iota01; mu01; pi11. -->\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions xml:id=\"faceA\" type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">4\u003C/height>\n                                \u003C/dimensions>\n                                \u003Cdimensions xml:id=\"faceB\" type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">8-9\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"https://pleiades.stoa.org/places/462487\">Segesta\u003C/placeName>\n                                \u003CplaceName type=\"modern\"/>\n                                \u003Cgeo>37.94171, 12.83649\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" evidence=\"archaeological-context material-context lettering\" notBefore-custom=\"-0500\" notAfter-custom=\"-0480\">500—480 BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Accidentally found by a local shepherd in the excavations area and attributed (Voza 1966, p.218, n.7) to the Segesta area of Grotta Vanella, on the north-east slope of Monte Barbaro\u003Cgeo>37.94240, 12.84509\u003C/geo>, 1.9km east of temple of Segesta.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#VM\" when=\"2021-12-15\">Mignosa 2021-12-15\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n            \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm key=\"dedication\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/79.html\" cert=\"low\">dedication\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2021-02-15\" who=\"#VM\">Valentina Mignosa created file, added metadata, text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020318_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020318.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"xly-Grek\" resp=\"#VM\">\n                \u003Cdiv type=\"te