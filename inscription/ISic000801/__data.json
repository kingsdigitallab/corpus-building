 \u003C/material>\n                                    \u003CobjectType ana=\"#object.plaque\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/259\">plaque\u003C/objectType>\n                                    \u003Cp>Fragment of white marble slab, broken on all sides; finished on the reverse\u003C/p>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">26\u003C/height>\n                                        \u003Cwidth unit=\"cm\">22\u003C/width>\n                                        \u003Cdepth unit=\"cm\">2.5\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                    \u003Cp>Parts of two lines of latin letters are preserved. Substantial vacat below line 2.\u003C/p>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Cp>The letters are neatly V-cut, of wide proportions, with minimal serifs. P is open; S\n                                    has a larger upper half, leaning slightly to the right. Words are separated by a\n                                    comma-like three pointed serif.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line2\">Lines 1-2\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">38-40\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Not measured\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462225\">Halaesa\u003C/placeName>\t\t\n                                \u003Coffset>near\n                                    \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525099\">Castel di Tusa\u003C/placeName>\n                                \u003C/offset>\n                            \t\u003Cgeo>37.99907, 14.26197\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0001\" notAfter-custom=\"0100\" precision=\"low\">Mid 1st century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Excavated 9 September 1970, near entrance to room 2 of west portico of the agora.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">On display.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#JP\">2011.06.15\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                    \n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.honorific #function.building\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/69.html\" cert=\"low\">honorific; building\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-08-22\" who=\"#JP\">Jonathan Prag revised the EpiDoc in line with new edition\u003C/change>\n                \u003Cchange when=\"2019-07-04\" who=\"#JP\">Jonathan Prag made minor edits to EpiDoc\u003C/change>\n                \u003Cchange when=\"2020-09-09\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-17\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic000801_tiled.tif\" height=\"6190px\" width=\"4912px\">\n                \u003Cdesc>Photo J. Prag\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic000801.jpg\" height=\"6190px\" width=\"4912px\">\n                \u003Cdesc>Photo J. Prag\u003C/desc>\n            \u003C/graphic>\n        \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\n                    \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>CILIV\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\n                    \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>R \u003Cg ref=\"#interpunct\">·\u003C/g> S \u003Cg ref=\"#interpunct\">·\u003C/g> P \u003Cg ref=\"#interpunct\">·\u003C/g> IN\u003Cunclear>O\u003C/unclear>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text from autopsy\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation not possible, too fragmentary-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>It is not possible to know what the likely extent of the stone was, either to the left and right, or\n                    above and below the surviving text. The letters of line 1 are most likely to be part of a name:\n                    Caecilius and Acilius are the most common, but a number of other names of this form such as\n                    Lucilius, Maecilius, Otacilius, Pacilius, Precilius, and Racilius are also attested. Both Caecilius\n                    (\u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic000803\">ISic000803\u003C/ref>; RPC I, 628-629; Cic. Verr. 2.2.23) and Acilius (\u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic000804\">ISic000804\u003C/ref>) are attested at Halaesa. The\n                    abbreviation in line 2 is most easily resolved as restituit sua pecunia (“restored (x) at his own\n                    expense”), although this cannot be certain, and Scibona’s suggestion of inopia for the word that\n                    follows is certainly plausible (i.e. that the individual named in the first part of the inscription\n                    restored something – a building perhaps – at his/her own expense due to the lack of funds on the\n                    part of the town or another body or individual). Manganaro’s suggestion of incohavit is also\n                    possible, and there are few other alternatives beginning INC or INO.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"AE\">\n                        \u003CcitedRange>1973.0268\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/R46KDTZX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001283\">https://biblio.inscriptiones.org/epig10001283\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Scibona\u003C/author>\n                        \u003Cdate>1971\u003C/date>\n                        \u003CcitedRange>15 no.4 fig.2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/UISW82JC\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001599\">https://biblio.inscriptiones.org/epig10001599\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1988\u003C/date>\n                        \u003CcitedRange>45-46\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/RZSFKACR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001427\">https://biblio.inscriptiones.org/epig10001427\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Scibona\u003C/author>\n                        \u003Cdate>2008\u003C/date>\n                        \u003CcitedRange>26 ph\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/6GFVUASD\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001593\">https://biblio.inscriptiones.org/epig10001593\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Prestianni Giallombardo\u003C/author>\n                        \u003Cdate>2012\u003C/date>\n                        \u003CcitedRange>181 fig.166\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/CJISRIUV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001357\">https://biblio.inscriptiones.org/epig10001357\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Prag and Tigano\u003C/author>\n                        \u003Cdate>2017\u003C/date>\n                        \u003CcitedRange>no.23\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/MI64NB9L\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002630\">https://biblio.inscriptiones.org/epig10002630\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
