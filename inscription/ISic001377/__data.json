>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>lunate sigma and the use of a supraline on numeral recorded by Amico\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0404\" notAfter-custom=\"0404\" evidence=\"internal-date\" cert=\"low\">404 CE, a conjecture by Ferrua of the consular date\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1741\">Recorded by Amico 1741 as previously (according to Carrera) 'penes Blascum Romanum Columnam', but subsequently in the museum of S. Nicolai.\u003C/provenance>\n                        \u003Cprovenance type=\"not-observed\" subtype=\"lost\">lost\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2024-07-10\" who=\"#JP\">Jonathan Prag revised from publication\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001377_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001377.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>ἐνθάδ\u003Csupplied reason=\"lost\">ε\u003C/supplied> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \n                    \u003Clb n=\"2\"/>ζησασ\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"3\"/>ἔτη \u003Cnum value=\"50\">\u003Chi rend=\"supraline\">ν\u003C/hi>\u003C/num> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \n                    \u003Clb n=\"4\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Corig>ΥΤΟ\u003C/orig>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003C/ab>\n            \u003C/div>\n          \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n              \u003ClistApp>\u003Capp>\u003Cnote>Text after Amico and Kaibel\u003C/note>\u003C/app>\n              \u003Capp loc=\"1\">\u003Cnote>Amico 1741: 274: ΕΝΘΑΚ\u003C/note>\u003C/app>\n                  \u003Capp loc=\"3\">\u003Cnote>Amico 1741: 274: ΕΤΗΝ; Amico 1741: 277: ΕΤΕΝ̅\u003C/note>\u003C/app>\n                  \u003Capp loc=\"4\">\u003Cnote>Amico 1741: 274: ΚΤΩ....; Kirchoff: [τελευτᾷ τῇ πρὸ -- Ὀ]κτω[βρίων]; Ferrua:  [ὑπατίᾳ — —ο]υ τὸ[\u003C/note>\u003C/app>\n              \u003C/listApp>  \n           \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Amico presents the text in two different variants, without seemingly recognising that it is the same text. Ferrua suggests that the traces in line 4 would be compatible with a consular dating, such as for an imperial consul, as with Honorius in 404 CE, but obviously the actual date is speculative. Kirchoff in CIG offered a possible restoration of line 3-4 on the basic of Amico's alternative reading.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Amico\u003C/author>\n                        \u003Cdate>1741\u003C/date>\n                        \u003CcitedRange>274, 277\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F5INCW3T\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002186\">https://biblio.inscriptiones.org/epig10002186\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0558\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>36.0844\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>4.9500\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrua\u003C/author>\n                        \u003Cdate>1982-1983\u003C/date>\n                        \u003CcitedRange>27 no.91\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q7DDNWD6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001996\">https://biblio.inscriptiones.org/epig10001996\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
