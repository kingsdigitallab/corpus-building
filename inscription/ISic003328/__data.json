/change>\n\t               \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                            \u003Cchange when=\"2021-02-23\" who=\"#VM\">Valentina Mignosa encoded text division, cleaned up the autogenerated text and checked the edition\u003C/change>\n                \u003Cchange when=\"2023-03-20\" who=\"#JP\">Jonathan Prag added image\u003C/change>\n                \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003328_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>Photo J. Prag, 02.03.2023, with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003328.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>Photo J. Prag, 02.03.2023, with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\" xml:id=\"DgεAK\"/>\u003Cw xml:id=\"DgεAU\">πρᾶτος\u003C/w> \n                    \u003Clb n=\"2\" xml:id=\"DgεAe\"/>\u003Cw xml:id=\"DgεAo\">ποτὶ\u003C/w> \u003Cw xml:id=\"DgεAy\">κε\n                    \u003Clb n=\"3\" break=\"no\" xml:id=\"DgεAΙ\"/>φαλάς\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003Cli