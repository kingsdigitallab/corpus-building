u003Cname xml:id=\"SS\" ref=\"https://orcid.org/0000-0003-3914-9569\">Simona Stoyanova\u003C/name>\n     \t       \u003Cresp>standardisation of template and tidying up encoding\u003C/resp>\n \t   \u003C/respStmt>\n\t    \u003CrespStmt>\n                    \u003Cname xml:id=\"system\">system\u003C/name>\n                    \u003Cresp>automated or batch processes\u003C/resp>\n                \u003C/respStmt>\n            \u003C/titleStmt>\n            \u003CpublicationStmt>\n                \u003Cauthority>I.Sicily\u003C/authority>\n                \u003Cidno type=\"filename\">ISic002728\u003C/idno>\n                \u003Cidno type=\"TM\">645148\u003C/idno>\n                \u003Cidno type=\"EDR\">136557\u003C/idno>\n                \u003Cidno type=\"EDH\"/>\n                \u003Cidno type=\"EDCS\">64400335\u003C/idno>\n                \u003Cidno type=\"PHI\">333215\u003C/idno>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic002728\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-23\">10.5281/zenodo.4387315\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative Commons-Attribution 4.0 licence.\u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Segesta\u003C/settlement>\n                        \u003Crepository role=\"site\" ref=\"http://sicily.classics.ox.ac.uk/museum/196\">Parco archeologico di Segesta\u003C/repository>\n                        \u003Cidno type=\"inventory\">SG7542\u003C/idno>\n                        \u003Cidno type=\"inventory\">IG2627\u003C/idno>\n                        \u003C!--Adding location for old id numbers if available-->\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>Fragment of a block of limestone, perhaps a statue base, of which the upper edge may be intact, but it is broken below and on the left; the right margin may be intact. The face is lost from the upper half of the front face.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.limestone\" ref=\"http://www.eagle-network.eu/voc/material/lod/66.html\">limestone\u003C/material>\n                                    \u003CobjectType ana=\"#object.block\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/189.html\">block\u003C/objectType>\n                                    \u003Cdimensions>\u003C!--ISegesta-->\n                                        \u003Cheight unit=\"cm\">11.5\u003C/height>\n                                        \u003Cwidth unit=\"cm\">20.1\u003C/width>\n                                        \u003Cdepth unit=\"cm\">23.2\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragment\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>Parts of two lines preserved, with a wide space between them; unclear if more lines were preserved above.\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage ana=\"#text_condition.incomplete\"/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>The letters are somewhat worn, but present a regular and wide/square module, broadly and deeply incised. Alpha with straight bar, epsilon with shorter middle bar, omicron smaller and above the line, rho with a small closed eye.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\u003C!-- ISegesta -->\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">13-17 (13 = omicron)\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"https://pleiades.stoa.org/places/462487\">Segesta\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2523171\">Segesta\u003C/placeName>\n                            \t\u003Cgeo>37.94171, 12.83649\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0200\" notAfter-custom=\"-0001\" evidence=\"lettering\">2nd — 1st century BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1995\">A casual find in the area of the agora, bordering the original road to the theatre\u003Cgeo>37.9397241,12.8416623\u003C/geo>\u003C/provenance>\u003C!-- findspot located using googlemaps, following ISegesta fig.138 -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.honorific\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/69.html\">honorific\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-09-23\" who=\"#JP\">Jonathan Prag added ISegesta ref\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2025-06-22\" who=\"#JP\">Jonathan Prag revised based upon publications\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic002728_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic002728.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\" source=\"http://zotero.org/groups/382445/items/3PK3JKXR\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003CpersName type=\"attested\">\u003Cname>Ἀ\u003Cunclear>ππ\u003C/unclear>ειραῖον\u003C/name>\u003C/persName>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\u003Cgap reason=\"illegible\" unit=\"character\" quantity=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#JP\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text of I.Segesta\u003C/note>\u003C/app>\n                    \u003Capp loc=\"2\">\u003Cnote>Two horizontal strokes are visible, forming the upper part of two letters, such as Γ or Ε\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>Nenci described the fragment as part of an architrave, but Ampolo and Erdas consider it more likely to be part of a statue base. Appeiraios is attested as the third onomastic element in the name of Diodoros son of Tittelos in \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic001106\">ISic001106\u003C/ref>. This is likely to be another honorific, but not of course necessarily for the same individual.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"bulletin\" n=\"SEG\">\n                        \u003CcitedRange>45.1393\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F2GG87EQ\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000796\">https://biblio.inscriptiones.org/epig10000796\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGDS2\">\n                        \u003CcitedRange>87\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/SM2KDHZC\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000160\">https://biblio.inscriptiones.org/epig10000160\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Nenci\u003C/author>\n                        \u003Cdate>1995\u003C/date>\n                        \u003CcitedRange>1183 no.2 tav.267.2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/WHBJDH75\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001415\">https://biblio.inscriptiones.org/epig10001415\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Nenci\u003C/author>\n                        \u003Cdate>1997\u003C/date>\n                        \u003CcitedRange>1191-1192 tav.234.2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/4FSSHE62\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001729\">https://biblio.inscriptiones.org/epig10001729\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>De Vido\u003C/author>\n                        \u003Cdate>2003\u003C/date>\n                        \u003CcitedRange>400 no.12\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/33HPA43I\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002041\">https://biblio.inscriptiones.org/epig10002041\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ampolo and Erdas\u003C/author>\n                        \u003Cdate>2019\u003C/date>\n                        \u003CcitedRange>G27\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/3PK3JKXR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001036\">https://biblio.inscriptiones.org/epig10001036\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
