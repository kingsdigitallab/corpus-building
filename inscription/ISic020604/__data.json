>\n                                    \u003Cdamage ana=\"#text_condition.incomplete\"/>\n                                \u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">7\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462313\"/>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/11103527\">Mendolito\u003C/placeName>\n                                \u003Cgeo>37.699040, 14.808181\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" evidence=\"archaeological-context material-context lettering\" notBefore-custom=\"-0500\" notAfter-custom=\"-0401\" precision=\"low\">5th century BCE (according to Cultraro 1989-1990, second quarter of the 5th century BCE).\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Contrada Ardichella, North of the Mendolito settlement area\u003Cgeo>37.699040, 14.808181\u003C/geo>.\u003C/provenance>\u003C!-- general coordinates for Mendolito settlement -->\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">No Autopsy\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n            \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n            \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>\n            \u003Cxi:include href=\"../alists/charDecl.xml\">\n                \u003Cxi:fallback>\n                    \u003Cp>ISicily glyphs authority list\u003C/p>\n                \u003C/xi:fallback>\n            \u003C/xi:include>\n        \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.building\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/86.html\">building\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n                \u003Cchange when=\"2020-12-23\" who=\"#JP\">Updated Zenodo DOI\u003C/change>\n                \u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-01-21\" who=\"#VM\">Valentina Mignosa added text and further data.\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020604_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020604.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"scx-Grek\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>ͱολ\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" subtype=\"transliteration\" xml:lang=\"scx-Latn\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>hol\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Manganaro 1961\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The document consists of two matching fragments, found accidentally during agricultural works. \n                    It was published by Cultraro (1989-1990, 451-452), who dates the piece to the second quarter of the \n                    5th century BCE on the basis of the archaeological context.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Cultraro\u003C/author>\n                        \u003Cdate>1989-1990\u003C/date>\n                        \u003CcitedRange>451-452\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/WZJDK5JD\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002383\">https://biblio.inscriptiones.org/epig10002383\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1961\u003C/date>\n                        \u003CcitedRange>110, pl. 50.2\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/4356IBUF\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002677\">https://biblio.inscriptiones.org/epig10002677\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Agostiniani\u003C/author>\n                        \u003Cdate>2009\u003C/date>\n                        \u003CcitedRange>115-116, no.1 fig. 2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/63R3F4J2\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002427\">https://biblio.inscriptiones.org/epig10002427\u003C/ref>\n                    \u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl>\n                        \u003Cauthor>Mignosa\u003C/author>\n                        \u003Cdate>2017-2018\u003C/date>\n                        \u003CcitedRange>217, no. 3\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/2EBZBFTF\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002428\">https://biblio.inscriptiones.org/epig10002428\u003C/ref>\u003C/bibl>\n                    \n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
