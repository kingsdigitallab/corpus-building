pository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>Marble plaque damaged on the upper right corner and lower left corner, set in plaster in modern times.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble\u003C/material>\n                                    \u003CobjectType ana=\"#object.plaque\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/257\">plaque\u003C/objectType>\n                                    \u003Cdimensions>\u003C!--measurements from Korhonen-->\n                                        \u003Cheight unit=\"cm\">21\u003C/height>\n                                        \u003Cwidth unit=\"cm\">22\u003C/width>\n                                        \u003Cdepth unit=\"cm\">0.5-5\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.damaged\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Irregular letters: angular Ε, lunate Σ, cursive Ω. A with a slanted bar (except at l. 1 where the bar is straight), Υ with the form of the Latin V. Interpuncts divide numerals from words.\u003C/p>\u003C!--measurements from Korhonen-->\n                                \u003Clocus from=\"line1\" to=\"line6\">Lines 1-6\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">20-35\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0301\" notAfter-custom=\"0500\" precision=\"low\">4th — 5th century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\">Found “in area extra Stesichori portam” (Amico).\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Observed by Amico and Castelli in Museo Biscari, later by Korhonen in magazzino del cortile, Collezione Biscari (formerly sala VI 96).\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-16\" who=\"#AA\">Alex Antoniou Epidoc editing and English translation\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-12-22\" who=\"#martaf\">Marta Fogagnolo revised the text encoding, added dimensions, translations and commentary\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001363_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001363.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#martaf #\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cw>Ἐνθάδε\u003C/w>\n                    \u003Clb n=\"2\"/>\u003Cw>κεῖται\u003C/w> \u003CpersName type=\"attested\">\u003Cname>Οὔρ\u003Clb n=\"3\" break=\"no\"/>σουλος\u003C/name>\u003C/persName> \u003Cw>ζή\u003Clb n=\"4\" break=\"no\"/>\u003Csupplied reason=\"lost\">σα\u003C/supplied>ς\u003C/w> \u003Cw>ἔτη\u003C/w> \u003Cg type=\"#interpunct\">·\u003C/g> \u003Cnum value=\"28\">κη\u003C/num> \u003Cw>μῆ\u003Clb n=\"5\" break=\"no\"/>\u003Csupplied reason=\"lost\">νας\u003C/supplied>\u003C/w> \u003Cnum value=\"6\">ς\u003C/num> \u003Cg type=\"#interpunct\">·\u003C/g> \u003Cw>\u003Cexpan>\u003Cabbr>ἡμέρ\u003C/abbr>\u003Cex>ας\u003C/ex>\u003C/expan>\u003C/w> \u003Cg type=\"#stop\"/> \u003Cnum value=\"8\">η\u003C/num> \u003Cg type=\"#interpunct\">·\u003C/g> \u003Cw>τελ\u003Clb n=\"6\" break=\"no\"/>\u003Csupplied reason=\"lost\">ευτᾷ\u003C/supplied>\u003C/w> \u003Cw>\u003Cexpan>\u003Cabbr>καλ\u003C/abbr>\u003Cex>ανδαῖς\u003C/ex>\u003C/expan>\u003C/w> \u003Cw>\u003Cexpan>\u003Cabbr>Αὐγ\u003C/abbr>\u003Cex>ούσταις\u003C/ex>\u003C/expan>\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\" resp=\"#martaf\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text based on photograph\u003C/note>\u003C/app>\n                    \u003Capp loc=\"lines 2-3\">\u003Cnote>Kirchhoff: Οὐρσοῦλος; Kaibel, Wessel, Korhonen: Οὔρ|σουλος\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 4\">\u003Cnote>Amico, Kirchhoff, Kaibel, Wessel: κε; Torremuzza, Ferrara, Korhonen: κη\u003C/note>\u003C/app>\n                    \u003Capp loc=\"lines 5-6\">\u003Cnote>Ferrara: τελ[εύτατος] (sic); Kirchhoff: τελ[ευτᾷ---]; Kaibel: τελ(ευτήσας); Korhonen: τελ|[ευτᾷ]\u003C/note>\u003C/app>\n                    \u003Capp loc=\"line 6\">\u003Cnote>Kirchhoff: καλ(ανδ---) Αὐγ[ούστ---]; Kaibel: [τῇ πρὸ ---] καλ(ανδῶν) Αὐγ(ούστων); Wessel: [πρὸ ---] καλ. Αὐγ.; Korhonen: καλ(ανδαῖς) Αὐγ(ούσταις). [τῇ πρ(ὸ) ---] καλ(ανδῶν) Αὐγ(ούστων) seems less probable because of the space left, considering that the verb τελευτάω is never abbreviated in inscriptions from Catania\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#martaf #AA\">\n                \u003Cp>Here lies Oursoulos, having lived for 28 years, 6 months and 8 days. He died on the Kalends of August.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#martaf\">\n                \u003Cp>Qui giace Oursoulos, che ha vissuto per 28 anni, 6 mesi e 8 giorni. È morto nelle Calende di agosto.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#martaf\">\n                \u003Cp>This inscription presents the traditional structure of the Christian funerary inscriptions from Catania, with the formula \u003Cforeign xml:lang=\"grc\">ἐνθάδε κεῖται ὁ δεῖνα\u003C/foreign>, the age of the deceased with the participle of \u003Cforeign xml:lang=\"grc\">ζάω\u003C/foreign> and a second sentence introduced by \u003Cforeign xml:lang=\"grc\">τελευτᾷ\u003C/foreign> (which never appears in the participle form: for this reason, the restoration of Kaibel, \u003Cforeign xml:lang=\"grc\">τελευτήσας\u003C/foreign>, has to be rejected) with the date of death, \u003Cforeign xml:lang=\"la\">dies natalis\u003C/foreign> for the Christians. The name of the deceased, \u003Cforeign xml:lang=\"grc\">Οὔρσουλος\u003C/foreign>, recurs in other inscriptions from the imperial age (IGB 3.1772).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Amico\u003C/author>\n                        \u003Cdate>1741\u003C/date>\n                        \u003CcitedRange>272 no.18\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/F5INCW3T\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002186\">https://biblio.inscriptiones.org/epig10002186\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>188 no.134\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SPTB56H3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1784\u003C/date>\n                        \u003CcitedRange>199 no.156\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7PSFHSUH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002083\">https://biblio.inscriptiones.org/epig10002083\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ferrara\u003C/author>\n                        \u003Cdate>1829\u003C/date>\n                        \u003CcitedRange>354 no.2\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/82T4IWDN\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002005\">https://biblio.inscriptiones.org/epig10002005\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0544\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>202\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>4.9483\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"IGCVO\">\n                        \u003CcitedRange>1374\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/9CT5ZWIK\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000394\">https://biblio.inscriptiones.org/epig10000394\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
