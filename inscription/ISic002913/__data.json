n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.building\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/86.html\" cert=\"low\">building\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2025-04-11\" who=\"#AT\">Alfredo Tosques added the text as reconstructed by Manganaro and a short apparatus\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\u003Csurface/>\u003C!-- \n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic002913_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic002913.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface> -->\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#AT\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003Csupplied reason=\"lost\" cert=\"low\">viam\u003C/supplied> \u003Csupplied reason=\"lost\">e\u003C/supplied>mporii \u003Csupplied reason=\"lost\" cert=\"low\">stravit\u003C/supplied>  \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/> \u003Csupplied reason=\"lost\" cert=\"low\">et\u003C/supplied> \u003Csupplied reason=\"lost\" cert=\"low\">populo\u003C/supplied> \u003Csupplied reason=\"lost\" cert=\"low\">epulum\u003C/supplied> triclini\u003Csupplied reason=\"lost\">s\u003C/supplied> \u003Csupplied reason=\"lost\" cert=\"low\">dedit\u003C/supplied> \u003Cgap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\u003Cnote>Text of Mangarno 2005\u003C/note>\u003C/app>\n                    \u003Capp loc=\"2\">\u003Cnote>Manganaro 1982 also suggested: [- epulum in] triclini[s populo dedit?]\u003C/note>\u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp>\u003C!--translation(s) - add @xml:lang for 'en' or 'it' to div-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>\u003C!--commented out pending revision-->\n                    \u003C!--Manganaro rejects the earlier suggested restoration of Porticus for the first line, instead adopting emporii, and restoring elements such as viam emporii stravit and then epulum in triclinis populo dedit (only mporii | triclini survives). He remarks that  the inscription is another link to a community / town history with a clear mention of an emporium, which in turn must be linked in some way to a Municipium, which at some point was in close relationship to the place where the Constantinian villa stood. But not clear if he's suggesting this precedes the villa? Note that Wilson is highly sceptical a) of the reading of emporium, and b) of whether any of these latin stones even come from Sicily, or might not simply be imported building material (equally of course, they could be imported from e.g. Enna, which was a damn sight closer!).-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1982\u003C/date>\n                        \u003CcitedRange>503 no.8 fig.11-12\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/VG592RBX\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001835\">https://biblio.inscriptiones.org/epig10001835\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Pucci\u003C/author>\n                        \u003Cdate>1971\u003C/date>\n                        \u003CcitedRange>252 fig.115\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/82KV6NAB\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001653\">https://biblio.inscriptiones.org/epig10001653\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Wilson\u003C/author>\n                        \u003Cdate>1983\u003C/date>\n                        \u003CcitedRange>93\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/C9HTRTCR\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002650\">https://biblio.inscriptiones.org/epig10002650\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>2005\u003C/date>\n                        \u003CcitedRange>186 no.8 fig.24-24bis\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/H5C4UQ9K\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001800\">https://biblio.inscriptiones.org/epig10001800\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
