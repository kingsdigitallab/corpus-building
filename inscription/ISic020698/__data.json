alendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.production-ownership\" ref=\"http://vocab.getty.edu/page/aat/300195926\">ownership\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n                \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n                \u003Cchange when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n                \u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-01-27\" who=\"#VM\">Valentina Mignosa added metadata and text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020698_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020698.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" resp=\"#VM\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname type=\"idionym\">Νενδας\u003C/name>\u003C/persName>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Cordano 1993\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>One of two kylikes found at Castiglione di Ragusa (Western necropolis, grave 111), both bearing the name Νενδας.\n                    The word is very common in the inscriptions of the indigenous Sicily (see \u003Cref target=\"http://zotero.org/groups/382445/items/4HMK3Q8K\">Sudano 2015\u003C/ref>, 702, note 16, and e.g. the \n                    inscription from Sciri \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic003362\">ISic003362\u003C/ref> and the graffito from Terravecchia di Grammichele \u003Cref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic020703\">ISic020703\u003C/ref>).\n                    Νενδας has been interpreted as theonym by \u003Cref target=\"http://zotero.org/groups/382445/items/HPEPK9TF\">Manganaro 2003\u003C/ref>, 149, then as a word part of a funerary votive formula by \n                    \u003Cref target=\"http://zotero.org/groups/382445/items/CVY2ZP7C\">Agostiniani, Prosdocimi 1976-1977\u003C/ref>, 229-230 (it often occurs in inscriptions on vessels from grave goods).\n                    The current interpretation is that it is an indigenous anthroponym (\u003Cref target=\"http://zotero.org/groups/382445/items/RHXSKW4A\">Agostiniani-Cordano 2002\u003C/ref>, 83-85).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Cordano\u003C/author>\n                        \u003Cdate>1993\u003C/date>\n                        \u003CcitedRange>155-156, fig.1\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/X85U7FJ6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002371\">https://biblio.inscriptiones.org/epig10002371\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Pelagatti\u003C/author>\u003C!-- wrong reference in Manganaro 1998 -->\n                        \u003Cdate>1972-1973\u003C/date>\n                        \u003CcitedRange>184\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/CGDHUIR6\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002368\">https://biblio.inscriptiones.org/epig10002368\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>2003\u003C/date>\n                        \u003CcitedRange>149\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/HPEPK9TF\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001843\">https://biblio.inscriptiones.org/epig10001843\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manganaro\u003C/author>\n                        \u003Cdate>1998\u003C/date>\n                        \u003CcitedRange>260, n. 45, 262\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/9C22F73R\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002370\">https://biblio.inscriptiones.org/epig10002370\u003C/ref>\u003C/bibl>\n                    \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
