da10 (?unclear); mu04; upsilon09; lunate omega. -->\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">4-5\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003C!-- can we put here the information that this inscription is probably a forgery? -->\n                                \u003CplaceName type=\"ancient\" ref=\"https://pleiades.stoa.org/places/462487\">Segesta\u003C/placeName>\n                                \u003CplaceName type=\"modern\"/>\n                                \u003Cgeo>37.94171, 12.83649\u003C/geo>\n                            \u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0400\" notAfter-custom=\"-0301\" precision=\"low\">4th century BCE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" from=\"1957\" to=\"1977\">Found during the excavations (from 1957 to 1977) by the Soprintendenza alle Antichità della Sicilia Occidentale in the area of Grotta Vanella, on the north-east slope of Monte Barbaro\u003Cgeo>37.94240, 12.84509\u003C/geo>, 1.9km east of temple of Segesta.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language> \n                \u003Clanguage ident=\"it\">Italian\u003C/language> \n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language> \n                \u003Clanguage ident=\"la\">Latin\u003C/language> \n                \u003Clanguage ident=\"he\">Hebrew\u003C/language> \n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language> \n                \u003Clanguage ident=\"xly\">Elymian\u003C/language> \n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>  \n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>  \n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.unknown\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/143.html\">unknown\u003C/term>\n                    \u003Cterm key=\"falsum\">Falsum\u003C/term> \u003C!-- could distinguish fake vs modern, to be discussed -->\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"draft\">\n            \u003ClistChange>\n                \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n                \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n                \u003Cchange when=\"2020-12-15\" who=\"#VM\">Valentina Mignosa added metadata, text\u003C/change>\n            \u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic020291_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic020291.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"xly-Grek\"> \n                    \u003Cab>\n                        \u003Clb n=\"1\"/>Ε\u003Cunclear>Λ\u003C/unclear>ΥΜΩΙ\n                    \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" subtype=\"transliteration\" xml:lang=\"xly-Latn\">\n                \u003Cab/>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text after Agostiniani 1977 (IAS I)\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\">\n                \u003Cp>\u003C!--No translation currently agreed-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\">\n                \u003Cp>\u003C!--No translation currently agreed-->\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\">\n                \u003Cp>This is the only inscription in the Elymian corpus that is surely identifiable as Greek. From the shape of the letters the inscription can be dated, according to Agostiniani, to a time not earlier than the Hellenistic period. Doubts about the authenticity of the inscription have been raised legitimately by Manni Piraino (1972).\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IAS1\">\n                        \u003Cauthor>Agostiniani\u003C/author>\n                        \u003Cdate>1977\u003C/date>\n                        \u003CcitedRange>291\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/QR2S9AJ9\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002201\">https://biblio.inscriptiones.org/epig10002201\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Manni Piraino\u003C/author>\n                        \u003Cdate>1972-1973\u003C/date>\n                        \u003CcitedRange>344-345\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/7T44W8CM\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001773\">https://biblio.inscriptiones.org/epig10001773\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Ambrosini\u003C/author>\n                        \u003Cdate>1970\u003C/date>\n                        \u003CcitedRange>465, 466\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/CMVPRJZD\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002187\">https://biblio.inscriptiones.org/epig10002187\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Tusa\u003C/author>\n                        \u003Cdate>1970\u003C/date>\n                        \u003CcitedRange>no.128\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/P6FHJGR7\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002567\">https://biblio.inscriptiones.org/epig10002567\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n                \u003ClistBibl type=\"discussion\">\n                    \u003Cbibl/>\n                \u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
