s>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>Castelli, Principe di Torremuzza (1753: 147) notes that the stone was damaged on all sides\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone\" ref=\"http://www.eagle-network.eu/voc/material/lod/2.html\">stone\u003C/material>\n                                    \u003CobjectType ana=\"#object.unknown\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/2.html\">unknown\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">unknown\u003C/height>\n                                        \u003Cwidth unit=\"cm\">unknown\u003C/width>\n                                        \u003Cdepth unit=\"cm\">unknown\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition ana=\"#condition.fragment\"/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>Remains to two lines of Greek letters, damaged at both ends (?)\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Gualtherus records the sigmas in line 2 and the epsilon in line 1 as lunate (and he is followed by Castelli)\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">unknown\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">unknown\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462225\">Halaesa\u003C/placeName>\t\t\n                                \u003Coffset>near\n                                    \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525099\">Castel di Tusa\u003C/placeName>\n                                \u003C/offset>\n                            \t\u003Cgeo>37.99907, 14.26197\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0300\" notAfter-custom=\"0600\" cert=\"low\">Kaibel speculates that it is 'aetatis fortasse Byzantinae'! (?)\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" notAfter=\"1624\">Dug up from the ruins of Halaesa (according to Torremuzza)\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"first-recorded\">Recorded as beside the great door in the front of the Church of S. Maria dei Palazzi by Gualtherus, 1621-1624.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"recorded\">Subsequently by Castelli, Principe di Torremuzza.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\u003Cterm ana=\"#function.unknown\" ref=\"https://www.eagle-network.eu/voc/typeins/lod/143.html\">unknown\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2019-07-09\" who=\"#JP\">Jonathan Prag revised the file based on study for 2017 edition\u003C/change>\n                \u003Cchange when=\"2020-09-09\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n          \u003Csurface type=\"antiquarianEdition\">\n              \u003Cgraphic n=\"screen\" url=\"ISic001179_tiled.tif\" height=\"9300px\" width=\"6617px\">\n                  \u003Cdesc>Copy of Gualtherus 1624 no.297 (from Arachne edition)\u003C/desc>\n              \u003C/graphic>\n              \u003Cgraphic n=\"print\" url=\"ISic001179.jpg\" height=\"9300px\" width=\"6617px\">\n                  \u003Cdesc>Copy of Gualtherus 1624 no.297 (from Arachne edition)\u003C/desc>\n              \u003C/graphic>\n  \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\"/>ΙΜΝ\u003Cgap reason=\"lost\" unit=\"character\" quantity=\"2\"/>ΥΑΤΟΝΔΕΠΟΙΚΙΛ\u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\"/>\n                    \u003Clb n=\"2\"/>\u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\"/>IXIII\u003Cgap reason=\"lost\" unit=\"character\" quantity=\"1\"/>ΣΟΥΣΑΠΑΝΤΟΣΚΙ\u003Cgap reason=\"lost\" unit=\"character\" extent=\"unknown\"/>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text after Gualtherus and Castelli\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\">\n                \u003Cp/>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" resp=\"#JP\">\n                \u003Cp>Gualtherus records the sigmas in line 2 and the epsilon in line 1 as lunate (and he is followed by Castelli) and, assuming that this is an accurate record of what he saw, suggests a date of the imperial or late antique period. Kaibel speculated that it might be a verse text of the Byzantine period, reading τόνδε ποικίλ[ον] at the end of line 1, but despaired of interpretation. In similar vein, if correct, [-]υατον could be the end of a transliterated Latin name such as Torquatus, but the difficulty is that it is entirely possible that some of the letters are wrongly transcribed.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0357\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\u003Cauthor>Gualtherus\u003C/author>\u003Cdate>(no date)\u003C/date>\u003CcitedRange>no.143\u003C/citedRange>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624 [1625]\u003C/date>\n                        \u003CcitedRange>no.297\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/ZP8DE8KS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001931\">https://biblio.inscriptiones.org/epig10001931\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1753\u003C/date>\n                        \u003CcitedRange>146 no.4\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/U7S4ZAG3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002086\">https://biblio.inscriptiones.org/epig10002086\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>cl.18 no.27\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/SPTB56H3\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>3.5599\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Prag and Tigano\u003C/author>\n                        \u003Cdate>2017\u003C/date>\n                        \u003CcitedRange>no.50\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/MI64NB9L\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002630\">https://biblio.inscriptiones.org/epig10002630\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
