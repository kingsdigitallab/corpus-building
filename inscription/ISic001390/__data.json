   \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Lost\u003C/settlement>\n                    \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\u003Cp>The stone is one of three described as 'petra nigra' by Gualtherus (1624: 50), but neither state of conservation nor dimensions are recorded.\u003C/p>\n                                    \u003Cmaterial ana=\"#material.stone.volcanic\" ref=\"http://www.eagle-network.eu/voc/material/lod/28.html\">volcanic\u003C/material>\n                                    \u003CobjectType ana=\"#object.unknown\" ref=\"http://www.eagle-network.eu/voc/objtyp/lod/2.html\">unknown\u003C/objectType>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\"/>\n                                        \u003Cwidth unit=\"cm\"/>\n                                        \u003Cdepth unit=\"cm\"/>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\u003Cp>The text is presented as split over three lines\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\u003Cp>Plain transcription in standard capitals makes it impossible to assess the letter-forms.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line1\">Line 1\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Interlineation line 1 to 2\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\">NA\u003C/height>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462074\">Adranon\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525769\">Adrano\u003C/placeName>\n                            \t\u003Cgeo>37.66358, 14.83283\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"-0400\" notAfter-custom=\"-0001\" cert=\"low\" precision=\"low\">A date in the period 4th — 1st century BCE seems most likely, but it is impossible to be certain in the absence of further information\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"first-recorded\">Described by Gualtherus (1624) as coming from the ruins popularly identified with the shrine of the god Hadranus (not identified). All later editions derive from Gualtherus.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\">None\u003C/provenance>\n                        \u003Cprovenance type=\"not-observed\" subtype=\"lost\">Lost.\u003C/provenance>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2020-06-14\" who=\"#JP\">Jonathan Prag edited the file in line with edition prepared for Adrano museum catalogue\u003C/change>             \n                \u003Cchange when=\"2020-09-10\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t\u003Cchange when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers\u003C/change>\n\t\t\u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2022-02-17\" who=\"#JP\">updated bibliography and named entities\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic001390_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic001390.jpg\" height=\"3680px\" width=\"5520px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Νίκαιος\u003C/name>\u003C/persName>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text from Gualtherus 1624\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Nikaios\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>The stone is presented in the edition of Gualtherus (1624, p.50 nr. 338) under the same heading as no.4 above, and so the same report appears to apply, namely ‘petra nigra’, state of conservation and dimensions not recorded; it is said to have come from the ruins popularly equated with the site of the temple of the local divinity Adranos. The text was not included in  Torremuzza 1769 or 1784. It is not clear why Kaibel (IG XIV, 571) says ‘admodum recens reperta’, i.e. only recently recovered.\u003C/p>\n                \u003Cp>The name \u003Cref target=\"http://clas-lgpn2.classics.ox.ac.uk/cgi-bin/lgpn_search.cgi?name=%CE%9D%CE%AF%CE%BA%CE%B1%CE%B9%CE%BF%CF%82\">Νίκαιος\u003C/ref> is common, although not in Sicily, where there is only one other attested example, probably from Syracuse.\u003C/p>\n                    \u003Cp>It is impossible to date this text, in the absence of further information, although it is most likely to belong to the Hellenistic period.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IG\">\n                        \u003CcitedRange>\n                            \u003Cref target=\"https://clio.columbia.edu/catalog/6873760\">14.0571\u003C/ref>\n                        \u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/Q2SBPG9F\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001072\">https://biblio.inscriptiones.org/epig10001072\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624 [1625]\u003C/date>\n                        \u003CcitedRange>\u003Cref target=\"http://arachne.uni-koeln.de/item/buchseite/310975\">50 no. 338\u003C/ref>\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/ZP8DE8KS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001931\">https://biblio.inscriptiones.org/epig10001931\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"corpus\" n=\"CIG\">\n                        \u003CcitedRange>3.5740\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/HTUDQBXS\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001252\">https://biblio.inscriptiones.org/epig10001252\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Russo\u003C/author>\n                        \u003Cdate>1911\u003C/date>\n                        \u003CcitedRange>59\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/CRSSWMDV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001615\">https://biblio.inscriptiones.org/epig10001615\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Prag\u003C/author>\n                        \u003Cdate>2015\u003C/date>\n                        \u003CcitedRange>319 no. D\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/SHKXSQLC\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10002338\">https://biblio.inscriptiones.org/epig10002338\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
