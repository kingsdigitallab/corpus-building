    \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"la\" resp=\"#VM #JP\" source=\"http://zotero.org/groups/382445/items/ZP8DE8KS\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003Cw n=\"5\">Dis\u003C/w> \u003Cg ref=\"#interpunct\" n=\"10\">·\u003C/g> \u003Cw n=\"15\">\u003Cexpan>\u003Cabbr>man\u003C/abbr>\u003Cex>ibus\u003C/ex>\u003C/expan>\u003C/w>\n                    \u003Clb n=\"2\"/>\u003CpersName type=\"attested\">\u003Cname>\u003Cw n=\"20\">Primi\u003C/w>\u003C/name>\u003C/persName> \u003Cg ref=\"#interpunct\" n=\"25\">·\u003C/g> \u003Cw n=\"30\">qui\u003C/w>\n                    \u003Clb n=\"3\"/>\u003Cw n=\"35\">\u003Cexpan>\u003Cabbr>vix\u003C/abbr>\u003Cex>it\u003C/ex>\u003C/expan>\u003C/w>  \u003Cg ref=\"#interpunct\" n=\"37\">·\u003C/g> \u003Cw n=\"40\">\u003Cexpan>\u003Cabbr>ann\u003C/abbr>\u003Cex>is\u003C/ex>\u003C/expan>\u003C/w>  \u003Cg ref=\"#interpunct\" n=\"42\">·\u003C/g> \u003Cnum value=\"18\">\u003Cw n=\"45\">XVIII\u003C/w>\u003C/num>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"edition\" subtype=\"simple-lemmatized\" resp=\"#VF\">\n                \u003Cab>\n                    \u003Cw n=\"5\" lemma=\"dis\">Dis\u003C/w>\n                    \u003Cw n=\"15\" lemma=\"manes\">manibus\u003C/w>\n                    \u003Cw n=\"20\" lemma=\"Primus\">Primi\u003C/w>\n                    \u003Cw n=\"30\" lemma=\"qui\">qui\u003C/w>\n                    \u003Cw n=\"35\" lemma=\"vivo\">vixit\u003C/w>\n                    \u003Cw n=\"40\" lemma=\"annus\">annis\u003C/w>\n                    \u003Cw n=\"45\" lemma=\"XVIII\">XVIII\u003C/w>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\n                    \u003Capp>\n                        \u003Cnote>Text of Gualtherus\u003C/note>\n                    \u003C/app>\n                \u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>To the shades of the underworld, of Primus, who lived 18 years\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\">\n                \u003Cp>This is the only instance of this formulation (Dis manibus + genitive + relative pronoun) at Termini Imerese\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624\u003C/date>\n                        \u003CcitedRange>no. 118\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/GIN6NANU\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002388\">https://biblio.inscriptiones.org/epig10002388\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Gualtherus\u003C/author>\n                        \u003Cdate>1624 [1625]\u003C/date>\n                        \u003CcitedRange>no.281\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/ZP8DE8KS\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10001931\">https://biblio.inscriptiones.org/epig10001931\u003C/ref>\n                    \u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Castelli\u003C/author>\n                        \u003Cdate>1769\u003C/date>\n                        \u003CcitedRange>cl.14 no.105\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/SPTB56H3\"/>\n                        \u003Cref target=\"https://biblio.inscriptiones.org/epig10002084\">https://biblio.inscriptiones.org/epig10002084\u003C/ref>\n                    \u003C/bibl>\n                    \u003C