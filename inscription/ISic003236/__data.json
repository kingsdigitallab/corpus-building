   \u003CrespStmt>\n                    \u003Cname xml:id=\"system\">system\u003C/name>\n                    \u003Cresp>automated or batch processes\u003C/resp>\n                \u003C/respStmt>\n\t    \u003CrespStmt>\n     \t       \u003Cname xml:id=\"SS\" ref=\"https://orcid.org/0000-0003-3914-9569\">Simona Stoyanova\u003C/name>\n     \t       \u003Cresp>standardisation of template and tidying up encoding\u003C/resp>\n \t   \u003C/respStmt>\n\t\u003C/titleStmt>\n            \u003CpublicationStmt>\n                \u003Cauthority>I.Sicily\u003C/authority>\n                \u003Cidno type=\"filename\">ISic003236\u003C/idno>\n                \u003Cidno type=\"TM\">645538\u003C/idno>\n                \u003Cidno type=\"EDR\"/>\n                \u003Cidno type=\"EDH\"/>\n                \u003Cidno type=\"EDCS\"/>\n                \u003Cidno type=\"PHI\">316218\u003C/idno>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic003236\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-09-10\">10.5281/zenodo.4022695\u003C/idno>\n                \u003Cidno type=\"URI\">http://sicily.classics.ox.ac.uk/inscription/ISic003236\u003C/idno>\n                \u003Cidno type=\"DOI\" when=\"2020-12-18\">10.5281/zenodo.4357350\u003C/idno>\n                \u003Cavailability>\n                    \u003Clicence target=\"http://creativecommons.org/licenses/by/4.0/\">Licensed under a Creative Commons-Attribution 4.0 licence.\u003C/licence>\n                \u003C/availability>\n            \u003C/publicationStmt>\n            \u003CsourceDesc>\n                \u003CmsDesc>\n                    \u003CmsIdentifier>\n                        \u003Ccountry>Italy\u003C/country>\n                        \u003Cregion>Sicily\u003C/region>\n                        \u003Csettlement>Catania\u003C/settlement>\n                        \u003Crepository role=\"museum\" ref=\"http://sicily.classics.ox.ac.uk/museum/018\">Museo Civico di Catania\u003C/repository>\n                        \u003Cidno type=\"inventory\">841\u003C/idno>\n                        \u003CaltIdentifier>\n                            \u003Csettlement/>\n                            \u003Crepository/>\n                            \u003Cidno type=\"old\"/>\n                        \u003C/altIdentifier>\n                    \u003C/msIdentifier>\n                    \u003CmsContents>\n                        \u003CtextLang mainLang=\"grc\">Ancient Greek\u003C/textLang>\n                    \u003C/msContents>\n                    \u003CphysDesc>\n                        \u003CobjectDesc>\n                            \u003CsupportDesc>\n                                \u003Csupport>\n                                    \u003Cmaterial ana=\"#material.stone.marble\" ref=\"http://www.eagle-network.eu/voc/material/lod/48.html\">marble\u003C/material>\n                                    \u003CobjectType ana=\"#object.urn\" ref=\"https://www.eagle-network.eu/voc/objtyp/lod/78.html\">Cinerary urn\u003C/objectType>\n                                    \u003Cp>A rectangular white marble urn, with a lid. The lid has\n                                            acroteria on each of the four corners. The urn has square fluted columns\n                                            depicted in relief on each vertical corner, and a tabula ansata in light\n                                            relief fills the front face.\u003C/p>\n                                    \u003Cdimensions>\n                                        \u003Cheight unit=\"cm\">24\u003C/height>\n                                        \u003Cwidth unit=\"cm\">45.5\u003C/width>\n                                        \u003Cdepth unit=\"cm\">28\u003C/depth>\n                                    \u003C/dimensions>\n                                \u003C/support>\n                                \u003Ccondition/>\n\t\t    \u003C/supportDesc>\n                            \u003ClayoutDesc>\n                                \u003Clayout>\n                                    \u003Cp>Greek text over four lines, centred in the main panel of the\n                                        tabula ansata on the front face of the urn.\u003C/p>\n                                    \u003Crs ana=\"#execution.chiselled\" ref=\"http://www.eagle-network.eu/voc/writing/lod/1\">chiselled\u003C/rs>\n                                \t\u003Cdamage/>\n\t\t\t\u003C/layout>\n                            \u003C/layoutDesc>\n                        \u003C/objectDesc>\n                        \u003ChandDesc>\n                            \u003ChandNote>\n                                \u003Cp>Neat and regular square letters. Phi and omicron are rhomboid in\n                                    form; standard four-bar sigma; epsilon has horizontal bars of equal length; alpha\n                                    has a slightly extended apex; mu has extended apices, and all four strokes are full\n                                    length and off-vertical. There is one instance of ligature in line 3, and\n                                    interpuncts are in the form of simple triangular incisions.\u003C/p>\n                                \u003Clocus from=\"line1\" to=\"line4\">Line 1-4\u003C/locus>\n                                \u003Cdimensions type=\"letterHeight\">\n                                    \u003Cheight unit=\"mm\">15-20\u003C/height>\n                                \u003C/dimensions>\n                                \u003Clocus from=\"line1\" to=\"line2\">Not measured\u003C/locus>\n                                \u003Cdimensions type=\"interlinear\">\n                                    \u003Cheight unit=\"mm\"/>\n                                \u003C/dimensions>\n                            \u003C/handNote>\n                        \u003C/handDesc>\n                    \u003C/physDesc>\n                    \u003Chistory>\n                        \u003Corigin>\n                            \u003CorigPlace>\n                                \u003CplaceName type=\"ancient\" ref=\"http://pleiades.stoa.org/places/462270\">Catina\u003C/placeName>\t\t\n                                \u003CplaceName type=\"modern\" ref=\"http://sws.geonames.org/2525068\">Catania\u003C/placeName>\n                            \t\u003Cgeo>37.49223, 15.07041\u003C/geo>\n\t\t\u003C/origPlace>\n                            \u003CorigDate datingMethod=\"#julian\" notBefore-custom=\"0069\" notAfter-custom=\"0150\" precision=\"low\">69 CE — first half of 2nd century CE\u003C/origDate>\n                        \u003C/origin>\n                        \u003Cprovenance type=\"found\" subtype=\"discovered\" when=\"1913\">Found in 1913 in a Roman tomb uncovered during building works in prop. Guglielmino, in the area known as Orto del Re (probably the insula bounded by via Nino Martoglio, via Stellata, via Orto San Clemente, and via Reclusorio del Lume)\u003Cgeo>37.508233, 15.080495\u003C/geo>.\u003C/provenance>\n                        \u003Cprovenance type=\"observed\">Display, Voci di pietra no.17\u003C/provenance>\n                        \u003Cprovenance type=\"observed\" subtype=\"autopsied\" resp=\"#JP\">2016-10-4 (Prag)\u003C/provenance>\n                        \u003Cacquisition/>\n                    \u003C/history>\n                \u003C/msDesc>\n            \u003C/sourceDesc>\n        \u003C/fileDesc>\n        \u003CencodingDesc>\n             \u003Cp>Encoded following the latest EpiDoc guidelines\u003C/p>\n             \u003Cxi:include href=\"../alists/ISicily-taxonomies.xml\">\n                 \u003Cxi:fallback>\n                     \u003Cp>Taxonomies for ISicily controlled values\u003C/p>\n                 \u003C/xi:fallback>\n             \u003C/xi:include>\n\t  \u003Cxi:include href=\"../alists/charDecl.xml\">\n\t     \u003Cxi:fallback>\n\t       \u003Cp>ISicily glyphs authority list\u003C/p>\n\t     \u003C/xi:fallback>\n\t   \u003C/xi:include>\n         \u003C/encodingDesc>\n        \u003CprofileDesc>\n            \u003CcalendarDesc>\n                \u003Ccalendar xml:id=\"julian\">\n                    \u003Cp>Julian Calendar\u003C/p>\n                \u003C/calendar>\n            \u003C/calendarDesc>\n            \u003ClangUsage>\n                \u003Clanguage ident=\"en\">English\u003C/language>\n                \u003Clanguage ident=\"it\">Italian\u003C/language>\n                \u003Clanguage ident=\"grc\">Ancient Greek\u003C/language>\n                \u003Clanguage ident=\"la\">Latin\u003C/language>\n                \u003Clanguage ident=\"he\">Hebrew\u003C/language>\n                \u003Clanguage ident=\"phn\">Phoenician\u003C/language>\n                \u003Clanguage ident=\"xpu\">Punic\u003C/language>\n                \u003Clanguage ident=\"osc\">Oscan\u003C/language>\n                \u003Clanguage ident=\"xly\">Elymian\u003C/language>\n                \u003Clanguage ident=\"scx\">Sikel\u003C/language>\n                \u003Clanguage ident=\"sxc\">Sikan\u003C/language>\n            \u003C/langUsage>\n            \u003CtextClass>\n                \u003Ckeywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    \u003Cterm ana=\"#function.funerary\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/92.html\">funerary\u003C/term>\n                \u003C/keywords>\n            \u003C/textClass>\n        \u003C/profileDesc>\n        \u003CrevisionDesc status=\"edited\">\n            \u003ClistChange>\n                \u003Cchange when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database\u003C/change>\n                \u003Cchange when=\"2017-06-11\" who=\"#JP\">Jonathan Prag revised the EpiDoc, added translation\u003C/change>\n                \u003Cchange when=\"2017-06-29\" who=\"#JP\">Jonathan Prag revised the file, added Italian elements and further metadata\u003C/change>\n                \u003Cchange when=\"2017-07-11\" who=\"#JP\">Jonathan Prag made minor additions to metadata\u003C/change>\n                \u003Cchange when=\"2020-09-10\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t   \u003Cchange when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode\u003C/change>\n            \t   \u003Cchange when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages\u003C/change>\n            \t  \u003Cchange when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography\u003C/change>\n\t    \u003Cchange when=\"2020-12-18\" who=\"#system\">Updated Zenodo DOI\u003C/change>\n            \t\u003Cchange when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references\u003C/change>\n                \u003Cchange when=\"2023-06-06\" who=\"#JP\">Jonathan Prag added image and revised text\u003C/change>\n\t\u003C/listChange>\n        \u003C/revisionDesc>\n    \u003C/teiHeader>\n    \u003Cfacsimile>\n        \u003Csurface type=\"front\">\n            \u003Cgraphic n=\"screen\" url=\"ISic003236_tiled.tif\" height=\"2856px\" width=\"4272px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana (Photo by students of the Liceo Lazzaro)\u003C/desc>\n            \u003C/graphic>\n            \u003Cgraphic n=\"print\" url=\"ISic003236.jpg\" height=\"2856px\" width=\"4272px\">\n                \u003Cdesc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana (Photo by students of the Liceo Lazzaro)\u003C/desc>\n            \u003C/graphic>\n         \u003C/surface>\n    \u003C/facsimile>\n    \u003Ctext>\n        \u003Cbody>\n            \u003Cdiv type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                \u003Cab>\n                    \u003Clb n=\"1\"/>\u003CpersName type=\"attested\">\u003Cname>Φλαβιανὸς\u003C/name>\u003C/persName> \u003Cg ref=\"#interpunct\">·\u003C/g> χρησ\n                    \u003Clb n=\"2\" break=\"no\"/>τὸς \u003Cg ref=\"#interpunct\">·\u003C/g> καὶ \u003Cexpan>\u003Cabbr>ἄμεν\u003C/abbr>\u003Cex>πτος\u003C/ex>\u003C/expan> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cexpan>\u003Cabbr>ἔζη\u003C/abbr>\u003Cex>σεν\u003C/ex>\u003C/expan> \u003Cg ref=\"#interpunct\">·\u003C/g>\n                    \u003Clb n=\"3\"/>ἔτη \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cnum value=\"37\">λζ\u003C/num> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cexpan>\u003Cabbr>μ\u003Chi rend=\"ligature\">ῆν\u003C/hi>\u003C/abbr>\u003Cex>ας\u003C/ex>\u003C/expan> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cnum value=\"3\">γ\u003C/num> \u003Cg ref=\"#interpunct\">·\u003C/g>\n                    \u003Clb n=\"4\"/>\u003Cexpan>\u003Cabbr>ἡμέ\u003C/abbr>\u003Cex>ρας\u003C/ex>\u003C/expan> \u003Cg ref=\"#interpunct\">·\u003C/g> \u003Cnum value=\"10\">ι\u003C/num> \u003Cg ref=\"#interpunct\">·\u003C/g>\n                \u003C/ab>\n            \u003C/div>\n            \u003Cdiv type=\"apparatus\">\n                \u003ClistApp>\u003Capp>\u003Cnote>Text from autopsy (Prag)\u003C/note>\u003C/app>\u003C/listApp>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>Phlabianos (Flavianus), worthy and blameless, lived for 37 years, 3 months, 10 days.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"translation\" xml:lang=\"it\" resp=\"#SA\">\n                \u003Cp>Flaviano, degno e innocente, visse 37 anni, 3 mesi, 10 giorni.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"commentary\" xml:lang=\"en\" resp=\"#JP\">\n                \u003Cp>According to Paolo Orsi, the urn still contained the deceased’s ashes when it was discovered, and\n                    traces of gilding were still visible on some of the letters and the capital of one of the relief\n                    columns. The use of the name Flavianus suggests a date after the beginning of the Flavian dynasty\n                    (beginning with Vespasian, 69 AD), but Flavianus’ status as slave, freedman, or free remains\n                    uncertain. The use of architectural elements in the form of the chest and a tabula ansata to frame\n                    the inscription are typical features of ash chests of this period.\u003C/p>\n            \u003C/div>\n            \u003Cdiv type=\"bibliography\">\n                \u003ClistBibl type=\"edition\">\n                    \u003Cbibl type=\"corpus\" n=\"IMusCatania\">\n                        \u003CcitedRange>87\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/2EUC82DH\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10000028\">https://biblio.inscriptiones.org/epig10000028\u003C/ref>\u003C/bibl>\n                    \u003Cbibl type=\"bulletin\" n=\"NSA\">\n                        \u003Cauthor>Orsi\u003C/author>\n                        \u003Ctitle>NSA\u003C/title>\n                        \u003Cdate>1915\u003C/date>\n                        \u003CcitedRange>216\u003C/citedRange>\n                        \u003Cptr target=\"http://zotero.org/groups/382445/items/4MCP2PVV\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001392\">https://biblio.inscriptiones.org/epig10001392\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Libertini\u003C/author>\n                        \u003Cdate>1937\u003C/date>\n                        \u003CcitedRange>50\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/P7FP4UWI\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001867\">https://biblio.inscriptiones.org/epig10001867\u003C/ref>\u003C/bibl>\n                    \u003Cbibl>\n                        \u003Cauthor>Wilson\u003C/author>\n                        \u003Cdate>1990\u003C/date>\n                        \u003CcitedRange>134\u003C/citedRange>\n                        \u003Cptr target=\"https://www.zotero.org/groups/382445/items/VF9MMBI9\"/>\n                    \u003Cref target=\"https://biblio.inscriptiones.org/epig10001509\">https://biblio.inscriptiones.org/epig10001509\u003C/ref>\u003C/bibl>\n                \u003C/listBibl>\n\t   \u003ClistBibl type=\"discussion\">\u003Cbibl/>\u003C/listBibl>\n            \u003C/div>\n        \u003C/body>\n    \u003C/text>\n\u003C/TEI>\n"],"uses":{"params":["slug"]}}]}
