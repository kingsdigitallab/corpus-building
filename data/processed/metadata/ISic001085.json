ange>\n            \t   <change when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages</change>\n            \t<change when=\"2020-11-20\" who=\"#SS\">Simona Stoyanova added EDCS numbers</change>\n\t\t<change when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography</change>\n\t    <change when=\"2020-12-22\" who=\"#system\">Updated Zenodo DOI</change>\n            \t<change when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references</change>\n                <change when=\"2024-02-17\" who=\"#JP\">Jonathan Prag revised file from publication</change>\n\t</listChange>\n        </revisionDesc>\n    </teiHeader>\n    <facsimile>\n        <surface type=\"front\">\n            <graphic n=\"screen\" url=\"ISic001085_tiled.tif\" height=\"4980px\" width=\"2696px\">\n                <desc>Crispi 1831 fig.1</desc>\n            </graphic>\n            <graphic n=\"print\" url=\"ISic001085.jpg\" height=\"4980px\" width=\"2696px\">\n                <desc>Crispi 1831 fig.1</desc>\n            </graphic>\n         </surface>\n    </facsimile>\n    <text>\n        <body>\n            <div type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#JP\">\n                <ab>\n                    <lb n=\"1\"/><gap reason=\"lost\" unit=\"character\" quantity=\"2\" cert=\"low\"/><orig>ΥΑΥΣ<gap reason=\"illegible\" unit=\"character\" quantity=\"1\"/>Ο</orig>\n                    <lb n=\"2\"/><gap reason=\"lost\" unit=\"character\" quantity=\"1\" cert=\"low\"/>ΟΙΑΤΟ\n                    <lb n=\"3\"/><gap reason=\"lost\" unit=\"character\" quantity=\"1\" cert=\"low\"/>ΖΟΣ\n                 </ab>\n            </div>\n            <div type=\"apparatus\">\n                <listApp>\n                    <app><note>Text based on drawing from Crispi 1831</note></app>\n                    <app loc=\"1\"><note>The fifth character is depicted simply as a rectangle in Crispi's drawing</note></app>\n                </listApp>\n            </div>\n