\n            <textClass>\n                <keywords scheme=\"http://www.eagle-network.eu/voc/typeins.html\">\n                    <term ana=\"#function.terminus\" ref=\"http://www.eagle-network.eu/voc/typeins/lod/96.html\">terminus</term>\n                </keywords>\n            </textClass>\n        </profileDesc>\n        <revisionDesc status=\"edited\">\n            <listChange>\n                <change when=\"2016-12-03\" who=\"#JCu\">James Cummings autogenerated EpiDoc output from database</change>\n                <change when=\"2020-10-05\" who=\"#SS\">Simona Stoyanova normalised Unicode</change>\n\t   <change when=\"2020-10-08\" who=\"#SS\">Simona Stoyanova updated list of languages</change>\n                <change when=\"2020-11-26\" who=\"#SS\">Simona Stoyanova restructured bibliography</change>\n\t   <change when=\"2020-12-23\" who=\"#system\">Updated Zenodo DOI</change>\n                <change when=\"2021-01-19\" who=\"#SS\">renumbered files, uris and references</change>\n                <change when=\"2021-09-06\" who=\"#VM\">Valentina Mignosa added text and further data (object dimensions, letter height, text layout, references, commentary)</change>\n                <change when=\"2023-05-04\" who=\"#SS\">added milestones to indicate change of face</change>\n\t</listChange>\n        </revisionDesc>\n    </teiHeader>\n    <facsimile>\n        <surface type=\"front\">\n            <graphic n=\"screen\" url=\"ISic003644_tiled.tif\" height=\"3680px\" width=\"5520px\">\n                <desc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana</desc>\n            </graphic>\n            <graphic n=\"print\" url=\"ISic003644.jpg\" height=\"3680px\" width=\"5520px\">\n                <desc>I.Sicily with the permission of the Assessorato Regionale dei Beni Culturali e dell’Identità Siciliana - Dipartimento dei Beni Culturali e dell’Identità Siciliana</desc>\n            </graphic>\n         </surface>\n    </facsimile>\n    <text>\n        <body>\n            <div type=\"edition\" xml:space=\"preserve\" xml:lang=\"scx\" resp=\"#VM\">\n                <div type=\"textpart\" subtype=\"section\" n=\"a\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"a\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>ΜΟΓΟΜ</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>ΣΙΕΛΔΟ</orig><gap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig>ΜΟΙΔΑ</orig>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"b\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"b\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig><unclear>ΝΑ</unclear>ΔΑΡΑ</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>ΚΟΙΤΟΒ</orig><gap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig><unclear>Ϝ</unclear>Ͱ<unclear>ΕΛ</unclear>Ͱ<unclear>Ο</unclear></orig>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"c\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"c\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>ΡΟΓΑΥ</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>ΛΤΙΝΑΣ</orig>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig>ϜΟΓ</orig><gap reason=\"lost\" quantity=\"2\" unit=\"character\"/>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"d\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"d\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>ΠΙΣΕΜΒ</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>ΡΟϜΑΣΔΙΣ</orig>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig><unclear>Ϝ</unclear></orig><gap reason=\"lost\" quantity=\"2\" unit=\"character\"/><unclear>Σ</unclear>ΑΙΟ\n                    </ab>\n                </div>\n            </div>\n            <div type=\"edition\" xml:space=\"preserve\" subtype=\"transliteration\" xml:lang=\"scx-Latn\" resp=\"#VM\">\n                <div type=\"textpart\" subtype=\"section\" n=\"a\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"a\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>mogom</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>sieldo</orig><gap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig>moida</orig>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"b\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"b\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig><unclear>na</unclear>dara</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>koitob</orig><gap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig><unclear>v</unclear>h<unclear>el</unclear>h<unclear>o</unclear></orig>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"c\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"c\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>rogau</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>ltinas</orig>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig>vog</orig><gap reason=\"lost\" quantity=\"2\" unit=\"character\"/>\n                    </ab>\n                </div>\n                <div type=\"textpart\" subtype=\"section\" n=\"d\">\n                    <ab>\n                        <milestone unit=\"face\" n=\"d\"/>\n                        <lb n=\"1\" style=\"text-direction:r-to-l\"/><orig>pisemb</orig>\n                        <lb n=\"2\" style=\"text-direction:l-to-r\"/><orig>rovasdis</orig>\n                        <lb n=\"3\" style=\"text-direction:r-to-l\"/><orig><unclear>v</unclear></orig><gap reason=\"lost\" quantity=\"2\" unit=\"character\"/><unclear>s</unclear>aio\n                    </ab>\n                </div>\n            </div>\n            <div type=\"apparatus\">\n                <listApp>\n                    <app>\n                        <note>Text after Manganaro 1961</note>\n                    </app>\n                </listApp>\n            </div>\n            <div type=\"translation\">\n                <p><!--No translation currently agreed--></p>\n            </div>\n            <div type=\"commentary\">\n                <p>The document is the so-called 'cippo Sanfilippo' (from the surname of the owner of the land where it was found), \n                    a lava stone parallelepiped, discovered in 1883, interpreted as a terminus and datable to the 6th century BCE.\n                    The cippus, previously kept in the Villa Sanfilippo, was donated in 1960 to the Museo Archeologico Etneo in Adrano, \n                    where it is now located. The text is difficult to read due to the poor state of conservation of the stone. \n                    However, it is clear that the text is boustrophedon and presents, as far as can be read, the alphabet typical of Sikel \n                    inscriptions from the Etna area. Worthy of note, in particular, is the presence of the arrow-shaped alpha, the digamma \n                    (with value of v, see also <ref target=\"http://sicily.classics.ox.ac.uk/inscription/ISic003364\">ISic003364</ref>).</p>\n            </div>\n            <div type=\"bibliography\">\n                <listBibl type=\"edition\">\n                    <bibl>\n                        <author>Manganaro</author>\n                        <date>1961</date>\n                        <citedRange>106-110, pls. L-LIII</citedRange>\n                        <ptr target=\"https://www.zotero.org/groups/382445/items/4356IBUF\"/>\n                    <ref target=\"https://biblio.inscriptiones.org/epig10002677\">https://biblio.inscriptiones.org/epig10002677</ref></bibl>\n                    <bibl>\n                        <author>Schmoll</author>\n                        <date>1968</date>\n                        <citedRange>201-202</citedRange>\n                        <ptr target=\"http://zotero.org/groups/382445/items/CMIJ47JE\"/>\n                    <ref target=\"https://biblio.inscriptiones.org/epig10002568\">https://biblio.inscriptiones.org/epig10002568</ref></bibl>\n                    <bibl>\n                        <author>Agostiniani</author>\n                        <date>1973</date>\n                        <citedRange>395</citedRange>\n                        <ptr target=\"http://zotero.org/groups/382445/items/EAFGZBIB\"/>\n                    <ref target=\"https://biblio.inscriptiones.org/epig10002426\">https://biblio.inscriptiones.org/epig10002426</ref></bibl>\n                    <bibl>\n                        <author>Zamboni</author>\n                        <date>1978</date>\n                        <citedRange>958</citedRange>\n                        <ptr target=\"http://zotero.org/groups/382445/items/3595JHKT\"/>\n                    <ref target=\"https://biblio.inscriptiones.org/epig10002521\">https://biblio.inscriptiones.org/epig10002521</ref></bibl>\n                    <bibl>\n                        <author>Albanese</author>\n                        <date>1991</date>\n                        <citedRange>545 no.1</citedRange>\n                        <ptr target=\"https://www.zotero.org/groups/382445/items/IVAU3I2N\"/>\n                    <ref target=\"https://biblio.inscriptiones.org/epig10002196\">https://biblio.inscriptiones.org/epig10002196</ref></bibl>\n                    <bibl>\n                        <author>Agostiniani</author>\n                        <date>2009</date>\n                        <citedRange>116 no.3 fig.4</citedRange>\n                        <ptr target=\"http://zotero.org/groups/382445/items/63R3F4J2\"/>\n                        <ref target=\"https://biblio.inscriptiones.org/epig10002427\">https://biblio.inscriptiones.org/epig10002427</ref></bibl>\n                </listBibl>\n\t   <listBibl type=\"discussion\">\n\t       <bibl>\n\t           <author>Pelagatti</author>\n\t           <date>2009</date>\n\t           <citedRange>22</citedRange>\n\t           <ptr target=\"http://zotero.org/groups/382445/items/BZEJN9HP\"/>\n\t       <ref target=\"https://biblio.inscriptiones.org/epig10002429\">https://biblio.inscriptiones.org/epig10002429</ref></bibl>\n\t       <bibl>\n\t           <author>Mignosa</author>\n\t           <date>2017-2018</date>\n\t           <citedRange>217, no. 1, pls. 2-3</citedRange>\n\t           <ptr target=\"http://zotero.org/groups/382445/items/2EBZBFTF\"/>\n\t       <ref target=\"https://biblio.inscriptiones.org/epig10002428\">https://biblio.inscriptiones.org/epig10002428</ref></bibl>\n\t   </listBibl>\n            </div>\n        </body>\n    </text>\n</TEI>\n",
