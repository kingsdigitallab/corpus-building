  "xml": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<div type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#MB\">\n  <div type=\"textpart\" subtype=\"section\" n=\"a\">\n    <ab>\n      ς\n      <milestone unit=\"face\" subtype=\"recto\"/>\n      <lb n=\"1\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"3\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"6\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"7\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"8\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"9\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"10\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"11\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"12\" style=\"text-direction:r-to-l\"/>\n      <lb n=\"13\" style=\"text-direction:r-to-l\"/>\n      <persName type=\"attested\">\n        <name>\n          Πίθα\n          <supplied reason=\"lost\">ϙος</supplied>\n        </name>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          Ἀφαι στίο\n          <lb n=\"2\" break=\"no\" style=\"text-direction:r-to-l\"/>\n          <unclear>ν</unclear>\n        </name>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          στας\n          <gap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n          <gap reason=\"illegible\" quantity=\"1\" unit=\"character\"/>\n          <unclear>α</unclear>\n        </name>\n      </persName>\n      <persName type=\"attested\">\n        <name>Ἐρίφιλος</name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">\n            Τρπε φίλο\n            <unclear>ε</unclear>\n            <lb n=\"4\" break=\"no\" style=\"text-direction:r-to-l\"/>\n          </name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          λας\n          <unclear>Ματυ</unclear>\n        </name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">\n            <choice>\n              <reg>\n                Καλέ ος\n                <unclear>λ</unclear>\n                <lb n=\"5\" break=\"no\" style=\"text-direction:r-to-l\"/>\n              </reg>\n              <orig>\n                Καέος\n                <unclear>λ</unclear>\n              </orig>\n            </choice>\n          </name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>Θοπύλος</name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Ξένονος</name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          θανις\n          <unclear>Ἄ</unclear>\n        </name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Ἀντάνδρο</name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          άρον\n          <unclear>Χ</unclear>\n        </name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">\n            Ηαγάρχο\n            <supplied reason=\"lost\">εσ</supplied>\n          </name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          ανος\n          <unclear>Σιλ</unclear>\n        </name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Ηαγεσάρχο</name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>\n          <unclear>Ἄθαν</unclear>\n          <supplied reason=\"lost\" cert=\"low\">ις</supplied>\n        </name>\n      </persName>\n      <persName type=\"attested\">\n        <name>Ξένον</name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Ηινύϙο</name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>Ηαγίας</name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Φιλοδάμο</name>\n        </persName>\n      </persName>\n      <persName type=\"attested\">\n        <name>Δεινίας</name>\n        <persName type=\"attested\">\n          <name type=\"patronymic\">Σφυρᾶ</name>\n        </persName>\n      </persName>\n      <gap reason=\"lost\" extent=\"unknown\" unit=\"character\"/>\n      <unclear>ο</unclear>\n      <space extent=\"unknown\" unit=\"character\"/>\n      <space extent=\"unknown\" unit=\"character\"/>\n    </ab>\n  </div>\n  <div type=\"textpart\" subtype=\"section\" n=\"b\">\n    <ab>\n      <milestone unit=\"face\" subtype=\"verso\"/>\n      <lb n=\"1\"/>\n      <persName type=\"attested\">\n        <name type=\"patronymic\">\n          Φιλονο\n          <supplied reason=\"lost\">ς</supplied>\n        </name>\n        <persName type=\"attested\">\n          <name>Ναϙοι</name>\n        </persName>\n      </persName>\n    </ab>\n  </div>\n</div>",
