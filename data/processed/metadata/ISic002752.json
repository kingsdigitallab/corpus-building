  "xml": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<div type=\"edition\" xml:space=\"preserve\" xml:lang=\"grc\" resp=\"#VM\">\n  <lg met=\"elegiac couplet\">\n    <l n=\"1\" met=\"hexameter\" real=\"+--|+--|++|+--|+--|++\">\n      σῶμα μὲν Εὐστοχί νος ἐν χθονὶ κεῖ\n      <lb n=\"1\"/>\n      <lb n=\"2\" break=\"no\"/>\n      <unclear>ο</unclear>\n      <unclear>τ</unclear>\n      <supplied reason=\"lost\">υ</supplied>\n      <supplied reason=\"lost\">τοῦ</supplied>\n      <supplied reason=\"lost\">αι</supplied>\n      <seg met=\"+-\">\n        <gap reason=\"lost\" extent=\"unknown\" unit=\"character\" precision=\"low\"/>\n      </seg>\n      <space extent=\"unknown\" unit=\"character\"/>\n    </l>\n    <l n=\"2\" met=\"pentameter\" real=\"+--|+--|+|+--|+--|+\">\n      ὃς λίπε ἐν Λυκίῃ νή\n      <lb n=\"3\"/>\n      <unclear>ν</unclear>\n      <unclear>π</unclear>\n      <supplied reason=\"lost\">ια</supplied>\n      <supplied reason=\"lost\">τέκνα</supplied>\n      <seg met=\"-+\">\n        <gap reason=\"lost\" extent=\"unknown\" unit=\"character\" precision=\"low\"/>\n      </seg>\n    </l>\n  </lg>\n  <lg met=\"elegiac couplet\">\n    <l n=\"3\" met=\"hexameter\" real=\"+--|++|+--|+--|+--|++\">\n      κάλλιπε δ’ αὐλ κοιρανίην ἀρετ\n      <lb n=\"4\"/>\n      <lb n=\"5\"/>\n      <space extent=\"unknown\" unit=\"character\"/>\n      <supplied reason=\"lost\">ὴν</supplied>\n      <supplied reason=\"lost\" cert=\"low\">ῆς</supplied>\n      <seg met=\"+--\">\n        <gap reason=\"lost\" extent=\"unknown\" unit=\"character\" precision=\"low\"/>\n      </seg>\n      <seg met=\"+\">\n        <gap reason=\"lost\" extent=\"unknown\" unit=\"character\" precision=\"low\"/>\n      </seg>\n    </l>\n    <l n=\"4\" met=\"pentameter\" real=\"+--|+--|+|+--|++|-\">\n      τ’ ἐπέδη ἄστει τῷδ\n      <supplied reason=\"lost\">τόν</supplied>\n      <supplied reason=\"lost\">ε</supplied>\n      <supplied reason=\"lost\" cert=\"low\">κακὴ</supplied>\n      <supplied reason=\"lost\"