:67,"settlement":58,"repository":133,"idno":134,"textLang":136,"facsimile":138,"provenance":54,"letterHeights":140,"publications":141,"zotero":142,"keywords":143,"html":147},"ISic004412","Epitaph for Aristoboula",{"_":10},{"_":13,"ana":14,"ref":15},{"_":17,"ana":18,"ref":19},{"_":21,"ana":22,"ref":23},{"layout":116},{"p":27,"rs":117,"damage":24},{"_":29,"ana":30,"ref":31},{"lettering":119,"dimensions":120},"Slightly irregular letters, becoming slightly more compressed to the right hand end: Alpha broad with broken bar; rho has small closed eye; sigma is four equal bars, angled apart; omicron is small and mid-line; beta has equal closed eyes, with slight gap between them; upsilon asymmetrical.",[121,122],{"type":36,"l":37,"from":38,"to":38,"h":39,"unit":40},{"type":42,"l":43,"from":38,"to":44,"h":45,"unit":40},{"_":124,"notBefore":125,"notAfter":126,"evidence":127,"precision":51},"Letter forms suggest a date in the 4th or earlier 3rd century BCE",-350,-250,"lettering",[129,130],{"_":54,"type":55,"ref":56},{"_":58,"type":59,"ref":60,"offset":61},[132],[64,65],{"_":69,"role":70,"ref":71},{"_":135,"type":74},"ME23439",{"_":76,"mainLang":77,"languages":137},[76],{"n":80,"url":139,"height":82,"width":83,"desc":84,"surfaceType":85},"ISic004412_tiled.tif",[],[89,90,91],[94,95,96],[144,145,146,13,17,21,101,102,103,104,102,105,106,77],"isic004412","epitaph for aristoboula",{"_":10},"\u003Cdiv class=\"textpart\" lang=\"grc\">\u003Cspan class=\"ab\" id=\"div-interpretive\">  \u003Ca id=\"al1-interpretive\">\u003C!--0-->\u003C/a>\u003Cspan class=\"linenumber\">1\u003C/span>Ἀριστοβούλας \u003C/span>\u003C/div>  \u003Cp class=\"miniapp\">\u003C/p>",{"file":149,"uri":149,"title":150,"status":151,"editionAuthor":11,"type":152,"objectType":153,"material":154,"condition":24,"layoutDesc":155,"handNote":158,"date":163,"places":166,"geo":169,"country":66,"region":67,"settlement":58,"repository":171,"idno":172,"textLang":174,"facsimile":176,"provenance":54,"letterHeights":178,"publication